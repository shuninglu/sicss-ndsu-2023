@article{ WOS:000469017400001,
Author = {Bieniek-Tobasco, Ashley and McCormick, Sabrina and Rimal, Rajiv N. and
   Harrington, Cherise B. and Shafer, Madelyn and Shaikh, Hina},
Title = {Communicating climate change through documentary film: imagery, emotion,
   and efficacy},
Journal = {CLIMATIC CHANGE},
Year = {2019},
Volume = {154},
Number = {1-2},
Pages = {1-18},
Month = {MAY},
Abstract = {We used qualitative in-depth interviews to evaluate the effects of a
   mass media climate change program on audiences' efficacy beliefs,
   outcome expectations, emotional responses, and motivations and
   intentions to address climate change. We conducted in-depth interviews
   with 73 participants from five US cities and three political parties who
   had watched episodes of the documentary television series, Season Two of
   Years of Living Dangerously. Eligible participants completed an in-depth
   interview within 24h of viewing a select episode. Data were transcribed
   and then coded and analyzed using QSR NVivo 10. Weak efficacy beliefs
   limited intentions to enact concrete behavioral change. Outcome
   expectations, national-level actions, imagery, and emotional responses
   to stories played an important role in these processes. Explicit
   information about expected outcomes of various actions, and specifically
   successes, should be provided in order to boost efficacy and incentivize
   behavior.},
