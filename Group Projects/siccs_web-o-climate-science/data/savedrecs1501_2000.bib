@article{ WOS:000318816900003,
Author = {Zamith, Rodrigo and Pinto, Juliet and Villar, Maria Elena},
Title = {Constructing Climate Change in the Americas: An Analysis of News
   Coverage in US and South American Newspapers},
Journal = {SCIENCE COMMUNICATION},
Year = {2013},
Volume = {35},
Number = {3},
Pages = {334-357},
Month = {JUN},
Abstract = {This study examined the portrayal of climate change in four national
   newspapers from Argentina, Brazil, Colombia, and the United States. The
   results indicated that leading media in Brazil and the United States
   highlighted the policy progress being made to mitigate climate change
   and presented the issue in economic terms, whereas coverage in Argentina
   and Colombia portrayed the issue as being urgent and emphasized the
   catastrophic consequences of climate change. The findings are consistent
   with previous work indicating a lack of focus on scientific controversy
   from non-U.S. media and present implications for comparative studies
   examining nuances in international coverage of climate change.},
