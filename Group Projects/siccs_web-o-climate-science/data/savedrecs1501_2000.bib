@inproceedings{ WOS:000462172400057,
Author = {Shariff, N. Mohd and Abidin, A. Zainol and Mohamed, A. E.},
Book-Group-Author = {IOP},
Title = {Investigating critical indicators of climate variability and change to
   tourism event},
Booktitle = {INTERNATIONAL CONFERENCE ON CLIMATE CHANGE (ICCC 2018)},
