@article{ WOS:000420200700003,
Author = {Adisa, R. S. and Balogun, K. S.},
Title = {Analysis of Training Needs of Extension Agents on Climate Change Issues
   in Ekiti State Agricultural Development Project (EKSADP), Nigeria},
Journal = {JOURNAL OF AGRICULTURAL EXTENSION},
