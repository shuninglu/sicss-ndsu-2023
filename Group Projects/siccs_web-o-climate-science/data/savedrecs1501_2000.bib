@article{ WOS:000342622600003,
Author = {Yang, Z. Janet and Rickard, Laura N. and Harrison, Teresa M. and Seo,
   Mihye},
Title = {Applying the Risk Information Seeking and Processing Model to Examine
   Support for Climate Change Mitigation Policy},
Journal = {SCIENCE COMMUNICATION},
Year = {2014},
Volume = {36},
Number = {3},
Pages = {296-324},
Month = {JUN},
Abstract = {This study applies the risk information seeking and processing (RISP)
   model to examine how information processing influences individuals'
   support for climate change mitigation policy. Perceived issue salience,
   attitude toward climate information, and systematic processing are
   positively related to policy support, whereas heuristic processing is
   not a significant predictor. The direct and indirect relationships
   illustrated in the model help expand the utility of the risk information
   seeking and processing model to address policy support. From an applied
   perspective, this study suggests important pathways to encourage
   systematic processing of information related to climate change, which
   may lead to increased public support for mitigation policies.},
