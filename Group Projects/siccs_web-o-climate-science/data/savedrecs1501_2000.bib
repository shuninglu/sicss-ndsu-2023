@article{ WOS:000369342200005,
Author = {McCright, Aaron M. and Charters, Meghan and Dentzman, Katherine and
   Dietz, Thomas},
Title = {Examining the Effectiveness of Climate Change Frames in the Face of a
   Climate Change Denial Counter-Frame},
Journal = {TOPICS IN COGNITIVE SCIENCE},
Year = {2016},
