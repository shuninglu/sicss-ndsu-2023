@article{ WOS:000421983800009,
Author = {Chandler, Lisa and Baldwin, Claudia},
Title = {Reflections from the Water's Edge: Collaborative photographic narratives
   addressing climate change},
Journal = {SOCIAL ALTERNATIVES},
Year = {2010},
Volume = {29},
Number = {4},
Pages = {30-36},
Abstract = {Photographs can function as powerful forms of communication reflecting
   collective perspectives about social and environmental issues. This
   paper demonstrates how the visual research methodology, photovoice, was
   employed to elicit the values and concerns of three different groups
   about being `at the water's edge' in times of uncertainty due to climate
   change. The project was undertaken within the environmental art event
   Floating Land, which explored the theme of climate change and rising sea
   levels on coastal and island communities. Photovoice draws out ideas
   communicated by participants' photos which are negotiated through group
   discussion and clarified with a caption. The aim of this process is to
   provide insights into shared values, with the goal of informing broader
   society and facilitating community change. In this project, each group
   collaboratively organised selected images into visual narratives which
   were presented to a wider audience. Through a comparison of the
   different group perspectives and an examination of the impact of the
   visual presentations, this paper examines some of the effects of
   communicating with images in this way. We argue that visual `stories'
   formed from groupings of photographs provide persuasive, alternative
   forms of expressing and understanding shared perspectives of climate
   change.},
