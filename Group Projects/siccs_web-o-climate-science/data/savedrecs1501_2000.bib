@article{ WOS:000532749400004,
Author = {Metcalfe, Jennifer},
Title = {Chanting to the choir: the dialogical failure of antithetical climate
   change blogs},
Journal = {JCOM-JOURNAL OF SCIENCE COMMUNICATION},
Year = {2020},
Volume = {19},
Number = {2},
Abstract = {Blogs provide potential for publics to engage more deliberatively
   through dialogue in controversial science than one-way dissemination
   methods. This study investigated who was commenting on two antithetical
   climate change blogsites; how they were commenting; and the quality of
   their dialogue. Most research into science blogs has focused on bloggers
   rather than commenters. This study found that both blogsites were
   dominated by a small number of commenters who used contractive dialogue
   to promote their own views to like-minded commenters. Such blogsites are
   consolidating their own polarised publics rather than deliberately
   engaging them in climate change science.},
