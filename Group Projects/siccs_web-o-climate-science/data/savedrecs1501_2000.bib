@article{ WOS:000527374800002,
Author = {Liu, Wei-jie and Shao, Ke-qi and Ning, Yue and Zhao, Xi-zeng},
Title = {Numerical Study of the Impact of Climate Change on Irregular Wave Run-up
   Over Reef-Fringed Coasts},
Journal = {CHINA OCEAN ENGINEERING},
