@article{ WOS:000295518500003,
Author = {Hattermann, Fred Fokko and Weiland, Mathias and Huang, Shaochun and
   Krysanova, Valentina and Kundzewicz, Zbigniew W.},
Title = {Model-Supported Impact Assessment for the Water Sector in Central
   Germany Under Climate Change-A Case Study},
