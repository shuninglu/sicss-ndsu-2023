@incollection{ WOS:000424510800008,
Author = {Wrobel, Markus and Reusser, Dominik},
Editor = {Schneider, B and Nocke, T},
Title = {Towards an Interactive Visual Understanding of Climate Change Findings
   on the Net: Promises and Challenges},
Booktitle = {IMAGE POLITICS OF CLIMATE CHANGE: VISUALIZATIONS, IMAGINATIONS,
   DOCUMENTATIONS},
Series = {Image-Series},
Year = {2014},
Volume = {55},
Pages = {187-210},
Abstract = {Web-based applications can facilitate the dissemination of scientific
   information and reduce existing access barriers to climate change
   research findings, In addition, interactive visualization offers
   increased flexibility in representing data compared to static pictures,
   and allows the user to adapt the visual representation to her or his
   personal interest to some extent, The focus of this article is on
   benefits and challenges emerging from combining these two information
   technological strands in order to improve the dissemination of
   scientific results on climate change to a wider audience. Web-based
   interactive visual representations of climate change findings (WIVCC)
   have the potential to improve the communication of research results by
   supporting users in better comprehending the inherent complex,
   multi-dimensional data of this field, However, applying WIVCC for
   effective climate change communication is no trivial task, We analyze
   current challenges by integrating findings from various disciplines,
   including climate change research, climate change communication,
   computer-based visualization and user interface design, Alter outlining
   general benefits and challenges and introducing two examples of existing
   applications. we address two issues in greater detail and present
   results relating to (i) the lack of established conventions in designing
   user interfaces for WIVCC applications and (ii) appropriate visual
   representation of spatially heterogeneous uncertainty, We discuss these
   findings together with a set of further open questions related to
   designing effective interactive visual representations of scientific
   results to foster web based climate change communication.},
