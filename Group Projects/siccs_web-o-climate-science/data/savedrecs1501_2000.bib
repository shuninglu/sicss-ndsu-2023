@article{ WOS:000600847000001,
Author = {Cha, Eun Jeong and Knutson, Thomas R. and Lee, Tsz-Cheung and Ying, Ming
   and Nakaegawa, Toshiyuki},
Title = {Third assessment on impacts of climate change on tropical cyclones in
   the Typhoon Committee Region - Part II: Future projections},
Journal = {TROPICAL CYCLONE RESEARCH AND REVIEW},
Year = {2020},
Volume = {9},
Number = {2},
Pages = {75-86},
Month = {JUN},
Abstract = {This paper assesses published findings on projections of future tropical
   cyclone (TC) activity in the ESCAP/WMO Typhoon Committee Region under
   climate change scenarios. This assessment also estimates the projected
   changes of key TC metrics for a 2 degrees C anthropogenic global warming
   scenario for the western North Pacific (WNP) following the approach of a
   WMO Task Team, together with other reported findings for this region.
   For projections of TC genesis/frequency, most models suggest a reduction
   of TC frequency, but an increase in the proportion of very intense TCs
   over the WNP in the future. However, some individual studies project an
