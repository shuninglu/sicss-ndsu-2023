Author = {Kubota, Takuji and Ushio, Tomoo and Shige, Shoichi and Kida, Satoshi and
   Kachi, Misako and Okamoto, Ken'ichi},
Title = {Verification of High-Resolution Satellite-Based Rainfall Estimates
   around Japan Using a Gauge-Calibrated Ground-Radar Dataset},
