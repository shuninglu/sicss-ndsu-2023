@article{ WOS:000301274200008,
Author = {Han, Kyong Ho and Noh, Su Pin and Hong, In Kwon and Park, Kyung Ai},
Title = {Cooling domain prediction of HFCs and HCFCs refrigerant with
   Joule-Thomson coefficient},
