@article{ WOS:000303938800003,
Author = {Liu, Yejun and Guo, Lei and Gong, Bo and Ma, Rui and Gong, Xiaoxue and
   Zhang, Lincong and Yang, Jiangzi},
Title = {Green survivability in Fiber-Wireless (FiWi) broadband access network},
Journal = {OPTICAL FIBER TECHNOLOGY},
