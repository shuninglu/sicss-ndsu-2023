@article{ WOS:000280801600005,
Author = {Nolan, Jessica M.},
Title = {``An Inconvenient Truth{''} Increases Knowledge, Concern, and
   Willingness to Reduce Greenhouse Gases},
Journal = {ENVIRONMENT AND BEHAVIOR},
Year = {2010},
Volume = {42},
Number = {5},
Pages = {643-658},
Month = {SEP},
Abstract = {Since May 24, 2006 millions of people have seen the movie ``An
   Inconvenient Truth.{''} Several countries have even proposed using the
   film as an educational tool in school classrooms. However, it is not yet
   clear that the movie accomplishes its apparent goals of increasing
   knowledge and concern, and motivating people to reduce their greenhouse
   gas emissions. Two studies are reported, one with a sample of community
