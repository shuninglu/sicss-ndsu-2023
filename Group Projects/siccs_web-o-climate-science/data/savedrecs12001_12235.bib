Author = {Bergstrom, DW and Tenuta, M and Beauchamp, EG},
Title = {Nitrous oxide production and flux from soil under sod following
   application of different nitrogen fertilizers},
Journal = {COMMUNICATIONS IN SOIL SCIENCE AND PLANT ANALYSIS},
