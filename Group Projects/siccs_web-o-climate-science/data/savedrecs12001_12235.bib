@article{ WOS:000264705200001,
Author = {Lim, Melvin and Zhou, Yan and Guo, Yanan and Sun, Chenghua and Wood,
   Barry and Wang, Lianzhou and Hulicova-Jurcakova, Denisa and Zou, Jin and
   Rudolph, Victor and Lu, G. Q. Max},
Title = {Visible-light photoresponsive heterojunctions of (Nb-Ti-Si) and
   (Bi/Bi-o) nanoparticles},
Journal = {ELECTROCHEMISTRY COMMUNICATIONS},
Year = {2009},
Volume = {11},
Number = {3},
Pages = {509-514},
Month = {MAR},
Abstract = {The increasing motivation to seek alternative sources of clean and
   sustainable energy has intensified, due to a growing awareness that
   fossil fuels are finite in quantity, and that the combustion products of
   such fuels contribute to global warming. Solar energy is considered one
   of the most readily available alternatives, but materials which are able
   to harness this form of energy need to be developed. This study details
   the development of a novel composite material, of the form of Bi/Bi-O
   nanoparticles supported on a (Nb-Ti-Si) oxide. Characterized using
   electrochemical and other methods, this material generates a
   photocurrent, and is capable of photo-oxidizing airborne-styrene in a
   fluidized-bed photoreactor when exposed to visible-light. (c) 2008
   Elsevier B.V. All rights reserved.},
Publisher = {ELSEVIER SCIENCE INC},
Address = {360 PARK AVE SOUTH, NEW YORK, NY 10010-1710 USA},
Type = {Article},
Language = {English},
