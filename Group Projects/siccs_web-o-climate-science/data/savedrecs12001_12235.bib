@article{ WOS:000309246100022,
Author = {Dobre, Ciprian},
Title = {USING INTELLIGENT TRAFFIC LIGHTS TO REDUCE VEHICLE EMISSIONS},
Journal = {INTERNATIONAL JOURNAL OF INNOVATIVE COMPUTING INFORMATION AND CONTROL},
