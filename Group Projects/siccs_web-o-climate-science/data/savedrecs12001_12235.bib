Author = {Sperling, Uwe},
Title = {ASPECTS OF CHANGE IN THE BRONZE AGE EASTERN BALTIC. THE SETTLEMENTS OF
   THE ASVA GROUP IN ESTONIA},
Journal = {ESTONIAN JOURNAL OF ARCHAEOLOGY},
