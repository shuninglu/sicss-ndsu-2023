   Williams, John R. and Roth, Paul and Hofmann, Paul and Zeier, Alexander},
Book-Group-Author = {IEEE},
Title = {A Comparative Study of Data Storage and Processing Architectures for the
   Smart Grid},
Booktitle = {2010 IEEE 1ST INTERNATIONAL CONFERENCE ON SMART GRID COMMUNICATIONS
   (SMARTGRIDCOMM)},
