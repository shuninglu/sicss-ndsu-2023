@article{ WOS:000405294700008,
Author = {Jensen, Klaus Bruhn},
Title = {Speaking of the weather: Cross-media communication and climate change},
Journal = {CONVERGENCE-THE INTERNATIONAL JOURNAL OF RESEARCH INTO NEW MEDIA
   TECHNOLOGIES},
Year = {2017},
Volume = {23},
