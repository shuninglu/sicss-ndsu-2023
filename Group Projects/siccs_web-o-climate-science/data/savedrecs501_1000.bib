@article{ WOS:000891869600001,
Author = {Canon, Chelsea R. and Boyle, Douglas P. and McAfee, Stephanie A.},
Title = {Visualizing the structure and development of climate change
   communication research},
Journal = {JCOM-JOURNAL OF SCIENCE COMMUNICATION},
Year = {2022},
Volume = {21},
Number = {7},
Abstract = {To better understand the structure, development, and function of the
   climate change communication knowledge domain, we performed
   time-evolving bibliometric mapping and topic modeling on 2,995 climate
   change communication publications from Web of Science. Structural and
   visual representations of scholarship are useful for identifying areas
   of opportunity and coordinating effort in interdisciplinary and
   action-oriented knowledge domains. Our analysis reveals a cohesive and
   dense yet ossified knowledge structure which suggests that while a
   systems approach is being applied in climate communication, there is a
   need to explore more constitutive strategies for the communication of
   climate change.},
