@inproceedings{ WOS:000221847700021,
Author = {Bugmann, HKM},
Editor = {Canham, CD and Cole, JJ and Lauenroth, WK},
Title = {Predicting the ecosystem effects of climate change},
Booktitle = {MODELS IN ECOSYSTEM SCIENCE},
