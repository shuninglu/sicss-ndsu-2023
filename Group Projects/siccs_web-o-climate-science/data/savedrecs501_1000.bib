@article{ WOS:000318327500003,
Author = {O'Neill, Saffron J. and Boykoff, Maxwell and Niemeyer, Simon and Day,
   Sophie A.},
Title = {On the use of imagery for climate change engagement},
Journal = {GLOBAL ENVIRONMENTAL CHANGE-HUMAN AND POLICY DIMENSIONS},
