Author = {Rengalakshmi, R. and Geethalakshmi, V and Bhuvaneswari, K.},
Title = {Spatial variation of rainfed maize yield response to climate change in
   Tamil Nadu},
Journal = {JOURNAL OF AGROMETEOROLOGY},
