@article{ WOS:000374771500002,
Author = {Ballantyne, Anne Gammelgaard},
Title = {Climate change communication: what can we learn from communication
   theory?},
Journal = {WILEY INTERDISCIPLINARY REVIEWS-CLIMATE CHANGE},
Year = {2016},
Volume = {7},
