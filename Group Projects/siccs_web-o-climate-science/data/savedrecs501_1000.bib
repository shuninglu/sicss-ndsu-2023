@article{ WOS:000783462700002,
Author = {Cheval, Sorin and Bulai, Ana and Croitoru, Adina-Eliza and Dorondel,
   Stefan and Micu, Dana and Mihaila, Dumitru and Sfica, Lucian and
   Tiscovschi, Adrian},
Title = {Climate change perception in Romania},
