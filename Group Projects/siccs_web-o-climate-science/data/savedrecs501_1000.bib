@article{ WOS:000213471700007,
Author = {Arnoldi, Emsie and Miller, Hilary},
Title = {Towards a Framework for the Practice of Climate Change Communication in
   Australia},
Journal = {ASIA PACIFIC PUBLIC RELATIONS JOURNAL},
Year = {2013},
Volume = {14},
Number = {1-2},
Pages = {85-106},
Abstract = {Climate change represents one of the most complex communication
   challenges for governments in advanced neoliberal democracies like
   Australia: to increase social capital and generate mass public support
   for the economic and lifestyle changes that will be required to make
   climate change policy effective. Drawing on an examination of secondary
   data on current theories used to underpin climate change communication
   and cross-analysis of theory and a case study from Australia, the
   researchers develop four pre-theoretical principles for the practice of
   climate change communication in Australia. Future research could build
   upon these pre-theoretical principles to better understand the
   complexities and challenges involved in climate change communication.},
