@article{ WOS:000418133300004,
Author = {Ojwang, Lenice and Rosendo, Sergio and Celliers, Louis and Obura, David
   and Muiti, Anastasia and Kamula, James and Mwangi, Maina},
Title = {Assessment of Coastal Governance for Climate Change Adaptation in Kenya},
