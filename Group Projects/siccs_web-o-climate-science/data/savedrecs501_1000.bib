@inproceedings{ WOS:000399737702043,
Author = {Barwell, Richard and Suurtamm, Christine},
Editor = {Pytlak, M and Rowland, T and Swoboda, E},
Title = {CLIMATE CHANGE AND MATHEMATICS EDUCATION: MAKING THE INVISIBLE VISIBLE},
Booktitle = {PROCEEDINGS OF THE SEVENTH CONGRESS OF THE EUROPEAN SOCIETY FOR RESEARCH
   IN MATHEMATICS EDUCATION (CERME 7)},
Year = {2011},
Pages = {1409-1418},
Note = {7th Congress of the
   European-Society-for-Research-in-Mathematics-Education (ERME), Rzeszow,
   POLAND, FEB 09-13, 2011},
Organization = {European Soc Res Math Educ},
Abstract = {Mathematics is crucial for describing, predicting and communicating
   climate change. In this paper, we use the perspective of critical
   mathematics education to examine how mathematics is constructed in
   reporting climate change. We focus particularly on accounts of
   mathematical modelling. Our discussion is illustrated by reference to
   two texts: a newspaper report and a policy document. We argue that
   mathematics, human mathematical activity and the role of human activity
   in causing climate change are all largely invisible in such texts.
   Mathematics education has an important role to play in making the
   invisible visible.},
