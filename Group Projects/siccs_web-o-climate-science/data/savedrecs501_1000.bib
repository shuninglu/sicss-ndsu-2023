@article{ WOS:000394405200004,
Author = {Jankovic, Vladimir and Schultz, David M.},
Title = {Atmosfear : Communicating the Effects of Climate Change on Extreme
   Weather},
Journal = {WEATHER CLIMATE AND SOCIETY},
