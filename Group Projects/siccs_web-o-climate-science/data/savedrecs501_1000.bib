@article{ WOS:000434940700001,
Author = {Perkins, David R. and Myers, Teresa and Francis, Zephi and Mazzone,
   Raphael and Maibach, Edward},
Title = {Attributes of Weathercasters Who Engage in Climate Change Education
   Outreach},
Journal = {WEATHER CLIMATE AND SOCIETY},
Year = {2018},
Volume = {10},
Number = {3},
Pages = {487-500},
Month = {JUL},
Abstract = {This research explores the role of weathercasters as local climate
   change educators and identifies attributes of those who present climate
   science to their viewers. In 2015, the authors attempted to survey all
   television weathercasters currently working in the United States (n =
   2128); 478 participated, yielding a 22.5\% participation rate. Using
   logistic regression to identify attributes of weathercasters who report
   on climate change on-air, it was found that the strongest predictors
   were participation in Climate Matters (a climate change reporting
   resource program) ( = 1.01, p < 0.001), personal interest in reporting
   on climate change ( = 0.93, p < 0.001), age (higher rates of reporting
   among older weathercasters) ( = 0.301, p < 0.05), and number of climate
   reporting interests ( = 1.37, p < 0.05). Linear regression was used to
   identify attributes of weathercasters who showed the most interest in
   climate change reporting. Weathercasters most interested in reporting
   about climate change on-air were more certain that climate change is
   happening ( = 0.344, p < 0.001), were convinced climate change is human
   caused ( = 0.226, p < 0.001), were older ( = 0.157, p < 0.001), and
   found the Third National Climate Assessment to be useful ( = 0.134, p <
   0.05). Weathercasters who are personally motivated to seek and share
   broad scientific information, acting as station scientists, appear to be
   those who are also proactive in sharing climate change information.
   Assisting motivated weathercasters with programs that reduce barriers to
   climate change education outreach complements their abilities to educate
   the public regarding climate change science.},
