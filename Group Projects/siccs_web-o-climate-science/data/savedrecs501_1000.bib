@article{ WOS:000463073100002,
Author = {Taylor, Andrea and Dessai, Suraje and de Bruin, Waendi Bruine},
Title = {Public priorities and expectations of climate change impacts in the
   United Kingdom},
Journal = {JOURNAL OF RISK RESEARCH},
Year = {2019},
Volume = {22},
Number = {2},
Pages = {150-160},
Month = {FEB 1},
Abstract = {Irrespective of the success of climate mitigation efforts, societies
   worldwide face the challenge of adapting to a changing climate. In this
   paper, we examine UK residents' expectations of future threats and
   opportunities associated with climate change impacts, along with
   willingness to prioritise different climate change impacts for
   investment. Using a national survey (n = 2007), we report on three main
   findings. First, UK residents tend to expect threats related to flooding
   and wet weather to be more likely and concerning than heat extremes or
   opportunities. Second, UK residents' expectations of climate change
   impacts do not align with expert assessments, especially showing lower
   estimates of heat-related threats as compared to experts. Third,
   willingness to allocate resources to potential climate change impacts
   tends to be more strongly associated with anticipated concern should
   they occur than climate change belief or the expected likelihood of them
   occurring. We discuss the implications of our findings for policies and
   communications about climate change adaptation in the UK and elsewhere.},
