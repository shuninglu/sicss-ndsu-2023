@article{ WOS:000361164100026,
Author = {Zhang Fang and You Daming},
Title = {RESEARCH ON GUANGXI BEIBU GULF ECONOMIC ZONE DEVELOPMENT MODE OF LOW
   CARBON ECONOMY FROM THE ANGLE OF INDUSTRIAL STRUCTURE},
Journal = {OXIDATION COMMUNICATIONS},
Year = {2015},
Volume = {38},
Number = {2A, SI},
Pages = {1161-1166},
Abstract = {The development of low carbon economy is proposed under the
   macro-background of the great threat of global warming and greenhouse
   effect on human living environment. This thesis sketches the location,
   resources and transportation advantage of Guangxi Beibu Gulf economic
   zone and analyses its current industrial structure situation as well.
   The authors assert that the development of low carbon economy could be
   achieved through the following four aspects: relevant security system
   should be established to support and develop tertiary industry;
   conventional industry should be reformed with low-carbon technology; the
   proportion of the agriculture, fishery and farming should be lowered
   among the economic output of Beibu Gulf economic zone; great attention
   should be paid to the development of ecological agriculture and circular
   economy. Furthermore, the author profoundly believes that the
   development of circular economy is the most realistic and most
   fundamental way of developing the low carbon economy of Beibu Gulf
   economic zone.},
