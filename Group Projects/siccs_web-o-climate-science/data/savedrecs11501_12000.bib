@article{ WOS:000484567400001,
Author = {Guerses, Oezguer and Kolatschek, Vanessa and Wang, Qiang and Rodehacke,
   Christian Bernd},
Title = {Brief communication: A submarine wall protecting the Amundsen Sea
   intensifies melting of neighboring ice shelves},
Journal = {CRYOSPHERE},
Year = {2019},
Volume = {13},
Number = {9},
Pages = {2317-2324},
Month = {SEP 6},
Abstract = {Disintegration of ice shelves in the Amundsen Sea, in front of the West
   Antarctic Ice Sheet, has the potential to cause sea level rise by
   inducing an acceleration of ice discharge from upstream grounded ice.
   Moore et al. (2018) proposed that using a submarine wall to block the
   penetration of warm water into the subsurface cavities of these ice
   shelves could reduce this risk. We use a global sea ice-ocean model to
   show that a wall shielding the Amundsen Sea below 350m depth
   successfully suppresses the inflow of warm water and reduces ice shelf
   melting. However, these warm water masses get redirected towards
   neighboring ice shelves, which reduces the net effectiveness of the
   wall. The ice loss is reduced by 10 \%, integrated over the entire
   Antarctic continent.},
