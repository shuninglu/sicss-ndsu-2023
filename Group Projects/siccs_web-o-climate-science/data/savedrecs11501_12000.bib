@article{ WOS:000449026400001,
Author = {Ridley, Jeff K. and Blockley, Edward W.},
Title = {Brief communication: Solar radiation management not as effective as CO2
   mitigation for Arctic sea ice loss in hitting the 1.5 and 2 degrees C
   COP climate targets},
Journal = {CRYOSPHERE},
Year = {2018},
Volume = {12},
Number = {10},
Pages = {3355-3360},
Month = {OCT 23},
Abstract = {An assessment of the risks of a seasonally ice-free Arctic at 1.5 and
   2.0 degrees C global warming above pre-industrial levels is undertaken
   using model simulations with solar radiation management to achieve the
   desired temperatures. An ensemble of the CMIP5 model HadGEM2-ES uses
   solar radiation management (SRM) to achieve the desired global mean
   temperatures. It is found that the risk for a seasonally ice-free Arctic
   is reduced for a target temperature for global warming of 1.5 degrees C
   (0.1 \%) compared to 2.0 degrees C (42 \%), in general agreement with
   other methodologies. The SRM produced more ice loss, for a specified
   global temperature, than for CO2 mitigation scenarios, as SRM produces a
   higher polar amplification.},
