@article{ WOS:000430733300022,
Author = {Chiang, Hsiao-Dong and Wang, Tao and Sheng, Hao},
Title = {A Novel Fast and Flexible Holomorphic Embedding Power Flow Method},
Journal = {IEEE TRANSACTIONS ON POWER SYSTEMS},
