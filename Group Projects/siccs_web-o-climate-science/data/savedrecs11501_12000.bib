@article{ WOS:000463007400006,
Author = {Gonzalez, G. and Bustamante, A. and Grez, A. A.},
Title = {Touching the Sky: Coccinellids (Coleoptera: Coccinellidae) at High
   Altitudes in South America},
Journal = {NEOTROPICAL ENTOMOLOGY},
