@article{ WOS:000552423000040,
Author = {Tang, C. A. and Webb, A. A. G. and Moore, W. B. and Wang, Y. Y. and Ma,
   T. H. and Chen, T. T.},
Title = {Breaking Earth's shell into a global plate network},
Journal = {NATURE COMMUNICATIONS},
