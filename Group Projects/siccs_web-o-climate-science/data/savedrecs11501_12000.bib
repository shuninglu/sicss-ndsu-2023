@article{ WOS:000511856300020,
Author = {Coutinho, Rodolfo W. L. and Boukerche, Azzedine and Vieira, Luiz F. M.
   and Loureiro, Antonio A. F.},
Title = {Underwater Sensor Networks for Smart Disaster Management},
Journal = {IEEE CONSUMER ELECTRONICS MAGAZINE},
Year = {2020},
Volume = {9},
Number = {2},
Pages = {107-114},
Month = {MAR},
Abstract = {Internet of underwater things (IoUT) will empower underwater
   humanitarian applications. IoUT will have the ability to sense locations
   in the underwater environment, process the data locally, and transmit it
   via underwater wireless communication. In this article, we discuss how
   IoUT can benefit humanitarian needs, such as mitigating disasters,
   relieving in global warming, and providing access to scientific data. We
   also describe the main approaches and challenges for water sensing and
   communications, and outline the challenges for IoUT-based humanitarian
