@article{ WOS:000486073100013,
Author = {Bindu, E. and Reddy, B. V. R.},
Title = {A novel solution to energy efficiency in multi-user multi-carrier
   decode-and-forward wireless relay networks},
Journal = {SUSTAINABLE COMPUTING-INFORMATICS \& SYSTEMS},
Year = {2019},
Volume = {23},
Pages = {136-143},
Month = {SEP},
Abstract = {In the last few years, energy efficient wireless communication systems
   are attracting great research attention due to increase in global
   warming. Current challenge is to meet the rising demand for channel
   capacity and energy efficiency without compromising on quality of
   service (QoS). Relay aided cellular network is one of the solutions that
   can mitigate the problem of energy consumption. This work proposes a new
   solution for maximizing energy efficiency in relay assisted
   multi-carrier multiuser wireless networks. In this context, a new
   dynamic (optimal) power allocation algorithm for Decode-and-Forward
   relay is proposed here. The energy efficiency is optimized in terms of
   consumed energy per bit subjected to constraint of total power. From
   numerical results, it is observed that energy efficiency performance
   improves with proposed dynamic power allocation algorithm. (C) 2019
   Elsevier Inc. All rights reserved.},
