@article{ WOS:000417998300001,
Author = {Uddin, Mueen and Okai, Safiya and Saba, Tanzila},
Title = {Green ICT framework to reduce carbon footprints in universities},
Journal = {ADVANCES IN ENERGY RESEARCH},
