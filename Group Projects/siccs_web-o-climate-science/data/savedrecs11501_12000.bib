@article{ WOS:000519717500005,
Author = {Broman, Elias and Bonaglia, Stefano and Holovachov, Oleksandr and
   Marzocchi, Ugo and Hall, Per O. J. and Nascimento, Francisco J. A.},
Title = {Uncovering diversity and metabolic spectrum of animals in dead zone
   sediments},
Journal = {COMMUNICATIONS BIOLOGY},
Year = {2020},
Volume = {3},
Number = {1},
Month = {MAR 6},
Abstract = {Broman, Bonaglia et al. report a large-scale study of the micrometazoan
   community in the oxygen-poor, sulfidic Baltic Sea dead zone that is the
   largest in the world. They find that animals including nematodes and
   zooplankton are adapted to survive in this sediment, suggesting dead
   zones may be important biological resources upon recovery.
   Ocean deoxygenation driven by global warming and eutrophication is a
   primary concern for marine life. Resistant animals may be present in
   dead zone sediments, however there is lack of information on their
   diversity and metabolism. Here we combined geochemistry, microscopy, and
   RNA-seq for estimating taxonomy and functionality of micrometazoans
   along an oxygen gradient in the largest dead zone in the world.
   Nematodes are metabolically active at oxygen concentrations below 1.8 mu
   mol L-1, and their diversity and community structure are different
   between low oxygen areas. This is likely due to toxic hydrogen sulfide
   and its potential to be oxidized by oxygen or nitrate. Zooplankton
   resting stages dominate the metazoan community, and these populations
   possibly use cytochrome c oxidase as an oxygen sensor to exit dormancy.
   Our study sheds light on mechanisms of animal adaptation to extreme
   environments. These biological resources can be essential for
   recolonization of dead zones when oxygen conditions improve.},
