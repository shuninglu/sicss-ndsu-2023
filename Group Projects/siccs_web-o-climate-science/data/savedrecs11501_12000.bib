Editor = {Biljanovic, P and Koricic, M and Skala, K and Grbac, TG and CicinSain, M and Sruk, V and Ribaric, S and Gros, S and Vrdoljak, B and Mauher, M and Tijan, E and Hormot, F},
Title = {Transportation and Power System Interdependency for Urban Fast Charging
   and Battery Swapping Stations in Croatia},
Booktitle = {2017 40TH INTERNATIONAL CONVENTION ON INFORMATION AND COMMUNICATION
   TECHNOLOGY, ELECTRONICS AND MICROELECTRONICS (MIPRO)},
