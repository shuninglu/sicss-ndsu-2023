@article{ WOS:000402940400048,
Author = {Kanwal, Kapil and Safdar, Ghazanfar Ali and Ur-Rehman, Masood and Yang,
   Xiaodong},
Title = {Energy Management in LTE Networks},
Journal = {IEEE ACCESS},
Year = {2017},
Volume = {5},
Pages = {4264-4284},
Abstract = {Wireless cellular networks have seen dramatic growth in number of mobile
   users. As a result, data requirements, and hence the base-station power
   consumption has increased significantly. It in turn adds to the
   operational expenditures and also causes global warming. The base
   station power consumption in long-term evolution (LTE) has, therefore,
   become a major challenge for vendors to stay green and profitable in
   competitive cellular industry. It necessitates novel methods to devise
   energy efficient communication in LTE. Importance of the topic has
   attracted huge research interests worldwide. Energy saving (ES)
   approaches proposed in the literature can be broadly classified in
   categories of energy efficient resource allocation, load balancing,
   carrier aggregation, and bandwidth expansion. Each of these methods has
   its own pros and cons leading to a tradeoff between ES and other
   performance metrics resulting into open research questions. This paper
   discusses various ES techniques for the LTE systems and critically
   analyses their usability through a comprehensive comparative study.},
