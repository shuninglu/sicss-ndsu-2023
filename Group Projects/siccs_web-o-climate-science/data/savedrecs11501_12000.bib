@article{ WOS:000549318100009,
Author = {Kabir, Mohammad Ekramul and Assi, Chadi and Antoun, Maurice and
   Alameddine, Hyame and Antoun, Joseph and Yan, Jun},
Title = {Demand-Aware Provisioning of Electric Vehicles Fast Charging
   Infrastructure},
Journal = {IEEE TRANSACTIONS ON VEHICULAR TECHNOLOGY},
Year = {2020},
