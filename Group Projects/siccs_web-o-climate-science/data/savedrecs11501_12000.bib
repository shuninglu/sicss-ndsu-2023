   van Heek, Julia; Arning, Katrin; Ziefle, Martina, Rhein Westfal TH Aachen, Human Comp Interact Ctr, Campus Blvd 57, D-52074 Aachen, Germany.},
DOI = {10.1016/j.enpol.2017.02.016},
ISSN = {0301-4215},
