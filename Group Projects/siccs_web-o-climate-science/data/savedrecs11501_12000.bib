@article{ WOS:000488179900001,
Author = {Naikwade, Pratap Vyankatrao},
Title = {Soil Organic Carbon Sequestration by Long-Term Application of Manures
   Prepared from Trianthema portulacastrurm Linn},
Journal = {COMMUNICATIONS IN SOIL SCIENCE AND PLANT ANALYSIS},
