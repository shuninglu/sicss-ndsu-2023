@inproceedings{ WOS:000591380600004,
Author = {Faniband, Yunus Parvej and Shaahid, S. M.},
Editor = {Kolhe, ML},
Title = {Forecasting Wind Speed using Artificial Neural Networks - A Case Study
   of a Potential Location of Saudi Arabia},
Booktitle = {2020 5TH INTERNATIONAL CONFERENCE ON ADVANCES ON CLEAN ENERGY RESEARCH
   (ICACER 2020)},
Series = {E3S Web of Conferences},
