@article{ WOS:000852448100011,
Author = {Dada, Lubna and Angot, Helene and Beck, Ivo and Baccarini, Andrea and
   Quelever, Lauriane L. J. and Boyer, Matthew and Laurila, Tiia and
   Brasseur, Zoe and Jozef, Gina and de Boer, Gijs and Shupe, Matthew D.
   and Henning, Silvia and Bucci, Silvia and Dutsch, Marina and Stohl,
   Andreas and Petaja, Tuukka and Daellenbach, Kaspar R. and Jokinen, Tuija
   and Schmale, Julia},
Title = {A central arctic extreme aerosol event triggered by a warm air-mass
   intrusion},
Journal = {NATURE COMMUNICATIONS},
Year = {2022},
Volume = {13},
Number = {1},
Month = {SEP 8},
Abstract = {Warm and moist air-mass intrusions into the Arctic are more frequent
   than the past decades. Here, the authors show that warm air mass
   intrusions from northern Eurasia inject record amounts of aerosols into
   the central Arctic Ocean strongly impacting atmospheric chemistry and
   cloud properties.
   Frequency and intensity of warm and moist air-mass intrusions into the
   Arctic have increased over the past decades and have been related to sea
   ice melt. During our year-long expedition in the remote central Arctic
   Ocean, a record-breaking increase in temperature, moisture and
   downwelling-longwave radiation was observed in mid-April 2020, during an
   air-mass intrusion carrying air pollutants from northern Eurasia. The
   two-day intrusion, caused drastic changes in the aerosol size
   distribution, chemical composition and particle hygroscopicity. Here we
   show how the intrusion transformed the Arctic from a remote low-particle
   environment to an area comparable to a central-European urban setting.
   Additionally, the intrusion resulted in an explosive increase in cloud
   condensation nuclei, which can have direct effects on Arctic clouds'
   radiation, their precipitation patterns, and their lifetime. Thus,
   unless prompt actions to significantly reduce emissions in the source
   regions are taken, such intrusion events are expected to continue to
   affect the Arctic climate.},
