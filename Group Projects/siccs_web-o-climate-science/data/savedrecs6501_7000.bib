@article{ WOS:000719238500001,
Author = {Thio, Ellen and Tan, MeiXuen and Li, Liang and Salman, Muhammad and
   Long, Xingle and Sun, Huaping and Zhu, Bangzhu},
Title = {The estimation of influencing factors for carbon emissions based on EKC
   hypothesis and STIRPAT model: Evidence from top 10 countries},
