Title = {Chemical Mechanisms and Their Applications in the Goddard Earth
   Observing System (GEOS) Earth System Model},
Journal = {JOURNAL OF ADVANCES IN MODELING EARTH SYSTEMS},
Year = {2017},
Volume = {9},
