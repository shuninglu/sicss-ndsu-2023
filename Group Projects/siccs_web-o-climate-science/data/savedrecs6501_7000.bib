@article{ WOS:A1996UY59900005,
Author = {Ng, PPT},
Title = {Leisure and social change in Hong Kong},
Journal = {LOISIR \& SOCIETE-SOCIETY AND LEISURE},
Year = {1996},
Volume = {19},
Number = {1},
Pages = {91-115},
Note = {XIIIth World Congress of Sociology, BIELEFELD, GERMANY, JUL 18-23, 1994},
Abstract = {Leisure is conceptualised as occurring in an individual's life space
   that consists of ways of spending time as shaped by the individual's
   roles in social life. Some major changes in Hong Kong society in the
   past three decades are examined to indicate their implications for
   leisure as these changes bear on such roles as family member, parent,
   worker, and consumer. These changes include late marriage and low
   fertility, rising educational levels, technological advancements,
   developments in mass communication, and expansion of leisure services.
   The implications for leisure under a climate of uncertainty during Hong
   Kong's transition to Chinese sovereignty in 1997 are also discussed.},
