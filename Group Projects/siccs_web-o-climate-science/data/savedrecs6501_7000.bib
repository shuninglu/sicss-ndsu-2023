@article{ WOS:000535395800033,
Author = {Sakata, R. and Seki, K. and Sakai, S. and Terada, N. and Shinagawa, H.
   and Tanaka, T.},
Title = {Effects of an Intrinsic Magnetic Field on Ion Loss From Ancient Mars
   Based on Multispecies MHD Simulations},
