@article{ WOS:000631934100017,
Author = {Heinrich, Viola H. A. and Dalagnol, Ricardo and Cassol, Henrique L. G.
   and Rosan, Thais M. and de Almeida, Catherine Torres and Silva Junior,
   Celso H. L. and Campanharo, Wesley A. and House, Joanna I. and Sitch,
