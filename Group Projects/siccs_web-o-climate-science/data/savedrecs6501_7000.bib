@article{ WOS:000983367700004,
Author = {Frietsch, Marina and Loos, Jacqueline and Loehr, Katharina and Sieber,
   Stefan and Fischer, Joern},
Title = {Future-proofing ecosystem restoration through enhancing adaptive
   capacity},
Journal = {COMMUNICATIONS BIOLOGY},
Year = {2023},
Volume = {6},
Number = {1},
Month = {APR 7},
Abstract = {This Perspective piece outlines potential avenues to support ecosystem
   restoration projects into the future.
   Social-ecological ecosystem restoration involves interacting challenges,
   including climate change, resource overexploitation and political
   instability. To prepare for these and other emerging threats, we
   synthesized key restoration and social-ecological systems literature and
   derived three guiding themes that can help to enhance the adaptive
   capacity of restoration sites: (i) work with the existing system, (ii)
   create self-sustaining, adaptive systems, and (iii) foster diversity and
   participation. We propose a two-step approach and provide an example
   from Rwanda detailing the application of these principles. While
   site-specific activities have to be designed and implemented by local
   practitioners, our synthesis can guide forward-thinking restoration
   practice.},
