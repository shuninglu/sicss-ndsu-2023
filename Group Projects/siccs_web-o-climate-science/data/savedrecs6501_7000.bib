@article{ WOS:000378014600020,
Author = {Zhou, Zhifang and Huang, Yue and Chen, Xiaohong and Li, Dayuan},
Title = {ACCOUNTING AND SUSTAINABLE MANAGEMENT OF CARBON COSTS IN IRON AND STEEL
   ENTERPRISES RESPONDING TO CLIMATE CHANGE},
Journal = {OXIDATION COMMUNICATIONS},
Year = {2016},
Volume = {39},
Number = {1A, SI},
Pages = {1037-1048},
Abstract = {The current carbon cost classification methods in practice are not
   standardised, leaving enterprises without any reliable way to calculate
   carbon cost, let alone apply the information during production.
   Considering the characteristics of iron and steel enterprises, carbon
   cost is divided into four factors: prevention, detection, carbon
   emissions, and contingency. The study introduces material flow and
   energy flow theories to facilitate carbon cost calculation, and proposes
   to combine life cycle and material flow model for carbon cost control.
   By examining a case study of a Chinese iron and steel enterprise, the
   case study shows that iron and steel enterprises taking the initiative
   in carbon management will effectively reduce overall carbon cost and
   promote its sustainable development.},
