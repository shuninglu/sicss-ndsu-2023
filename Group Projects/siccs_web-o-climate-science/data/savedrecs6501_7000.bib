Title = {Best Practices for Preparation of GHG Inventory for Industrial Processes},
Booktitle = {16TH INTERNATIONAL CONFERENCE ON PROCESS INTEGRATION, MODELLING AND
   OPTIMISATION FOR ENERGY SAVING AND POLLUTION REDUCTION (PRES'13)},
