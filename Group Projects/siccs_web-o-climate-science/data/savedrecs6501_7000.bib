@article{ WOS:000369505400020,
Author = {Humfeld, Sarah C. and Grunert, Brice},
Title = {EFFECTS OF TEMPERATURE ON SPECTRAL PREFERENCES OF FEMALE GRAY TREEFROGS
   (HYLA VERSICOLOR)},
Journal = {HERPETOLOGICAL CONSERVATION AND BIOLOGY},
