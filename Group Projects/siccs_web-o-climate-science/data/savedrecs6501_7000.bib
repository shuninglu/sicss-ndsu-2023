@article{ WOS:000458761300016,
Author = {Niles, Meredith T. and Emery, Benjamin F. and Reagan, Andrew J. and
   Dodds, Peter Sheridan and Danforth, Christopher M.},
Title = {Social media usage patterns during natural hazards},
