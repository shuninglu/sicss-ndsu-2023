@article{ WOS:000402896100001,
Author = {Wang, Xiao},
Title = {Understanding Climate Change Risk Perceptions in China: Media Use,
   Personal Experience, and Cultural Worldviews},
Journal = {SCIENCE COMMUNICATION},
Year = {2017},
Volume = {39},
Number = {3},
Pages = {291-312},
Month = {JUN},
Abstract = {Based on a survey of 516 Chinese consumers, the present analysis
   revealed that climate- and environment-related media use (e.g., news and
   other information) positively predicted policy support in China, whereas
   personal experience positively predicted perceptions of risks to humans
   and the nonhuman environment. Perceived risks to humans and the nonhuman
   environment were used as two different variables. After controlling for
   various demographic variables, cultural worldviews, and media use, risks
   to the nonhuman environment, but not risks to humans, predicted the
   Chinese's policy support. Results indicate that the media and personal
   experience have differential effects on risk perceptions and policy
   support.},
