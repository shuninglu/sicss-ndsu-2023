@article{ WOS:000876894600004,
Author = {Oreskes, Naomi},
Title = {The trouble with the supply-side model of science},
Journal = {PROCEEDINGS OF THE INDIAN NATIONAL SCIENCE ACADEMY},
Year = {2022},
