@article{ WOS:000245182200009,
Author = {Groves, David G. and Lempert, Robert J.},
Title = {A new analytic method for finding policy-relevant scenarios},
Journal = {GLOBAL ENVIRONMENTAL CHANGE-HUMAN AND POLICY DIMENSIONS},
Year = {2007},
Volume = {17},
Number = {1},
Pages = {73-85},
Month = {FEB},
Abstract = {Scenarios play a prominent role in policy debates over climate change,
   but questions continue about how best to use them. We describe a new
   analytic method, based on robust decision making, for suggesting
   narrative scenarios that emerge naturally from a decision analytic
   framework. We identify key scenarios as those most important to the
   choices facing decision makers and find such cases with statistical
   analysis of datasets created by multiple runs of computer simulation
   models. The resulting scenarios can communicate quantitative judgments
   about uncertainty as well as support a well-defined decision process
   without many drawbacks of current approaches. We describe an application
   to long-term water planning in California. (c) 2006 Elsevier Ltd. All
   rights reserved.},
