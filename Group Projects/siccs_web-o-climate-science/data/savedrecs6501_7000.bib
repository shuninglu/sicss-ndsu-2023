@inproceedings{ WOS:000326729000003,
Author = {Key, Richard and Sander, Stanley and Eldering, Annmarie and Blavier,
   Jean-Francois and Bekker, Dmitriy and Manatt, Ken and Rider, David and
   Wu, Yen-Hung},
Editor = {Shen, SS and Lewis, PE},
Title = {The Geostationary Fourier Transform Spectrometer},
Booktitle = {IMAGING SPECTROMETRY XVII},
