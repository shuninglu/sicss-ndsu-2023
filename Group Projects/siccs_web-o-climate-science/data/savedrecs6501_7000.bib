@article{ WOS:000214641600001,
Author = {Campbell, Scott W. and Park, Yong Jin},
Title = {Social Implications of Mobile Telephony: The Rise of Personal
   Communication Society},
Journal = {SOCIOLOGY COMPASS},
Year = {2008},
Volume = {2},
Number = {2},
Pages = {371-387},
Month = {MAR},
Abstract = {The media landscape has changed dramatically in recent decades, from one
   predominated by traditional mass communication formats to today's more
   personalized network environment. Mobile communication plays a central
   role in this transition, with adoption rates that surpass even those of
   the Internet. This essay argues that the widespread diffusion and use of
   mobile telephony is iconic of a shift toward a new `personal
   communication society', evidenced by several key areas of social change,
   including symbolic meaning of the technology, new forms of coordination
   and social networking, personalization of public spaces, and the mobile
   youth culture. The conclusion speculates on future trends in the
   sociotechnological climate.},
