@article{ WOS:000771676800001,
Author = {Oubennaceur, Khalid and Chokmani, Karem and Lessard, Florence and
   Gauthier, Yves and Baltazar, Catherine and Toussaint, Jean-Patrick},
Title = {Understanding Flood Risk Perception: A Case Study from Canada},
