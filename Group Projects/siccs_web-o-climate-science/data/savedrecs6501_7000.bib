@article{ WOS:000818656300001,
Author = {Firoozabadi, Zahra Dehghani and Nikkhah, Hamid Reza and Foruzesh, Peyman},
Title = {Study of relationship between morpho-physiological traits and grain
   yield under terminal drought stress conditions in barley genotypes},
Journal = {CEREAL RESEARCH COMMUNICATIONS},
