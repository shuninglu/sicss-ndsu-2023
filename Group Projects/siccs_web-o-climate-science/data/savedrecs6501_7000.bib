Editor = {Remondino, F and ElHakim, S},
Title = {USING TERRESTRIAL LASER SCANNING AND LIDAR DATA FOR PHOTO-REALISTIC
   VISUALISATION OF CLIMATE IMPACTS AT HERITAGE SITES},
Booktitle = {4TH ISPRS INTERNATIONAL WORKSHOP 3D-ARCH 2011: 3D VIRTUAL RECONSTRUCTION
   AND VISUALIZATION OF COMPLEX ARCHITECTURES},
