@inproceedings{ WOS:000350263700056,
Author = {Yu Yang},
Editor = {Wu, Y and Li, Y},
Title = {Paddy Growers' Attitudes Toward Climate Risks: Willingness to Pay for
   Disaster Insurance},
Booktitle = {PROCEEDINGS OF THE 6TH (2014) INTERNATIONAL CONFERENCE ON FINANCIAL RISK
   AND CORPORATE FINANCE MANAGEMENT, VOLS. I AND II},
Year = {2014},
Pages = {330-335},
Note = {6th International Conference on Financial Risk and Corporate Finance
   Management, Dalian, PEOPLES R CHINA, JUN 30-JUL 01, 2014},
Organization = {Dalian Univ Technol, Fac Management \& Econ},
Abstract = {Natural disasters may increase in frequency and severity in the future
   as a result of climate change. This study elicits individual Paddy
   grower's risk beliefs and the demand for low-probability, high-impact
   windstorm insurance using the contingent valuation survey method among
   Paddy growers in Panjin city, Liaoning of China. The results show that
   WTP for windstorm insurance is less than proportionally related to
   increased windstorm probabilities that to sampled Paddy growers in the
   questionnaire. Communication of baseline probabilities and changes in
   windstorm probabilities using risk graders facilitate the comprehension
   of risk by respondents, and has a considerable effect on the level of
   the WTP and its sensitivity to probability changes. The results indicate
   that the current ex post government compensation scheme of windstorm
   damage lowers demand for private crop disaster insurance.},
