@inproceedings{ WOS:000306342400002,
Author = {Hilts, Andrew and Yu, Eric},
Editor = {Atzmueller, M and Hotho, A and Strohmaier, M and Chin, A},
Title = {Intentional Modeling of Social Media Design Knowledge for
   Government-Citizen Communication},
Booktitle = {ANALYSIS OF SOCIAL MEDIA AND UBIQUITOUS DATA},
Series = {Lecture Notes in Artificial Intelligence},
Year = {2011},
Volume = {6904},
Pages = {20-36},
Note = {Modeling Social Media Workshop (MSM 2010)Mining Ubiquitous and Social
   Environments International Workshop (MUSE 2010), Toronto,
   CANADABarcelona, SPAIN, JUN 13, 2010SEP 20, 2010},
Abstract = {Social media can be employed as powerful tools for enabling broad
   participation in public policy making. However, variations in the design
   of a social media technology system can lead to different levels or
   kinds of engagement, including low participation or polarized
   interchanges. An effective means toward learning of and analyzing the
   complex motivations, expectations, and actions among various actors in
   political communication can help designers create satisfactory social
   media systems.
   This paper uses the i{*} modeling framework to analyze the impact that
   alternative configurations of a social media technology can have on the
   goals and relationships of the actors involved. In doing so, we
   demonstrate and provide preliminary validation for a research-informed
   model creation and analysis approach to assessing competing design
   alternatives in an online climate change debate community.},
