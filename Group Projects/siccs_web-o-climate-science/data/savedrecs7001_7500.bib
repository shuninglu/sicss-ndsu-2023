@article{ WOS:000776953600001,
Author = {Zachary, Daniel S.},
Title = {Modelling shifts in social opinion through an application of classical
   physics},
Journal = {SCIENTIFIC REPORTS},
Year = {2022},
Volume = {12},
Number = {1},
Month = {MAR 31},
Abstract = {This paper explores the abstraction of classical physics and applies
   several metrics that explore the evolution of social opinion. These
   metrics include an abstraction of Newtonian kinematics: mass, position,
   speed, acceleration, and Newtonian dynamics, an abstraction of force.
   Poll data is fit to a 2nd-order polynomial and a logistic function.
   These fits are used to understand the acceleration of opinion shift, and
   we explore recent social, cultural, and environmental trends, such as
   views on global climate change. We compare our results with the
   evolution of communication technologies and time spent on devices over
   the past 120 years. We show that the model connects the evolution in
   opinion with an abstraction of a Galilean concept: acceleration is
   independent of mass. Finally, we discuss the model of social
   polarization and the non-linear effect of media such as echo chambers.},
