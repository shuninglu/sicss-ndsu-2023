@inproceedings{ WOS:000947273300087,
Author = {Pandey, Priya and Matta, Gunjan and Aggarwal, Monika},
Book-Group-Author = {IEEE},
Title = {Performance comparisons between Avalanche and PIN photodetectors for use
   in underwater optical wireless communication systems},
Booktitle = {OCEANS 2021: SAN DIEGO - PORTO},
Year = {2021},
Note = {OCEANS Conference, ELECTR NETWORK, SEP 20-23, 2021},
Abstract = {In underwater optical wireless communication, transmitting information
   is of enormous interest in the military, scientific community, and
   industry, as it has an essential role in surveillance, pollution
   monitoring, oil control and maintenance, climate change monitoring, and
   various oceanography research. Large number of unmanned devices and
   vehicles have been deployed underwater, to smoothen all these actions.
   In this paper, the performance comparison of the PIN photodetector and
   APD (Avalanche photodetector) in an underwater optical wireless
   communication is investigated. In this paper, the main aim is to
