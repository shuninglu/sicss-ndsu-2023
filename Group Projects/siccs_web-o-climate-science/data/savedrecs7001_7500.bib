@article{ WOS:000548005000001,
Author = {Jang, Hyongdoo and Topal, Erkan},
Title = {Transformation of the Australian mining industry and future prospects},
Journal = {MINING TECHNOLOGY-TRANSACTIONS OF THE INSTITUTIONS OF MINING AND
