@inproceedings{ WOS:000084758600001,
Author = {Beamish, RJ and Mahnken, C},
Book-Group-Author = {UNIV. OF ALASKA SEA GRANT COLLEGE PROGRAM
   UNIV. OF ALASKA SEA GRANT COLLEGE PROGRAM
   UNIV. OF ALASKA SEA GRANT COLLEGE PROGRAM},
