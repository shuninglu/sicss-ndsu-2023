@article{ WOS:000534126000001,
Author = {Sager, Fritz and Mavrot, Celine and Hinterleitner, Markus and Kaufmann,
   David and Grosjean, Martin and Stocker, Thomas F.},
Title = {Utilization-focused scientific policy advice: a six-point checklist},
Journal = {CLIMATE POLICY},
