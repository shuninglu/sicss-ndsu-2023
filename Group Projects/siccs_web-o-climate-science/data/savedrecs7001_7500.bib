Author = {Palginomm, Valdeko and Orviku, Kaarel and Suursaar, Ulo and Kont, Are
   and Tonisson, Hannes and Rivis, Reimo},
Title = {Lessons Learned from Record-High Storm Surges and Associated Inundations
   in Parnu, SW Estonia},
