@article{ WOS:000266610100006,
Author = {Laothawornkitkul, Jullada and Taylor, Jane E. and Paul, Nigel D. and
   Hewitt, C. Nicholas},
Title = {Biogenic volatile organic compounds in the Earth system},
Journal = {NEW PHYTOLOGIST},
Year = {2009},
Volume = {183},
Number = {1},
Pages = {27-51},
Abstract = {Biogenic volatile organic compounds produced by plants are involved in
   plant growth, development, reproduction and defence. They also function
   as communication media within plant communities, between plants and
   between plants and insects. Because of the high chemical reactivity of
   many of these compounds, coupled with their large mass emission rates
   from vegetation into the atmosphere, they have significant effects on
   the chemical composition and physical characteristics of the atmosphere.
   Hence, biogenic volatile organic compounds mediate the relationship
   between the biosphere and the atmosphere. Alteration of this
   relationship by anthropogenically driven changes to the environment,
   including global climate change, may perturb these interactions and may
   lead to adverse and hard-to-predict consequences for the Earth
   system.New Phytologist (2009) 183: 27-51doi:
