@article{ WOS:000415672200002,
Author = {Matlock, Teenie and Coe, Chelsea and Westerling, A. Leroy},
Title = {Monster wildfires and metaphor in risk communication},
Journal = {METAPHOR AND SYMBOL},
Year = {2017},
Volume = {32},
Number = {4},
Pages = {250-261},
Abstract = {This work examines the use and understanding of metaphor in wildfire
   discourse. We focus on the framing of wildfires as monsters, seen in
   statements such as Monster wildfire rages in Colorado and Two monster
   wildfires in Northern California are slowly being tamed, which reflect a
   wildfire is monster metaphor. Study 1 analyzes how and when this phrase
   is used in TV news reports of wildfires, and Study 2A and Study 2B
   investigate how it influences reasoning about risks associated with
   wildfire. The results show that metaphor is widely used in framing news
   reports about significant wildfires, and that its use influences how
   people reason about them. The work is part of a project aimed at
   developing better ways to communicate about risks related to natural
   events and climate change.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
