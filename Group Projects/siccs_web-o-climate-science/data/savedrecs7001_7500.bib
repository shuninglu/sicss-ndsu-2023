@inproceedings{ WOS:000542972500011,
Author = {Schneider, Toby and Schmidt, Henrik},
Book-Group-Author = {IEEE},
Title = {NETSIM: A Realtime Virtual Ocean Hardware-in-the-loop Acoustic Modem
   Network Simulator},
Booktitle = {2018 FOURTH UNDERWATER COMMUNICATIONS AND NETWORKING CONFERENCE (UCOMMS)},
Year = {2018},
Note = {4th Underwater Communications and Networking Conference (UComms),
   Lerici, ITALY, AUG 28-30, 2018},
Organization = {IEEE; Allied Command Transformat; Off Naval Res Sci \& Technol; IEEE
   Ocean Engn Soc; Kongsberg; Atlas Elektronik; Evo Log; S \& T Org; CMRE;
   Global Off Naval Res Sci \& Technol},
Abstract = {This paper presents netsim, a combined software/hardware system for
   performing realtime realistic operation of autonomous underwater
   vehicles (AUVs) with acoustic modem telemetry in a virtual ocean
   environment. The design of the system is flexible to the choice of
   physical link hardware, allowing for the system to be tested against
   existing and new modems. Additionally, the virtual ocean channel
   simulator is designed to perform in real time by coupling less frequent
   asynchronous queries to high-fidelity models of the ocean environment
   and acoustic propagation with frequent pertubation-based updates for the
   exact position of the simulated AUVs. The results demonstrate the
   performance of this system using the WHOI Micro-Modem 2 hardware in the
   virtual ocean environment of the Arctic Beaufort Sea around 73 degrees
   latitude. The acoustic environment in this area has changed dramatically
   in recent years due to the changing climate.},
