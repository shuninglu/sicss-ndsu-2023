@article{ WOS:000590098800001,
Author = {Razavi-Termeh, Seyed Vahid and Khosravi, Khabat and Sadeghi-Niaraki,
   Abolghasem and Choi, Soo-Mi and Singh, Vijay P.},
Title = {Improving groundwater potential mapping using metaheuristic approaches},
