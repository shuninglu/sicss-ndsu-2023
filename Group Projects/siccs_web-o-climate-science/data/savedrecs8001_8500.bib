@inproceedings{ WOS:000404786200047,
Author = {Grisa, Thomas M.},
Editor = {Dunn, CN and VanWeele, B},
Title = {Communicating the Severity of Rain Storms to the Public Using a
   Dimensionless Scale Instead of Probabilities or Recurrence Intervals},
Booktitle = {WORLD ENVIRONMENTAL AND WATER RESOURCES CONGRESS 2017: WATERSHED
   MANAGEMENT, IRRIGATION AND DRAINAGE, AND WATER RESOURCES PLANNING AND
   MANAGEMENT},
Year = {2017},
Pages = {549-557},
Note = {17th Annual World Environmental and Water Resources Congress,
   Sacramento, CA, MAY 21-25, 2017},
Organization = {Amer Soc Civil Engineers; Amer Soc Civil Engineers, Environm \& Water
   Resources Inst},
Abstract = {The public does not understand recurrence intervals for 100-year storms,
   which can occur more frequently than once every 100 years. A better way
   of communicating the severity of these rain storms to the public is to
   use a dimensionless index similar to ratings for other natural disasters
   (e.g. Richter scale for earthquakes, Fujita scale for tornadoes, and
   Saffir-Simpson scale for hurricanes). The public understands the ratings
   for these events which use a simple scale to rate the severity of the
   event, not the rarity of the event.
   This paper proposes a dimensionless scale for severe rainstorms,
   building upon existing data that rates the event on a simple scale,
   based on its severity. This helps the public understand that extreme
   rains can occur more frequently than otherwise thought, incenting the
   public to take steps to protect themselves from the impact of these
   severe storms, instead of falsely assuming that they are ``safe{''} for
   another 99 years. This will become more important as the climate
   continues to change, which can change the frequency of occurrence of
   these events.},
