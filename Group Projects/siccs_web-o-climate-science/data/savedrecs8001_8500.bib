@article{ WOS:000453975600001,
Author = {Few, Roger and Morchain, Daniel and Spear, Dian and Mensah, Adelina and
   Bendapudi, Ramkumar},
Title = {Transformation, adaptation and development: relating concepts to
   practice},
Journal = {PALGRAVE COMMUNICATIONS},
Year = {2017},
Volume = {3},
Month = {SEP 1},
Abstract = {In recent years there has been a growing number of academic reviews
   discussing the theme of transformation and its association with
   adaptation to climate change. On the one hand this has stimulated
   exchange of ideas and perspectives on the parameters of transformation,
   but it has also given rise to confusion in terms of identifying what
   constitutes a non-incremental form of adaptation on the ground. What
   this article aims to do instead is help researchers and practitioners
   relate different interpretations of transformation to practice by
   proposing a typological framework for categorising forms of change that
   focuses on mechanisms and objectives. It then discusses how these
   categorisations link to the broader conceptions and critiques noted
   above, with the idea that this will enable those who seek to analyse or
   plan adaptation to better analyse what types of action are potentially
   constitutive of transformation. In doing so, it should equally assist in
   the identification and specification of critical questions that need to
   be asked of such activity in relation to issues of sustainability and
   equity.},
Publisher = {PALGRAVE MACMILLAN LTD},
Address = {BRUNEL RD BLDG, HOUNDMILLS, BASINGSTOKE RG21 6XS, HANTS, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Few, R (Corresponding Author), Univ East Anglia, Sch Int Dev, Norwich Res Pk, Norwich, Norfolk, England.
