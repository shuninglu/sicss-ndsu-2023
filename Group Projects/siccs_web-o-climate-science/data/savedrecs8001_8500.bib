@article{ WOS:000809196700001,
Author = {Gili, Stefania and Vanderstraeten, Aubry and Chaput, Amelie and King,
   James and Gaiero, Diego M. and Delmonte, Barbara and Vallelonga, Paul
   and Formenti, Paola and Di Biagio, Claudia and Cazanau, Mathieu and
   Pangui, Edouard and Doussin, Jean-Francois and Mattielli, Nadine},
Title = {South African dust contribution to the high southern latitudes and East
   Antarctica during interglacial stages},
Journal = {COMMUNICATIONS EARTH \& ENVIRONMENT},
Year = {2022},
Volume = {3},
Number = {1},
Month = {JUN 9},
Abstract = {Mineral dust is a natural tracer of atmospheric composition and climate
   variability. Yet, there is still much to be known about the Southern
   Hemisphere dust cycle. Major efforts have attempted to solve the puzzle
   of the origin of the potential source areas contributing dust to the
   Southern Ocean and East Antarctica. Here we present a comprehensive
   geochemical characterization of a source area, whose role as a dust
   supplier to high latitude environments has significantly been
   underestimated. Sediments collected within the major dust-producing
   areas along the Namibian coast in Southern Africa (Kuiseb, Omaruru and
