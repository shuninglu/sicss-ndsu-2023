@article{ WOS:000223085400041,
Author = {Peeters, FJC and Acheson, R and Brummer, GJA and de Ruijter, WPM and
   Schneider, RR and Ganssen, GM and Ufkes, E and Kroon, D},
Title = {Vigorous exchange between the Indian and Atlantic oceans at the end of
   the past five glacial periods},
