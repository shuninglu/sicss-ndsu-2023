@article{ WOS:000470621600001,
Author = {Nhapi, Tatenda G.},
Title = {Natural Resource Degradation through Tobacco Farming in Zimbabwe: CSR
   Implications and the Role of the Government},
Journal = {COMMUNICATIO-SOUTH AFRICAN JOURNAL FOR COMMUNICATION THEORY AND RESEARCH},
Year = {2019},
Volume = {45},
Number = {3, SI},
Pages = {23-39},
Month = {JUL 3},
Abstract = {Tobacco farming exemplifies the discussion of the impact of Zimbabwe's
   Fast Track Land Reform (FTLR) in the economic domain. However, a
   disjuncture between its success and environmental implications is
   illustrated by the ensuing socio-ecological implications at play in the
   different districts in which tobacco is grown. Also, there is a dearth
   of vibrant Corporate Social Responsibility programmes to offset
   environmental degradation caused by tobacco farming as a result of tree
   cutting. This article is based on a critical review of Zimbabwean
   natural resources and socio-economic development literature to draw
   attention to some of the problematic and contradictory issues underlying
   natural resources management in economic development domains especially
   given how, post FTLR, tobacco growing has become one the major export
   cash crops on which the economy is hinged. In an African
   resource-harnessing context, marginalised and vulnerable local
   communities are most often direct casualties of environmental and social
   impacts engendered by the extraction of natural resources. This article
   focuses on the fundamental questions of sustainable development, climate
   change, and Zimbabwean tobacco farming trends. Finally, pathways are
   offered for robust CSR interventions that enhance sustainable
   development and reversing tobacco farming-induced deforestation in
   Zimbabwe.},
