@article{ WOS:000309742400046,
Author = {Romo, J. A. and Maruri, M. and Perez-Fontan, F. and Fernandez, I.},
Title = {Characterization of Maximum Radar Reflectivity Height During Stratiform
   Rain Events},
Journal = {IEEE TRANSACTIONS ON ANTENNAS AND PROPAGATION},
Year = {2012},
Volume = {60},
Number = {10},
Pages = {4884-4891},
Month = {OCT},
Abstract = {The effects of rain on terrestrial and satellite communication systems,
   especially at frequencies above 10 GHz, have been statistically dealt
   with at length. It is also well known that rain height plays an
   important role in signal fading and co-channel interference due to
   scattering. The rain height is directly related to the 0 degrees C
   isotherm. At this height hydrometeors change from solid to liquid state
   in the melting layer, increasing their reflectivity and causing the
   bright band effect in radar measurements. The bright band is defined by
   the top, bottom and maximum reflectivity heights. The peak reflectivity
   height can be obtained from radar volume scans. This paper presents a
   statistical characterization of the time and space variability of the
   maximum reflectivity height during stratiform rain events. This paper
   also focuses on the dependence between simultaneous meteorological
   parameters derived from weather radar and from surface automatic weather
   observation stations at local scale in a temperate climate region. The
   data used in this study was obtained from 2006 to 2011.},
