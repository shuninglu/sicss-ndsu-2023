@article{ WOS:000867312100008,
Author = {Ultee, Lizz and Felikson, Denis and Minchew, Brent and Stearns, Leigh A.
   and Riel, Bryan},
Title = {Helheim Glacier ice velocity variability responds to runoff and terminus
   position change at different timescales},
Journal = {NATURE COMMUNICATIONS},
