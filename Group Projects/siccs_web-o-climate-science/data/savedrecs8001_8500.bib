@article{ WOS:000435610300159,
Author = {Jang, Jeachul and Kang, Eun-Chul and Lee, Hyung Keun and Jeong, Siyoung
   and Park, Seong-Ryong},
Title = {Energy Demand Comparison between Hollow Fiber Membrane Based
   Dehumidification and Evaporative Cooling Dehumidification Using TRNSYS},
Journal = {ENERGIES},
