Author = {Colding, Johan and Wallhagen, Marita and Sorqyist, Patrik and Marcus,
   Lars and Hillman, Karl and Samuelsson, Karl and Barthel, Stephan},
Title = {Applying a Systems Perspective on the Notion of the Smart City},
