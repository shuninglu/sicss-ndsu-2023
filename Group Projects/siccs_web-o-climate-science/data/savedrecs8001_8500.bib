@article{ WOS:000501244000002,
Author = {Mekala, Mahammad Shareef and Viswanathan, P.},
Title = {(t,n): Sensor Stipulation with THAM Index for Smart Agriculture
   Decision-Making IoT System},
Journal = {WIRELESS PERSONAL COMMUNICATIONS},
