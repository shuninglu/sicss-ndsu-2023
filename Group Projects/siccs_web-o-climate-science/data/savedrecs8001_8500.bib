@article{ WOS:000579682400016,
Author = {Wu, Han and Wang, Junwu},
Title = {WATERLOGGING HAZARD ASSESSMENT OF DEEP FOUNDATION PIT PROJECTS BASED ON
   FOWA OPERATOR AND GRAY CLUSTERING},
Journal = {JOURNAL OF NONLINEAR AND CONVEX ANALYSIS},
Year = {2020},
Volume = {21},
Number = {8, SI},
Pages = {1839-1849},
Note = {5th International Conference on Fuzzy Systems and Data Mining (FSDM)
   held in conjunction with the 9th International Conference on
   Electronics, Communications and Networks (CECNet), Kitakyushu, JAPAN,
   OCT 18-21, 2019},
Abstract = {Because of global climate change and urbanization, waterlogging
   disasters occur frequently, posing great challenges to the safety of
   deep foundation pit construction. To scientifically evaluate
   waterlogging hazard, this paper put forward an evaluation model based on
   fuzzy ordered weighted averaging (FOWA) operator and grey clustering.
   Firstly, the index system for waterlogging hazard assessment of deep
   foundation pit projects was constructed by literature review. Then, FOWA
   operator was used to calculate the weights of all indicators, which
   could reduce the subjectivity and amount of weight calculation. Finally,
   grey clustering, which effectively solved the fuzziness and grey of
   assessment, was selected to evaluate waterlogging hazard. Three deep
   foundation pit projects of Chengdu Metro Line 11 in China were assessed
   by this model. The results indicated intensity of rainfall and depth of
   foundation had the greatest impact, which were the key factors in
