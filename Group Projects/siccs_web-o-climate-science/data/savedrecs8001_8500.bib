@article{ WOS:000744659900008,
Author = {Homyack, Jessica and Sucre, Eric and Magalska, Lauren and Fox, Thomas},
Title = {Research and Innovation in the Private Forestry Sector: Past Successes
   and Future Opportunities},
Journal = {JOURNAL OF FORESTRY},
