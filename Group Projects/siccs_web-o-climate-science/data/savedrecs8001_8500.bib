@inproceedings{ WOS:000669813301128,
Author = {Wright, Lynn D. and Caruson, Kiki and D'Elia, Christopher and Draayer,
   Jerry and Nichols, Reid and Weiss, Robert and Zarillo, Gary},
Book-Group-Author = {IEEE},
Title = {Assessing and Planning for the Impacts of Storms, Flooding and Sea Level
   Rise on Vulnerable Gulf of Mexico Coastal Communities: A White Paper},
Booktitle = {GLOBAL OCEANS 2020: SINGAPORE - U.S. GULF COAST},
Series = {OCEANS-IEEE},
