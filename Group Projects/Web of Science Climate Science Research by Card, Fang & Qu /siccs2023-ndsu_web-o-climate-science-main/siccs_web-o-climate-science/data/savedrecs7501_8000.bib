@article{ WOS:000358858500020,
Author = {Feng, Kuishuang and Davis, Steven J. and Sun, Laixiang and Hubacek,
   Klaus},
Title = {Drivers of the US CO2 emissions 1997-2013},
Journal = {NATURE COMMUNICATIONS},
Year = {2015},
Volume = {6},
Month = {JUL},
Abstract = {Fossil fuel CO2 emissions in the United States decreased by B11\%
   between 2007 and 2013, from 6,023 to 5,377 Mt. This decline has been
   widely attributed to a shift from the use of coal to natural gas in US
   electricity production. However, the factors driving the decline have
   not been quantitatively evaluated; the role of natural gas in the
   decline therefore remains speculative. Here we analyse the factors
   affecting US emissions from 1997 to 2013. Before 2007, rising emissions
   were primarily driven by economic growth. After 2007, decreasing
   emissions were largely a result of economic recession with changes in
   fuel mix (for example, substitution of natural gas for coal) playing a
   comparatively minor role. Energy-climate policies may, therefore, be
   necessary to lock-in the recent emissions reductions and drive further
   decarbonization of the energy system as the US economy recovers and
   grows.},
