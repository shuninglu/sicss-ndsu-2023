@inproceedings{ WOS:000400645600002,
Author = {Monzon, Andres and Garcia-Castro, Alvaro and Valdes, Cristina},
Editor = {Kabashkin, I and Yatskiv, I and Prentkovskis, O},
Title = {Methodology to Assess the Effects of ICT-measures on Emissions. The Case
   Study of Madrid},
