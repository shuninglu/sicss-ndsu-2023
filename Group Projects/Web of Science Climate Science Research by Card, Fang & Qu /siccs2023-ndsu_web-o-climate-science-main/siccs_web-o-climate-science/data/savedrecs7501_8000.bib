@article{ WOS:000392409200027,
Author = {Wei Zhen-Feng and Huang Gang and Huang Yi},
Title = {SPATIAL AND TEMPORAL CHANGE OF NPP AND ITS RESPONSE TO CLIMATE IN RECENT
   14 YEARS IN THREE-RIVER HEADWATERS REGION},
Journal = {OXIDATION COMMUNICATIONS},
Year = {2016},
Volume = {39},
Number = {4, 2},
Pages = {3490-3498},
Abstract = {This study analysed the spatiotemporal variation of net primary
   productivity (NPP) for different vegetation types by using the Carnegie
   Ames Stanford Approach (CASA) model, and the driving forces caused the
   NPP changed were also investigated by using partial correlation
   analysis. The results showed that during 1999-2012, the spatial pattern
   of NPP across the TRHR characterised as an increasing trend from
   northwest to southeast. In recent 14 years, the spatial averaged NPP in
   three-river headwaters showed a weak increasing trend. Most areas showed
   a positive correlation between NPP and annual precipitation, with
   positive correlation and negative correlation being 89.32 and 10.68\%,
   respectively. The correlation between NPP and annual mean temperature
   showed weak correlation was detected in most areas.},
