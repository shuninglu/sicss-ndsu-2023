@article{ WOS:000439712900003,
Author = {Petry, V, Maria and Valls, Fernanda C. L. and Petersen, Elisa S. and
   Finger, Julia V. G. and Kruger, Lucas},
Title = {Population trends of seabirds at Stinker Point, Elephant Island,
   Maritime Antarctica},
Journal = {ANTARCTIC SCIENCE},
