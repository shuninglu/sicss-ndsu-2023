@incollection{ WOS:000431677800005,
Author = {Hunt, James H. and Toth, Amy L.},
Editor = {Rubenstein, DR and Abbot, P},
Title = {Sociality in Wasps},
Booktitle = {COMPARATIVE SOCIAL EVOLUTION},
Year = {2017},
Pages = {84-123},
Abstract = {Wasps encompass solitary, communal, and facultative, obligate, and
   swarm-founding social species and are important model organisms for
   study of the origin and elaboration of insect sociality. Common names
   for social species are hover wasps, paper wasps, yellowjackets, hornets,
   and swarm-founding wasps. Excepting a few communal species, all social
   wasps are in a single family, Vespidae. Social wasps occur worldwide
   except in extreme dry or cold climates. Nourishment dynamics and
   dominance interactions shape intra-colony social structure.
   Communication can be chemical, vibrational, or visual. Differentiation
   of egg-layers and workers can occur among adults or larvae via
