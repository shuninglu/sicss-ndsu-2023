@article{ WOS:000697039000001,
Author = {van Stekelenburg, Aart and Schaap, Gabi and Veling, Harm and Buijzen,
   Moniek},
Title = {Boosting Understanding and Identification of Scientific Consensus Can
   Help to Correct False Beliefs},
Journal = {PSYCHOLOGICAL SCIENCE},
Year = {2021},
