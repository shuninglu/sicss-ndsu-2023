@article{ WOS:000418779000014,
Author = {Shi, Feng and Shi, Yongren and Dokshin, Fedor A. and Evans, James A. and
   Macy, Michael W.},
Title = {Millions of online book co-purchases reveal partisan differences in the
   consumption of science},
Journal = {NATURE HUMAN BEHAVIOUR},
Year = {2017},
Volume = {1},
Number = {4},
Month = {APR},
Abstract = {Passionate disagreements about climate change, stem cell research and
   evolution raise concerns that science has become a new battlefield in
   the culture wars. We used data derived from millions of online
   co-purchases as a behavioural indicator for whether shared interest in
   science bridges political differences or selective attention reinforces
   existing divisions. Findings reveal partisan preferences both within and
   across scientific disciplines. Across fields, customers for liberal or
   `blue' political books prefer basic science (for example, physics,
   astronomy and zoology), whereas conservative or `red' customers prefer
   applied and commercial science (for example, criminology, medicine and
   geophysics). Within disciplines, `red' books tend to be co-purchased
   with a narrower subset of science books on the periphery of the
   discipline. We conclude that the political left and right share an
   interest in science in general, but not science in particular. This
   underscores the need for research into remedies that can attenuate
   selective exposure to `convenient truth', renew the capacity for science
   to inform political debate and temper partisan passions.},
