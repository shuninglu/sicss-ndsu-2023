@inproceedings{ WOS:000920419800042,
Author = {Becher, Kilian and Schaefer, Mirko and Schroepfer, Axel and Strufe,
   Thorsten},
Book-Group-Author = {IEEE},
Title = {Efficient Public Verification of Confidential Supply-Chain Transactions},
Booktitle = {2022 IEEE CONFERENCE ON COMMUNICATIONS AND NETWORK SECURITY (CNS)},
Series = {IEEE Conference on Communications and Network Security},
Year = {2022},
Pages = {308-316},
Note = {IEEE Conference on Communications and Network Security (CNS), Austin,
   TX, OCT 03-05, 2022},
Organization = {IEEE},
Abstract = {Ensuring sustainable sourcing of crude materials and production of goods
   is a pressing problem in consideration of the growing world population
   and rapid climate change. Supply-chain traceability systems based on
   distributed ledgers can help to enforce sustainability policies like
   production limits.
   We propose two mutually independent distributed-ledger-based protocols
   that enable public verifiability of policy compliance. They are designed
   for different supply-chain scenarios and use different privacy-enhancing
   technologies in order to protect confidential supply-chain data: secret
   sharing and homomorphic encryption. The protocols can be added to
   existing supply-chain traceability solutions with minor effort. They
   ensure confidentiality of transaction details and offer public
   verifiability of producers' compliance, enabling institutions and even
   end consumers to evaluate sustainability of supply chains. Through
   extensive theoretical and empirical evaluation, we show that both
   protocols perform verification for lifelike supply-chain scenarios in
   perfectly practical time.},
Publisher = {IEEE},
Address = {345 E 47TH ST, NEW YORK, NY 10017 USA},
Type = {Proceedings Paper},
Language = {English},
Affiliation = {Becher, K (Corresponding Author), Tech Univ Dresden, Dresden, Germany.
