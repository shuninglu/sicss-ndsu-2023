@article{ WOS:000894342500001,
Author = {Pandzic, Kreso and Likso, Tanja and Bonacci, Ognjen},
Title = {A Review of Extreme Air Temperature Analysis in Croatia},
Journal = {ATMOSPHERE},
Year = {2022},
