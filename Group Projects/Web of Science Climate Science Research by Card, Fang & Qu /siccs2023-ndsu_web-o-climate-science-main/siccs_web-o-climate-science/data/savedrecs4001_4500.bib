@article{ WOS:000865479300010,
Author = {Arvai, Joseph and Gregory, Robin},
Title = {Beyond choice architecture: a building code for structuring climate risk
   management decisions},
Journal = {BEHAVIOURAL PUBLIC POLICY},
Year = {2021},
Volume = {5},
Number = {4, SI},
Pages = {556-575},
Month = {OCT},
Abstract = {Although the need for urgent climate change action is clear, insights
   about how to make better climate risk management decisions are limited.
   While significant attention from behavioral researchers has focused on
   choice architecture, we argue that many of the contexts for addressing
   climate risks require increased attention to the needs of a deliberative
   and dynamic choice environment. A key facet of this kind of decision is
   the need for decision-makers and stakeholders to identify and balance
   conflicting economic, social and environmental objectives. This
   recognition of difficult, context-specific trade-offs highlights the
   need for structuring the decision-making process so that objectives are
   clearly articulated and prioritized. Equally, policy analyses and
   deliberations must effectively link priorities with climate risk
   management options. This restructuring of decision-making about climate
   change calls for more than a nudge. Scientific and technical efforts
   must be redirected to help stakeholders and decision-makers better
   understand the diverse implications of climate change management
   alternatives and to become better equipped to take actions commensurate
   with the urgency of the problem.},
