@article{ WOS:000766398100001,
Author = {DePalma, Victoria},
Title = {The Role of Religiosity on Deontological Climate Message Acceptance: A
   US Survey},
Journal = {HUMAN ECOLOGY},
Year = {2022},
Volume = {50},
Number = {2},
Pages = {363-372},
Month = {APR},
Abstract = {Through analysis of a nationally representative survey (n = 1,200), I
   demonstrate that religious individuals are more likely than their
   non-religious counterparts to agree with a deontological reason to
   address climate change. Findings support existing literature showing
   religious individuals are more likely to assess ethical situations
   deontologically, that is, based on whether they align with duties,
   norms, rules, or obedience to an authority. While religiosity tracked
   greater agreement with a deontological climate message, there was no
   correlation to specific religious traditions, suggesting that religious
   importance rather than religious group identity is the stronger
   predictor of this preference. Further, the propensity for religious
   individuals to assess ethical situations deontologically persists when
   exposed to ethical climate messages, illustrating how ethical appeals
   could be used to increase message acceptability among targeted
   demographic groups. Overall, this study justifies continued research
   into the role of ethics in climate communication.},
