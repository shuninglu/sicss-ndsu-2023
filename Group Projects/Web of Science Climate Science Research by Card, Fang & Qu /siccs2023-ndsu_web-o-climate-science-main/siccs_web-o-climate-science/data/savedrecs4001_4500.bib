@article{ WOS:000403246500014,
Author = {Zanin, Marina and Tessarolo, Geiziane and Machado, Nathalia and
   Albernaz, Ana Luisa M.},
Title = {Climatically-mediated landcover change: impacts on Brazilian territory},
Journal = {ANAIS DA ACADEMIA BRASILEIRA DE CIENCIAS},
