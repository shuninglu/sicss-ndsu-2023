@article{ WOS:000374330100002,
Author = {Pulkkinen, Hannele and Roininen, Taneli and Katajajuuri, Juha-Matti and
   Jarvinen, Maija},
Title = {Development of a Climate Choice meal concept for restaurants based on
   carbon footprinting},
Journal = {INTERNATIONAL JOURNAL OF LIFE CYCLE ASSESSMENT},
