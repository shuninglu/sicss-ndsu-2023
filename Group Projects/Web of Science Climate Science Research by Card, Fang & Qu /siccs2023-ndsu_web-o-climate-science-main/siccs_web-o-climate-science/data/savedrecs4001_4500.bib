@article{ WOS:000260964100095,
Author = {Toth, Tamas and Villanyi, Judit and Balazs, Peter and Kapuszta, Joszef},
Title = {GLOBAL ENVIRONMENT CHANGE AND FOOD SECURITY: A SOCIO-ECONOMIC
   PERSPECTIVE},
Journal = {CEREAL RESEARCH COMMUNICATIONS},
Year = {2008},
Volume = {36},
