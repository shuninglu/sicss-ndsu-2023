Title = {The Role of Smart Flood Management in the Ancient Blue-Green
   Infrastructure},
Booktitle = {PROCEEDINGS OF THE 2020 IEEE INTERNATIONAL SYMPOSIUM ON TECHNOLOGY AND
   SOCIETY (ISTAS)},
