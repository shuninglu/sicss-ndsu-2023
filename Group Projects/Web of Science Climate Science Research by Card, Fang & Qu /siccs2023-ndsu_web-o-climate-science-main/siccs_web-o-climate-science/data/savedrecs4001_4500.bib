@article{ WOS:000429223600004,
Author = {Hestres, Luis E.},
Title = {Take Action Now: Motivational Framing and Action Requests in Climate
   Advocacy},
Journal = {ENVIRONMENTAL COMMUNICATION-A JOURNAL OF NATURE AND CULTURE},
Year = {2018},
Volume = {12},
Number = {4},
Pages = {462-479},
Abstract = {This article explores similarities and differences between
   Internet-mediated climate change advocacy organizations and their legacy
   counterparts. It relies on a content analysis of advocacy emails
   produced by Environmental Defense Fund, Natural Resources Defense
   Council, Sierra Club, Greenpeace USA, League of Conservation Voters,
   Climate Reality Project, 1Sky, and 350.org. The study finds differences
   and similarities in strategic Internet use, including greater emphasis
   by climate groups on high threshold, offline actions; greater emphasis
   by legacy groups on low threshold, online actions; and high reliance by
   both framings that demand accountability from political elites.
   Implications for Internet-mediated and climate advocacy are discussed.},
