@article{ WOS:000887967900013,
Author = {Lee, Benjamin R. and Miller, Tara K. and Rosche, Christoph and Yang,
   Yong and Heberling, J. Mason and Kuebbing, Sara E. and Primack, Richard
   B.},
Title = {Wildflower phenological escape differs by continent and spring
   temperature},
