@article{ WOS:000390870900057,
Author = {Tourani, Ahmadreza and Jamshidi, Amirhossein},
Title = {THE ROLE OF STRUCTURE NOVELTY OF SUSTAINABLE URBAN FORM IN ENERGETIC
   CITIES (CASE STUDIES OF MASDAR AND BUSHEHR CITIES)},
Journal = {TURKISH ONLINE JOURNAL OF DESIGN ART AND COMMUNICATION},
Year = {2016},
Volume = {6},
Pages = {3118-3130},
Month = {NOV},
Abstract = {Bushehr and Masdar are two of the most famous cities in warm and humid
   climate which are situated in the same macro climate of Persian Gulf
   region. This research tries to discuss one of the most important themes
   which threats the sustainability and comfortability of tomorrow's life.
   Analyzing Bushehr and Masdar as energetic cities was an effective way of
   finding the cities problems and the result led to the presentation of a
   more comprehensive pattern for planning an energetic city in warm and
   humid climate. For this purpose main questiones were found worthy of
   investigation: What characteristics should be presented in designing an
   energetic city?, What are the aims of changing a city into an energetic
   city?, In this paper we provide a model which responds to the
   sustainability of Bushehr and Masdar. This model uses five levels of
   activities, with each level responding to at least two significant
   characteristics of an energetic city with regard to the compact urban
   form. Ultimately the presented model was confirmed by the Delphi s
