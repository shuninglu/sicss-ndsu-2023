@article{ WOS:000849456400001,
Author = {Wooltorton, Sandra and Guenther, John and Poelina, Anne and Blaise,
   Mindy and Collard, Len and White, Peta},
Title = {Learning regenerative cultures: Indigenous nations in higher education
   renewal in Australia},
