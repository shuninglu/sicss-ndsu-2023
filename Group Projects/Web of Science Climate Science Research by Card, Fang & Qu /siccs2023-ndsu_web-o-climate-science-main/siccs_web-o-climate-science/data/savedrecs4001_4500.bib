@article{ WOS:000399358300134,
Author = {Nugraha, Fida and Ardiwinata, Maya and Kadiyono, Annisa and Iskandar,
   Tb. Zulrizka},
Title = {Psychological Climate and Psychological Contract Breach After Merger: A
   Preliminary Study},
Journal = {ADVANCED SCIENCE LETTERS},
Year = {2016},
Volume = {22},
Number = {12, SI},
Pages = {4536-4539},
Month = {DEC},
Note = {Advancement on Informatics, Business and Management International
   Conference (AIBMIC), Bandung, INDONESIA, MAY 31-JUN 02, 2016},
Organization = {Malaysia Tech Scientist Assoc},
Abstract = {Change in an organization will lead to other changes within it that
   affected its members. How employee perceived their relationship with the
   organization (psychological climate) change as well following the
   changes in organizational practices. Psychological climate will affect
   how employees perceived the organization in fulfilling its obligations
   towards them (psychological contract). Previous studies shown that
   unfavorable psychological climate will leads to psychological contract
   breach. Similar result reveal from the educational institution that
   resulted from merger in Bandung, Indonesia. Convenience sampling of 77
   lecturers, using Pearson correlation showed that the two are negatively
   correlated (r =-.315). Communication plays important role to minimize
   negative perceptions in the areas.},
