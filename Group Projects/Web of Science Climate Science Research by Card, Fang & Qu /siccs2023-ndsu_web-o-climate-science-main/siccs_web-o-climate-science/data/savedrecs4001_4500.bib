@incollection{ WOS:000674456500007,
Author = {Khan, A. Saleem and Chen, Robert S. and de Sherbinin, Alex},
Editor = {Filho, WL and Jacob, D},
Title = {COREDAR: A Coastal Climate Service Framework on Sea-Level Rise Risk
   Communication for Adaptation Policy Planning},
Booktitle = {HANDBOOK OF CLIMATE SERVICES},
