@article{ WOS:000394157300008,
Author = {Lim-Camacho, Lilly and Ariyawardana, Anoma and Lewis, Gemma K. and
   Crimp, Steven J. and Somogyi, Simon and Ridoutt, Brad and Howden, Stuart
   Mark},
Title = {Climate adaptation of food value chains: the implications of varying
   consumer acceptance},
Journal = {REGIONAL ENVIRONMENTAL CHANGE},
