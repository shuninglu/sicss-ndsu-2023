@article{ WOS:000812510900001,
Author = {Schleussner, Carl-Friedrich and Ganti, Gaurav and Rogelj, Joeri and
   Gidden, Matthew J.},
Title = {An emission pathway classification reflecting the Paris Agreement
   climate objectives},
