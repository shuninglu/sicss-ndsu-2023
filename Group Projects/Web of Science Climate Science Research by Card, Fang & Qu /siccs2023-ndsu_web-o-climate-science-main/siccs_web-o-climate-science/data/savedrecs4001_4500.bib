@article{ WOS:000421710600003,
Author = {Boehm, R.},
Title = {Reconstructing the climate of the 250 years of instrumental records at
   the northern border of the Mediterranean (the Alps)},
Journal = {NUOVO CIMENTO C-COLLOQUIA AND COMMUNICATIONS IN PHYSICS},
Year = {2006},
Volume = {29},
Number = {1},
Pages = {13-19},
Month = {JAN-FEB},
Abstract = {The paper provides a selection of first results based on a newly
   developed instrumental climate database for the European Alps and their
   wider surroundings. After an outline on data availability, network
   density, series durations and quality aspects some examples for the two
   main climate elements temperature and precipitation show some principal
   features of climate variability and trends in the region. Regional as
   well as seasonal differences are discussed. The overview closes with
   examples dealing with changes of climate variability: For temperature as
   well as for precipitation (the former stronger, the latter weaker and
   with regional modifications) inter-annual (-seasonal, -monthly)
   variability has not increased but decreased during the past two
   centuries of well-proved instrumental data in the study region.},
