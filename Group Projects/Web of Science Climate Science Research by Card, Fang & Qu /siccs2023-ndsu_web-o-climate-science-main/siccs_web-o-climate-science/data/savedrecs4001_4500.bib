@article{ WOS:000373680100007,
Author = {Depoers, Florence and Jeanjean, Thomas and Jerome, Tiphaine},
Title = {Voluntary Disclosure of Greenhouse Gas Emissions: Contrasting the Carbon
   Disclosure Project and Corporate Reports},
