@article{ WOS:000612234000001,
Author = {Dai, Zhen and Burns, Elizabeth T. and Irvine, Peter J. and Tingley,
   Dustin H. and Xu, Jianhua and Keith, David W.},
Title = {Elicitation of US and Chinese expert judgments show consistent views on
   solar geoengineering},
