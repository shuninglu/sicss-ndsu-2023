@article{ WOS:000230755300008,
Author = {Albrecht, J and Arts, B},
Title = {Climate policy convergence in Europe: an assessment based on National
   Communications to the UNFCCC},
Journal = {JOURNAL OF EUROPEAN PUBLIC POLICY},
Year = {2005},
Volume = {12},
Number = {5},
Pages = {885-902},
