@article{ WOS:000434688300001,
Author = {Hoffmann, Peter and Menz, Christoph and Spekat, Arne},
Title = {Bias adjustment for threshold-based climate indicators},
Journal = {ADVANCES IN SCIENCE AND RESEARCH},
Year = {2018},
Volume = {15},
