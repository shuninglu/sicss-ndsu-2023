Language = {English},
Affiliation = {Rode, DC (Corresponding Author), Carnegie Mellon Univ, Carnegie Mellon Elect Ind Ctr, Pittsburgh, PA 15213 USA.
   Rode, David C.; Fischbeck, Paul S., Carnegie Mellon Univ, Carnegie Mellon Elect Ind Ctr, Pittsburgh, PA 15213 USA.
   Fischbeck, Paul S., Carnegie Mellon Univ, Dept Social \& Decis Sci, Pittsburgh, PA 15213 USA.
   Fischbeck, Paul S., Carnegie Mellon Univ, Dept Engn \& Publ Policy, Pittsburgh, PA 15213 USA.},
DOI = {10.1504/IJGW.2021.112896},
ISSN = {1758-2083},
