@article{ WOS:000868475200003,
Author = {Sakamoto, Tatsuya and Takahashi, Motomitsu and Chung, Ming-Tsung and
   Rykaczewski, Ryan R. and Komatsu, Kosei and Shirai, Kotaro and Ishimura,
