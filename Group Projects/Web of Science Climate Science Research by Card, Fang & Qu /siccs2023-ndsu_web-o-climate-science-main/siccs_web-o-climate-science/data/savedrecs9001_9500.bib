@inproceedings{ WOS:000666945500102,
Author = {Zivic, Natasa},
Book-Group-Author = {IEEE},
Title = {Distributed Ledger Technology for Automotive Production 4.0},
Booktitle = {2020 28TH TELECOMMUNICATIONS FORUM (TELFOR)},
Year = {2020},
Pages = {399-401},
Note = {28th Telecommunications Forum (TELFOR), Belgrade, SERBIA, NOV 24-25,
   2020},
Organization = {Telecommunicat Soc; Univ Belgrade; IEEE Commun Soc; IEEE Reg 8; Telekom
   Srbija a d; VLATACOM d o o; Univ Belgrade, Sch Elect Engn; IEEE Serbia
   \& Montenegro COM Chapter; Nokia; Roaming Networks; VIP Mobile; Cisco;
   Telenor; TERI Engn; ERICSSON; Huawei; SBB; IRITEL a d; Inst Mihajlo
   Pupin},
Abstract = {Distributed Ledger Technology (DLT) is a relative new technology, which
   already has a significant impact, especially in a field of financing.
   Nevertheless, there are many more fields, like technical, social,
   juristic, security etc., which will be more and more influenced by the
   usage of DLT. One of these fields is an industrial one: Industry 4.0
   with its numerous technical and technological branches. There are
   revolutionary changes in all of these branches involved through the
   Internet of Things and through the high-level Robotics/Automation.
   One of integral components of the global economy, the automotive
   industry, has been faced many challenges in recent times like climate
   changes, new regulations, security and safety challenges of Car-to-Car
   and Car-to-Infrastructure communications, new hardware and software
   architecture, new functionalities and new business models. In order to
   successfully master new demands, new technologies and alternative
   business models are needed. Those technologies have to be integrated
   into Industry 4.0, in particularly Car Industry 4.0. One of these new
   technologies is DLT.
   This paper proposes the usage of DLT in an essential part of a Car
   Industry: its production. The car production is already fully automated
   and cannot be imagined without robots nowadays. Involvement of DLT in
   network of robots brings numerous advantages in car production, which
   will be analyzed in this paper.},
Publisher = {IEEE},
