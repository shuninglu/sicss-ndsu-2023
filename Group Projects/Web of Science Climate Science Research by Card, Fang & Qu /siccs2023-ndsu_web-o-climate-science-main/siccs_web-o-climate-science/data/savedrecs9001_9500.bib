@article{ WOS:000262615700009,
Author = {Marra, M. J. and Crozier, M. and Goff, J.},
Title = {Palaeoenvironment and biogeography of a late MIS 3 fossil beetle fauna
   from South Taranaki, New Zealand},
Journal = {JOURNAL OF QUATERNARY SCIENCE},
