@article{ WOS:000653511600001,
Author = {Mazur, Emily N. and Walker, Shelby E. and Don, Cristen and Conway,
   Flaxen D. L.},
Title = {Designing and Delivering Webinars to Improve Science Communication and
   Engagement between Environmental Researchers and Natural Resource
   Practitioners},
