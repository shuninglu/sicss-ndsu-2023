Title = {Curbing Insecurity in Sub-Saharan Africa Through ICTs for Development
   (ICT4D)},
Booktitle = {PROCEEDINGS OF THE 15TH EUROPEAN CONFERENCE ON EGOVERNMENT},
Year = {2015},
