@inproceedings{ WOS:000309055000018,
Author = {Bohan, Richard P.},
Book-Group-Author = {IEEE},
Title = {EDUCATION AND TRAINING: INCULCATING HUMAN CAPITAL AS A STRATEGIC
   INVESTMENT},
