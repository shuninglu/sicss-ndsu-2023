@article{ WOS:000458170700004,
Author = {Shields, Morgan W. and Johnson, Anne C. and Pandey, Sunita and Cullen,
   Ross and Gonzalez-Chang, M. and Wratten, Steve D. and Gurr, Geoff M.},
Title = {History, current situation and challenges for conservation biological
   control},
