@article{ WOS:A1994TF69200006,
Author = {SCANLAN, JC},
Title = {STATE AND TRANSITION MODELS FOR RANGELANDS .5. THE USE OF STATE AND
   TRANSITION MODELS FOR PREDICTING VEGETATION CHANGE IN RANGELANDS},
Journal = {TROPICAL GRASSLANDS},
Year = {1994},
Volume = {28},
Number = {4},
Pages = {229-240},
