@article{ WOS:000433355900002,
Author = {Birnbaum, David and Gretsinger, Kathryn and Antonio, Marcy G. and
   Loewen, Elizabeth and Lacroix, Paulette},
Title = {Revisiting public health informatics: patient privacy concerns},
