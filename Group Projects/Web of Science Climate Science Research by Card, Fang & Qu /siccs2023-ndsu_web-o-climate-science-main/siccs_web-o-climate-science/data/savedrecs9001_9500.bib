@article{ WOS:000437518400010,
Author = {Holzmann, H.},
Title = {STATUS AND PERSPECTIVES OF HYDROLOGICAL RESEARCH IN SMALL BASINS IN
   EUROPE},
Journal = {CUADERNOS DE INVESTIGACION GEOGRAFICA},
