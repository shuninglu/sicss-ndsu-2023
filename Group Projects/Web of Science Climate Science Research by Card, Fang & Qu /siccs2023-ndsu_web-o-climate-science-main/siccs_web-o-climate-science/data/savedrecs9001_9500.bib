@article{ WOS:000982776900010,
Author = {Diao, Haijun and Yin, Lina and Wang, Lu and Liang, Bin and Chen, Yanyan},
Title = {Sustainable multimedia service cloud platform framework based on
   intelligent management system},
Journal = {SOFT COMPUTING},
Year = {2023},
Month = {2023 MAY 6},
Abstract = {In the past decade, the development of artificial intelligence models in
   various fields has received widespread attention. The development of new
   technologies such as artificial intelligence can help humanity improve
   productivity, address inequality, eradicate poverty, improve water
   resource utilization efficiency, address climate change, and other
   challenges. It can also increase humanity's ability to respond to global
   challenges, which is crucial to building a sustainable future. This
   paper proposes a framework for an intelligent multimedia service cloud
   platform based on Hadoop big data technology to solve the problem that
   various sustainable development service platforms cannot fully cover
   policy, editing, distribution, dissemination analysis, evaluation, and
   other aspects. The platform is mainly divided into two subplatforms:
   multimedia fusion service platform and client management platform. The
   multimedia integrated business platform includes: command center,
   creation center, data center, distribution center, operation center,
   interaction center, member center, evaluation center, statistics center,
   management center, and big data application service center. Through each
   module, its content comprehensively covers policy, editing,
   distribution, communication analysis, evaluation, and other links. At
   the same time, this cloud platform relies on the cloud service
   multiclient operation management model to build a unified management and
   distribution of multiclient basic information, permissions, and various
   multimedia services. Therefore, this study contributes to the efficient
   operation of multimedia business production and operation.},
