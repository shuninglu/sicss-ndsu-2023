@article{ WOS:000810123400010,
Author = {Zhang, Fei and Dellinger, Mathieu and Hilton, Robert G. and Yu, Jimin
   and Allen, Mark B. and Densmore, Alexander L. and Sun, Hui and Jin,
   Zhangdong},
Title = {Hydrological control of river and seawater lithium isotopes},
