@article{ WOS:000522721100007,
Author = {Odidi, Sylvan and Tantanee, Sarintip and Nusit, Korakod and
   Buranajarukorn, Panu},
Title = {FACTORS INFLUENCING THE UPTAKE OF FLOOD MITIGATION MEASURED IN
   BUDALANGI, KENYA},
Journal = {GEOGRAPHIA TECHNICA},
