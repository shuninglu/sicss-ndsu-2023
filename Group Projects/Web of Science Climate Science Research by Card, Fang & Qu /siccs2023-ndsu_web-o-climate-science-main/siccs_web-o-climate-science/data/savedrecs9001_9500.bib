@article{ WOS:000312359500005,
Author = {Wang, Z. and Schellenberg, M. P. and Biligetu, B. and Zhao, M. L. and
   Han, G. D.},
Title = {SHORT COMMUNICATION: Forage mixture responses to water stress in
   semi-arid prairie grassland: a pot experiment},
Journal = {CANADIAN JOURNAL OF PLANT SCIENCE},
Year = {2012},
Volume = {92},
