Author = {Huang, Lei and Yang, Qianqi and Li, Jie and Chen, Jin and He, Ruoying
   and Zhang, Can and Chen, Kai and Dong, Steven Guanpeng and Liu, Yang},
Title = {Risk perception of heat waves and its spatial variation in Nanjing,
   China},
