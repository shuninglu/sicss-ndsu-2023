@article{ WOS:000753879100011,
Author = {Liu, Xilong and Ansari, Nirwan},
Title = {Dual-Battery Enabled Profit Driven User Association in Green
   Heterogeneous Cellular Networks},
Journal = {IEEE TRANSACTIONS ON GREEN COMMUNICATIONS AND NETWORKING},
Year = {2018},
Volume = {2},
Number = {4},
Pages = {1002-1011},
Month = {DEC},
Abstract = {With awareness of the potential harmful effects to the environment and
   climate change, on-grid brown energy consumption of information and
   communications technology (ICT) has drawn much attention. Cellular base
   stations (BSs) are among the major energy guzzlers in ICT, and their
   contributions to the global carbon emissions increase sustainedly. It is
   essential to leverage green energy to power BSs to reduce their on-grid
   brown energy consumption. However, in order to furthest save on-grid
   brown energy and decrease the on-grid brown energy electricity expenses,
   most existing green energy related works only pursue to maximize the
   green energy utilization, while compromising the services received by
   the mobile users. In reality, dissatisfaction of services may eventually
   lead to loss of market shares and profits of the network providers. In
   this paper, we propose a dual-battery enabled profit driven user
   association scheme by jointly considering the traffic delivery latency
   and green energy utilization to maximize the profits for the network
   providers in heterogeneous cellular networks. Since this profit driven
   user association optimization problem is NP-hard, we further propose
   some heuristics to solve the problem with low computational complexity.
   Finally, the performance of the proposed algorithm has been validated
   through extensive simulations.},
Publisher = {IEEE-INST ELECTRICAL ELECTRONICS ENGINEERS INC},
Address = {445 HOES LANE, PISCATAWAY, NJ 08855-4141 USA},
Type = {Article},
Language = {English},
Affiliation = {Liu, XL (Corresponding Author), New Jersey Inst Technol, Helen \& John C Hartmann Dept Elect \& Comp Engn, Adv Networking Lab, Newark, NJ 07102 USA.
   Liu, Xilong; Ansari, Nirwan, New Jersey Inst Technol, Helen \& John C Hartmann Dept Elect \& Comp Engn, Adv Networking Lab, Newark, NJ 07102 USA.},
DOI = {10.1109/TGCN.2018.2869039},
ISSN = {2473-2400},
Keywords = {Green heterogeneous cellular networks; dual-battery; green energy
