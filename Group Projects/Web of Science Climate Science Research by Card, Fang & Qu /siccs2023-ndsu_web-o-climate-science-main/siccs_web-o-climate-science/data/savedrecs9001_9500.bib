@inproceedings{ WOS:000459814200045,
Author = {Baumgaertner, Lars and Penning, Alvar and Lampe, Patrick and
   Richerzhagen, Bjoern and Steinmetz, Ralf and Freisleben, Bernd},
Book-Group-Author = {IEEE},
Title = {Environmental Monitoring Using Low-Cost Hardware and Infrastructureless
   Wireless Communication},
Booktitle = {2018 IEEE GLOBAL HUMANITARIAN TECHNOLOGY CONFERENCE (GHTC)},
