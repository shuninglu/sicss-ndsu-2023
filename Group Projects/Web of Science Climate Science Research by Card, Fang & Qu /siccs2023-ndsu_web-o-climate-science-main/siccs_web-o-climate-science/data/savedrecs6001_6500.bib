@article{ WOS:000487643200001,
Author = {Simpraga, Maja and Ghimire, Rajendra P. and Van Der Straeten, Dominique
   and Blande, James D. and Kasurinen, Anne and Sorvari, Jouni and
   Holopainen, Toini and Adriaenssens, Sandy and Holopainen, Jarmo K. and
