@article{ WOS:000849359800008,
Author = {Xu, Buqing and Zhang, Gan and Gustafsson, Orjan and Kawamura, Kimitaka
   and Li, Jun and Andersson, August and Bikkina, Srinivas and Kunwar,
   Bhagawati and Pokhrel, Ambarish and Zhong, Guangcai and Zhao, Shizhen
   and Li, Jing and Huang, Chen and Cheng, Zhineng and Zhu, Sanyuan and
