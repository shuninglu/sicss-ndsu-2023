@article{ WOS:000568859800001,
Author = {Aswani, R. S.},
Title = {Non-Traditionalmaritime security threats in the Indian Ocean Region:
   Policy alternatives},
Journal = {JOURNAL OF PUBLIC AFFAIRS},
Year = {2022},
