@article{ WOS:000409368500001,
Author = {Farebrother, Elizabeth},
Title = {Review of Making Nature: How We See Animals},
Journal = {PAPERS FROM THE INSTITUTE OF ARCHAEOLOGY},
Year = {2017},
Volume = {27},
Number = {1},
Month = {APR 13},
Abstract = {The intensifying effect of climate change on our global environment has
   - implications for humans and animals alike. Making Nature emphasises
   the symbiosis that has existed between humans and animals for thousands
   of years. Four separate rooms: `Ordering', `Displaying', `Observing' and
   `Making' combine to explore how human decision-making influences our
   classification of animals and our attitudes towards them. Themes of
   behaviour, communication, display and modification are - effectively
   integrated with taxonomic literature, artistic representation and
   scientific debates to demonstrate changing human-animal relationships.},
