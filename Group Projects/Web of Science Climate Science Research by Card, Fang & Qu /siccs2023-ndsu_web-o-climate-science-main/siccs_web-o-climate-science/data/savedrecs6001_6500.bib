@article{ WOS:000993346900001,
Author = {Antonescu, Bogdan and Ene, Dragos and Boldeanu, Mihai and Andrei, Simona
   and Marmureanu, Luminita and Marin, Cristina and Pirloaga, Razvan},
Title = {Future changes in heatwaves characteristics in Romania},
