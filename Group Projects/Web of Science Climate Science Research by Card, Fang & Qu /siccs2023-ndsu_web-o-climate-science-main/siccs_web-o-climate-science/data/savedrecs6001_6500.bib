@article{ WOS:000307462300003,
Author = {Pidgeon, Nick and Corner, Adam and Parkhill, Karen and Spence, Alexa and
   Butler, Catherine and Poortinga, Wouter},
Title = {Exploring early public responses to geoengineering},
