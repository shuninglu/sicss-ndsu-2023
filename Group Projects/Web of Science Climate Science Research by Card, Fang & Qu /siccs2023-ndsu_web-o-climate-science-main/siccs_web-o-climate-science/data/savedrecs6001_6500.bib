@article{ WOS:000756448800001,
Author = {Smiley, Richard W.},
Title = {Root-Lesion Nematodes Affecting Dryland Cereals in the Semiarid Pacific
   Northwest USA},
Journal = {PLANT DISEASE},
Year = {2021},
