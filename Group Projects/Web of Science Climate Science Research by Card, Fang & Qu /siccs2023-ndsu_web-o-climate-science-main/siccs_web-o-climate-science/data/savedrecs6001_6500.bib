@article{ WOS:000238626200052,
Author = {Kanas, N. A. and Salnitskiy, V. P. and Ritsher, J. B. and Gushin, V. I.
   and Weiss, D. S. and Saylor, S. A. and Kozerenko, O. P. and Marmar, C.
   R.},
Title = {Human interactions in space: ISS vs. Shuttle/Mir},
Journal = {ACTA ASTRONAUTICA},
