@article{ WOS:000889215400007,
Author = {Harmackova, V, Zuzana and Pedde, Simona and Bullock, James M. and
   Dellaccio, Ornella and Dicks, Jennifer and Linney, George and Merkle,
   Magnus and Rounsevell, Mark D. A. and Stenning, Jon and Harrison, Paula
