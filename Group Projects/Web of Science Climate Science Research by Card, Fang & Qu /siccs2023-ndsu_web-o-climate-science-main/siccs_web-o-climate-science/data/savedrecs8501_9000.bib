@article{ WOS:000285696000003,
Author = {Herold, N. and Huber, M. and Greenwood, D. R. and Mueller, R. D. and
   Seton, M.},
Title = {Early to Middle Miocene monsoon climate in Australia},
Journal = {GEOLOGY},
Year = {2011},
Volume = {39},
