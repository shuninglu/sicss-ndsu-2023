@article{ WOS:000575637800009,
Author = {Yoo, Cheolhee and Im, Jungho and Park, Sumin and Cho, Dongjin},
Title = {Spatial Downscaling of MODIS Land Surface Temperature: Recent Research
   Trends, Challenges, and Future Directions},
Journal = {KOREAN JOURNAL OF REMOTE SENSING},
