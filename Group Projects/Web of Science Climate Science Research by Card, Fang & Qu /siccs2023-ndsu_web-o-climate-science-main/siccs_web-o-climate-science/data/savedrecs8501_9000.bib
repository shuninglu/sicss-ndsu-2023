Title = {Rethinking Pension Communication - The Role of Metaphors in Information
   Visualization},
Booktitle = {DIGITAL HUMAN MODELING AND APPLICATIONS IN HEALTH, SAFETY, ERGONOMICS
   AND RISK MANAGEMENT: HEALTH, OPERATIONS MANAGEMENT, AND DESIGN, PT II},
