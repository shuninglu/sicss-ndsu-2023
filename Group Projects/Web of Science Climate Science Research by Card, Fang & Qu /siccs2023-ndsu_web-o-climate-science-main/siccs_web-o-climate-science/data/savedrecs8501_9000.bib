@inproceedings{ WOS:000393582600023,
Author = {Suescun, Catalina Aranzazu and Cardei, Mihaela},
Book-Group-Author = {IEEE},
Title = {Event-based Clustering for Composite Event Detection in Wireless Sensors
   Networks},
Booktitle = {2016 IEEE 35TH INTERNATIONAL PERFORMANCE COMPUTING AND COMMUNICATIONS
   CONFERENCE (IPCCC)},
