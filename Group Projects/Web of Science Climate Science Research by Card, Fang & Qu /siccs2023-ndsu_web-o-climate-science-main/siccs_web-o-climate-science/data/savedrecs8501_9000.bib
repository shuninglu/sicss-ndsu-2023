@article{ WOS:000546431900006,
Author = {Calvo, Dafne and Cano-Oron, Lorena and Abengozar, Almudena E.},
Title = {Materials and assessment of literacy level for the recognition of social
   bots in political misinformation contexts},
Journal = {REVISTA ICONO 14-REVISTA CIENTIFICA DE COMUNICACION Y TECNOLOGIAS},
Year = {2020},
Volume = {18},
