@article{ WOS:000285358700012,
Author = {Howe, Bruce M. and Chao, Yi and Arabshahi, Payman and Roy, Sumit and
   McGinnis, Tim and Gray, Andrew},
Title = {A Smart Sensor Web for Ocean Observation: Fixed and Mobile Platforms,
   Integrated Acoustics, Satellites and Predictive Modeling},
