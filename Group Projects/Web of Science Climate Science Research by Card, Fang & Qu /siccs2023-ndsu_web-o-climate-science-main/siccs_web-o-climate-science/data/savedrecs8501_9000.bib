@inproceedings{ WOS:000080781300004,
Author = {Chapman, R and Howden-Chapman, P},
Editor = {Hickey, KL and Kantarelis, D},
Title = {Risk and uncertainty in environmental and environmental health policy},
Booktitle = {OUR NATURAL ENVIRONMENT: AT A CROSSROAD, PROCEEDINGS},
Year = {1997},
Pages = {35-50},
Note = {3rd International Interdisciplinary Conference on the Environment,
   WORCESTER, MA, JUN 25-28, 1997},
Organization = {Interdisciplinary Environm Assoc},
Abstract = {In policy development, uncertainty is often underestimated or
   misunderstood, leading to miscommunication between scientists, policy
   advisers and the public, about the nature of complex environmental and
   environmental health issues. This can lead to incautious risk management
   decisions, with possible adverse consequences for the environment and
   at-risk human populations. A limited set of perspectives and
   considerations is sometimes brought to bear on complex policy issues
   which could profit from wider examination. Together, these factors can
   lead to judgments on environmental and environmental health policy
   matters which underemphasize precaution.
   This paper discusses psychological evidence about how people perceive
   risk and uncertainty, and how risk and uncertainty assessment,
   management and communication are treated in the development of policy.
   Conventional approaches to risk and uncertainty are contrasted with the
   post-normal science approach. We explore the role and desired approach
   of multistakeholder working groups as an organizational form for
   examining evidence and developing advice on difficult risk tradeoffs and
   issues of uncertainty. We also discuss the case for finding ways bq
   which the costs imposed by actions which contribute to risk and
   uncertainty can be laid at the door of those taking the actions.
   We present two policy case studies of risk and uncertainty: climate
   change and BSE/nvCJD (bovine spongiform encephalopathy/new variant
   Creutzfeldt-Jakob Disease).},
Publisher = {INTERDISCIPLINARY ENVIRONMENT ASSOCIATION},
Address = {ASSUMPTION COLLEGE 500 SALISBURY ST PO BOX 15005, WORCESTER, MA
   01615-0005 USA},
Type = {Proceedings Paper},
Language = {English},
ISBN = {0-9654033-2-7},
Keywords-Plus = {BOVINE SPONGIFORM ENCEPHALOPATHY; CREUTZFELDT-JAKOB-DISEASE; SCIENCE;
   LINK},
