Title = {Vulnerability Index Analysis of Bepondi Island as a Reference for Small
   and Outer Islands Management in Indonesian},
Booktitle = {3RD INTERNATIONAL SYMPOSIUM ON MARINE AND FISHERIES RESEARCH (3RD ISMFR)},
Series = {E3S Web of Conferences},
