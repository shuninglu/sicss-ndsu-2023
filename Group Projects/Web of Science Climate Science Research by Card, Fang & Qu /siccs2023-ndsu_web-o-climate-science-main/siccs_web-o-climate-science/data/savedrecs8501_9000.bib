@article{ WOS:000218773900001,
Author = {Chiang, Chia-Chu and Lee, Roger},
Title = {Coordination Languages and Models for Open Distributed Systems},
Journal = {INTERNATIONAL JOURNAL OF SOFTWARE INNOVATION},
