   Whitacre, Jay F.},
Title = {Evaluation of a proposal for reliable low-cost grid power with 100\%
   wind, water, and solar},
Journal = {PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA},
Year = {2017},
