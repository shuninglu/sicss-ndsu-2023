@article{ WOS:000472704400005,
Author = {Kang, Seong-Cheol and Lee, Hoyoung},
Title = {Economic appraisal of implementing electric vehicle taxis in Seoul},
Journal = {RESEARCH IN TRANSPORTATION ECONOMICS},
Year = {2019},
