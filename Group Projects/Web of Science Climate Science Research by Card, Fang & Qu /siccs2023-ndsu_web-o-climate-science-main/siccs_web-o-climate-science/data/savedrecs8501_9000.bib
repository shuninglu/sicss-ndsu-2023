@inproceedings{ WOS:000425842300006,
Author = {Cruz, Marco R. M. and Fitiwi, Desta Z. and Santos, Sergio F. and
   Catalao, Joao P. S.},
Editor = {CamarinhaMatos, LM and ParreiraRocha, M and Ramezani, J},
Title = {Flexibilizing Distribution Network Systems via Dynamic Reconfiguration
   to Support Large-Scale Integration of Variable Energy Sources Using a
   Genetic Algorithm},
Booktitle = {TECHNICAL INNOVATION FOR SMART SYSTEMS (DOCEIS 2017)},
Series = {IFIP Advances in Information and Communication Technology},
Year = {2017},
Volume = {499},
Pages = {72-80},
Note = {8th IFIP WG 5.5/SOCOLNET Advanced Doctoral Conference on Computing,
