@article{ WOS:000409739200002,
Author = {Edvardsson, Malin Picha and Arushanyan, Yevgeniya and Moberg, Asa},
Title = {Local television content production: process structures and climate
   impacts - a case study},
Journal = {JOURNAL OF PRINT AND MEDIA TECHNOLOGY RESEARCH},
