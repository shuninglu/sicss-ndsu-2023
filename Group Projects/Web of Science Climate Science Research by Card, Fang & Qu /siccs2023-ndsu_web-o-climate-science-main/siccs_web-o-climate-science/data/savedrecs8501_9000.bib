@article{ WOS:000397434000001,
Author = {Rabinowitz, Peter M. and Natterson-Horowitz, Barbara J. and Kahn, Laura
   H. and Kock, Richard and Pappaioanou, Marguerite},
Title = {Incorporating one health into medical education},
