@article{ WOS:000538252400004,
Author = {Subhashini, R. and Sethuraman, R.},
Title = {IoT-Based Air Pollution Monitoring Using Silver Birch Trees},
Journal = {NATIONAL ACADEMY SCIENCE LETTERS-INDIA},
