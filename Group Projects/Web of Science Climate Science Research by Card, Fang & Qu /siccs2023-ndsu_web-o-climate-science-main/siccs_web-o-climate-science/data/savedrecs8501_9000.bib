@article{ WOS:000492110300001,
Author = {Mills, Jane and Ingram, Julie and Dibari, Camilla and Merante, Paolo and
   Karaczun, Zbigniew and Molnar, Andras and Sanchez, Berta and Iglesias,
   Ana and Ghaley, Bhim Bahadur},
Title = {Barriers to and opportunities for the uptake of soil carbon management
   practices in European sustainable agricultural production},
