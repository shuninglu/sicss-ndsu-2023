@article{ WOS:000462017000001,
Author = {Chudley, Thomas R. and Willis, Ian C.},
Title = {Glacier surges in the north-west West Kunlun Shan inferred from 1972 to
   2017 Landsat imagery},
Journal = {JOURNAL OF GLACIOLOGY},
