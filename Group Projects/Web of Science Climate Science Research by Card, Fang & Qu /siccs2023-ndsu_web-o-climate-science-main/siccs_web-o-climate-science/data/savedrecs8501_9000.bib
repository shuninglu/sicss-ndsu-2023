Author = {Ustun, Taha Selim and Ozansoy, Cagil R. and Zayegh, Aladin},
Title = {Implementing Vehicle-to-Grid (V2G) Technology With IEC 61850-7-420},
Journal = {IEEE TRANSACTIONS ON SMART GRID},
Year = {2013},
