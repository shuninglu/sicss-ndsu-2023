@inproceedings{ WOS:000766212100036,
Author = {Nucci, Antonio and Hibberd, Matthew},
Editor = {Soares, MM and Rosenzweig, E and Marcus, A},
Title = {Eco-activism, Human-Computer Interaction and Fast Fashion},
Booktitle = {DESIGN, USER EXPERIENCE, AND USABILITY: DESIGN FOR DIVERSITY,
   WELL-BEING, AND SOCIAL DEVELOPMENT, DUXU 2021, PT II},
