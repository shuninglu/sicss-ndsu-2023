   Schandera, Louisa and Kelsey, R. Heath and Porter, Dwayne E. and
   Friedman, Daniela B.},
Title = {``You Gotta Choose Your Words Carefully{''}: Findings from Interviews
   with Environmental Health Scientists about Their Research Translation
   Perceptions and Training Needs},
Journal = {JOURNAL OF HEALTH COMMUNICATION},
Year = {2020},
Volume = {25},
