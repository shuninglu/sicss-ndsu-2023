@article{ WOS:000514802100005,
Author = {Ali, Mohd. Nor Shahizan and Besar, Junaidi Awang and Lyndon, Novel and
   Awang, Abd. Hair and Salim, Neesa Ameera Mohamed},
Title = {Audience interpretation of climate change documentary},
Journal = {INTERNATIONAL JOURNAL OF ADVANCED AND APPLIED SCIENCES},
Year = {2020},
Volume = {7},
Number = {3},
Pages = {37-44},
Month = {MAR},
Abstract = {The world has now come to a time where there is an abundance of
   information being spread every single second. With more information and
   content being created, it leads to more confusion within the community
   as more sources lose credibility for reporting something false. This has
   somehow resulted in instances where facts become less influential than
   opinion. This phenomenon has been coined in as `post-truths in 2016 and
   it seems as though this trend of politics is here to stay. There are
   alarming issues ongoing and one such as climate change is an important
   hurdle that we must collectively work against. This research was
   conducted to discover the interpretations made from the contents of
   Before The Flood movie in 2016-A documentary that advocates for
   environmentalism. This research was developed from the model theory of
   Stuart Hall reception theory, which adopted the content analysis in
   reviewing 70 comments collected from the audience. In general, the
   results have shown that commentators put themselves in certain roles in
   society namely- consumer and normal citizens. When holding on to this
   view, it results in paradoxes of opinions. In conclusion, issues of
   politics and religion affect the audience members engaging in the online
   comment section while under the process of message interpretation. (C)
   2020 The Authors. Published by IASE.},
Publisher = {INST ADVANCED SCIENCE EXTENSION},
Address = {PO BOX 23-31,, TAIPEI, 00000, TAIWAN},
