@article{ WOS:000654378900002,
Author = {Fan, Yuanchao and Tjiputra, Jerry and Muri, Helene and Lombardozzi,
   Danica and Park, Chang-Eui and Wu, Shengjun and Keith, David},
Title = {Solar geoengineering can alleviate climate change pressures on crop
   yields},
Journal = {NATURE FOOD},
Year = {2021},
Volume = {2},
Number = {5},
Pages = {373+},
Month = {MAY},
Abstract = {Solar geoengineering (SG) and CO2 emissions reduction can each alleviate
   anthropogenic climate change, but their impacts on food security are not
   yet fully understood. Using an advanced crop model within an Earth
   system model, we analysed the yield responses of six major crops to
   three SG technologies (SGs) and emissions reduction when they provide
   roughly the same reduction in radiative forcing and assume the same land
   use. We found sharply distinct yield responses to changes in radiation,
   moisture and CO2, but comparable significant cooling benefits for crop
   yields by all four methods. Overall, global yields increase similar to
   10\% under the three SGs and decrease 5\% under emissions reduction, the
   latter primarily due to reduced CO2 fertilization, relative to business
   as usual by the late twenty-first century. Relative humidity dominates
   the hydrological effect on yields of rainfed crops, with little
   contribution from precipitation. The net insolation effect is negligible
   across all SGs, contrary to previous findings.},
