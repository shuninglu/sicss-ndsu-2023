@article{ WOS:000337503800018,
Author = {Zeuss, Dirk and Brandl, Roland and Braendle, Martin and Rahbek, Carsten
   and Brunzel, Stefan},
Title = {Global warming favours light-coloured insects in Europe},
Journal = {NATURE COMMUNICATIONS},
Year = {2014},
Volume = {5},
Month = {MAY},
Abstract = {Associations between biological traits of animals and climate are well
   documented by physiological and local-scale studies. However, whether an
   ecophysiological phenomenon can affect large-scale biogeographical
   patterns of insects is largely unknown. Insects absorb energy from the
   sun to become mobile, and their colouration varies depending on the
   prevailing climate where they live. Here we show, using data of 473
   European butterfly and dragonfly species, that dark-coloured insect
   species are favoured in cooler climates and light-coloured species in
   warmer climates. By comparing distribution maps of dragonflies from 1988
   and 2006, we provide support for a mechanistic link between climate,
   functional traits and species that affects geographical distributions
   even at continental scales. Our results constitute a foundation for
   better forecasting the effect of climate change on many insect groups.},
