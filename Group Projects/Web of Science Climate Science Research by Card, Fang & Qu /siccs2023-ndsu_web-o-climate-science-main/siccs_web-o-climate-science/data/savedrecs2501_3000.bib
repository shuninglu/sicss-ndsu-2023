@article{ WOS:000362717500005,
Author = {Rebich-Hespanha, Stacy and Rice, Ronald E. and Montello, Daniel R. and
   Retzloff, Sean and Tien, Sandrine and Hespanha, Joao P.},
Title = {Image Themes and Frames in US Print News Stories about Climate Change},
Journal = {ENVIRONMENTAL COMMUNICATION-A JOURNAL OF NATURE AND CULTURE},
Year = {2015},
Volume = {9},
Number = {4},
Pages = {491-519},
Month = {OCT 2},
Abstract = {Research on frames in climate change (CC) news coverage has advanced
   substantially over the past decade, but the emerging understanding of
   the framing role of visual imagery that often accompanies news texts
   remains fragmented. We report on a set of image frames identified
   through content analysis of 350 images associated with 200 news articles
   from 11 US newspaper and magazine sources from 1969 through late 2009.
   We reliably identified and quantified the occurrence of 118 image
   themes. We then hierarchically clustered the themes based on their
   co-occurrence in images to identify an integrated framework of 42 image
   frames. We highlight frames associated with particular types of images
   (e.g., photographs and maps) or geographic regions. From among the full
   set of frames, we identify 15 that commonly appear in US CC news imagery
   and discuss the ways in which image frames make salient (or render
   invisible) particular categories of people, geographic regions, aspects
   of science, and spheres of activity.},
