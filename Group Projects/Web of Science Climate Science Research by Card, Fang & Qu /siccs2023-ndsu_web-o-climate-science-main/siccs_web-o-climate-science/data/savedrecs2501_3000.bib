@article{ WOS:000902119600001,
Author = {Abbas, Farhat and O'Neill Rothenberg, Dylan and Zhou, Yiwei and Ke,
   Yanguo and Wang, Hui-Cong},
Title = {Volatile organic compounds as mediators of plant communication and
   adaptation to climate change},
Journal = {PHYSIOLOGIA PLANTARUM},
