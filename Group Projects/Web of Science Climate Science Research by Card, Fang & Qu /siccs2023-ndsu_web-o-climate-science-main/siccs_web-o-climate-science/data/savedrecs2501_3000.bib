@article{ WOS:000670518600001,
Author = {Megarry, William and Hadick, Kacey},
Title = {Lessons from the Edge: Assessing the impact and efficacy of digital
   technologies to stress urgency about climate change and cultural
   heritage globally},
Journal = {HISTORIC ENVIRONMENT-POLICY \& PRACTICE},
