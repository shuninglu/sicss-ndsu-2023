@article{ WOS:000490309900001,
Author = {Prince, Melvin and Forr, James and Wardlaw, Jim and Plummer, Joseph},
Title = {CEO Archetype Identity Drives Organization Culture},
Journal = {JOURNAL OF LEADERSHIP STUDIES},
