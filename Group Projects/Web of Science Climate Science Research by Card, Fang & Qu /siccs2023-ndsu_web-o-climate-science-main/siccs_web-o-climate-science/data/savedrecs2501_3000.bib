@article{ WOS:000539695800001,
Author = {Acosta, Mariola and van Wessel, Margit and van Bommel, Severine and
   Ampaire, Edidah L. and Jassogne, Laurence and Feindt, Peter H.},
Title = {The power of narratives: Explaining inaction on gender mainstreaming in
   Uganda's climate change policy},
