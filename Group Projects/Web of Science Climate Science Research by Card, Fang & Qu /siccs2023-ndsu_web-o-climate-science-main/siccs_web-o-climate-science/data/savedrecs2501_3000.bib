@article{ WOS:000528787700001,
Author = {Wei, Yi-Ming and Han, Rong and Wang, Ce and Yu, Biying and Liang,
   Qiao-Mei and Yuan, Xiao-Chen and Chang, Junjie and Zhao, Qingyu and
   Liao, Hua and Tang, Baojun and Yan, Jinyue and Cheng, Lijing and Yang,
   Zili},
Title = {Self-preservation strategy for approaching global warming targets in the
   post-Paris Agreement era},
Journal = {NATURE COMMUNICATIONS},
Year = {2020},
Volume = {11},
Number = {1},
Month = {APR 14},
Abstract = {A strategy that informs on countries' potential losses due to lack of
   climate action may facilitate global climate governance. Here, we
   quantify a distribution of mitigation effort whereby each country is
   economically better off than under current climate pledges. This
   effort-sharing optimizing approach applied to a 1.5 degrees C and 2
   degrees C global warming threshold suggests self-preservation emissions
   trajectories to inform NDCs enhancement and long-term strategies.
   Results show that following the current emissions reduction efforts, the
   whole world would experience a washout of benefit, amounting to almost
   126.68-616.12 trillion dollars until 2100 compared to 1.5 degrees C or
   well below 2 degrees C commensurate action. If countries are even unable
   to implement their current NDCs, the whole world would lose more
   benefit, almost 149.78-791.98 trillion dollars until 2100. On the
   contrary, all countries will be able to have a significant positive
   cumulative net income before 2100 if they follow the self-preservation
   strategy. The emission allocation strategies of global scenarios do not
   specify the potential benefits from extra climate mitigation efforts.
   Here the authors show that compared to the current Nationally
   Distributed Contributions, the proposed self-preservation strategy might
   generate 126-616 trillion dollars of additional benefits by 2100.},
