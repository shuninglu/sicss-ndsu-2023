Title = {Building Collaborative Partnerships: An Example of a 3rd Mission
   Activity in the Field of Local Climate Change Adaptation},
Booktitle = {UNIVERSITIES AS LIVING LABS FOR SUSTAINABLE DEVELOPMENT: SUPPORTING THE
   IMPLEMENTATION OF THE SUSTAINABLE DEVELOPMENT GOALS},
Series = {World Sustainability Series},
