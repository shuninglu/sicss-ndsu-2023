@article{ WOS:000412094500001,
Author = {Rezaee, Sara and Brooks, Mary R. and Pelot, Ronald},
Title = {Review of fishing safety policies in Canada with respect to extreme
   environmental conditions and climate change effects},
Journal = {WMU JOURNAL OF MARITIME AFFAIRS},
