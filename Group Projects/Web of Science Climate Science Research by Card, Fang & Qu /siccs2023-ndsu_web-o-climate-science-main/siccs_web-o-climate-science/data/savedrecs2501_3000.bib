@article{ WOS:000480252300005,
Author = {Yang, Janet Z. and Chu, Haoran and Kahlor, LeeAnn},
Title = {Fearful Conservatives, Angry Liberals: Information Processing Related to
   the 2016 Presidential Election and Climate Change},
