@inproceedings{ WOS:000454737900087,
Author = {Cahyani, R. and Mardiana, D.},
Book-Group-Author = {IOP},
Title = {Validation and Students Scientific Inquiry-Aided by Multimedia on
   Climate Change},
Booktitle = {2ND INTERNATIONAL CONFERENCE ON STATISTICS, MATHEMATICS, TEACHING, AND
   RESEARCH 2017},
Series = {Journal of Physics Conference Series},
Year = {2018},
Volume = {1028},
Note = {2nd International Conference on Statistics, Mathematics, Teaching, and
   Research (ICSMTR), Makassar, INDONESIA, OCT 09-10, 2017},
Organization = {UNM, Math \& Nat Sci Fac},
Abstract = {The research aims to (1) obtain the validity of scientific inquiry
   instruments on Climate Change material; (2) analyze students' scientific
   inquiry activities based on the different time of problem distribution
   with multimedia; (3) Obtain students' scientific attitude during
   learning. The design was research \&development. The participants were
   two classes of PGMI. Validity obtained from expert judgment, scientific
   inquiry activities obtained from observations during learning. Research
   shows that the validity of scientific inquiry instruments (multimedia,
   worksheets and scientific attitude sheets) are good and appropriate.
   Different time of problem distribution influence the students'
   scientific inquiry activity. Communication is a dominant indicator of
