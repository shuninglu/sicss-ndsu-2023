@article{ WOS:000221878100001,
Author = {Arias, C and Whatley, R},
Title = {Distribution patterns of Early Jurassic Ostracoda and possible
   communication routes through the European epicontinental sea: evidence
   of changes in ocean circulation patterns and its consequence to climate
   change},
Journal = {NEUES JAHRBUCH FUR GEOLOGIE UND PALAONTOLOGIE-ABHANDLUNGEN},
Year = {2004},
Volume = {232},
Number = {1},
Pages = {1-55},
Month = {APR},
Abstract = {Spatial distribution patterns of Early Jurassic Ostracoda are used to
   reconstruct the palaeoceanography of Northwest European Epicontinental
   seas and the adjacent Tethys. Knowledge of these patterns in turn can
   aid in elucidating climate conditions governing the prevailing
   oceanography. Ostracod diversity increased progressively throughout the
   early Jurassic before declining at the Pliensbachian/Toarcian boundary,
   the acme occurring in the Late Pliensbachian. During the Hettangian and
   Early Sinemurian, migration patterns follow a clockwise gyre from the
   British basin through the southern North Sea to Danish basins and thence
   to the southern German basins. The Late Sinemurian saw an important
   change in direction. The entrance of Tethyan ostracods into the
   epicontinental seas is reflected in a northward extension of the faunas,
   following a counter-clockwise gyre from the German to the British
   basins, crossing the southern North Sea. During the Pliensbachian, the
   communication between the Northwest European basins and Tethys is
   stronger, witnessed by the penetration of Tethyan forms through an area
   situated in the South of France and/or Germany. This direction of
   movement continues in the Pliensbachian, extending to the Iberian basins
   and with an open communication between the Paris Basin and the British
   basins. This communication is maintained during the Lower Toarcian, with
   restrictions in the Tenuicostatum and Serpentinus zones which may be
   related to unfavourable conditions for benthos associated with the
   deposition of black shales; the communication is re-established in the
   Bifrons Zone. This pattern suggests a predominant southward direction in
   the area occupied by the French and German basins and follows a
   counter-clockwise gyre. The movement of ostracod faunas during the Lower
   Jurassic is considered in the light of changes in ocean circulation
   patterns which are themselves the result of global climatic changes
   within a warm world driven by monsoonal circulations.},
