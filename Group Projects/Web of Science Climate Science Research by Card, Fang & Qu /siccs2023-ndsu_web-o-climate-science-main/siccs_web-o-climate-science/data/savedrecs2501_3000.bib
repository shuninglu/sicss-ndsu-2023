@article{ WOS:000415274000006,
Author = {Schmidt-Thome, Philipp and Schmidt-Thome, Kaisa},
Title = {Natural hazards and climate change: stakeholder communication and
   decision-making processes An analysis of the outcomes of the 2006 Davos
   Conference on Disaster Reduction},
Journal = {MANAGEMENT OF ENVIRONMENTAL QUALITY},
