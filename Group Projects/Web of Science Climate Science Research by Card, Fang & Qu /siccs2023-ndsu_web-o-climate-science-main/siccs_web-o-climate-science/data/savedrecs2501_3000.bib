@article{ WOS:000497253000055,
Author = {Azadi, Yousof and Yazdanpanah, Masoud and Mahmoudi, Hossein},
Title = {Understanding smallholder farmers' adaptation behaviors through climate
   change beliefs, risk perception, trust, and psychological distance:
   Evidence from wheat growers in Iran},
Journal = {JOURNAL OF ENVIRONMENTAL MANAGEMENT},
Year = {2019},
Volume = {250},
Month = {NOV 15},
Abstract = {Climate change (CC) poses severe threats to the agriculture sector in
   developing countries. Increasing empirical evidence has confirmed that
   adaptation strategies to CC at the farm level can significantly reduce
   the negative impacts of CC and minimize vulnerability. Consequently,
   understanding adaptation to CC has become a major concern for farmers to
   facilitate adaptation actions and assist them in improving their
   adaptive capacity. In this context, this study investigated the most
   prominent psychological drivers of, and impediments to, adaptation
   responses. To this end, 350 farmers from Kermanshah district in western
   of Iran were selected as our sample through a multi-stage, stratified,
   random sampling method. Structural equation modeling found a complex
   relationship between overall CC belief, risk perception, psychological
   distance, trust and risk salience, and farmers' adaptation behaviors.
   Climate risk perception, trust, and psychological distance were much
   more effective in driving farmers' adaptation behaviors. The findings
   yield recommendations for public policy and risk communication that
   could encourage adaptation behaviors among Iranian farmers.},
