@article{ WOS:000905169800004,
Author = {Rosas Rodriguez, Maria Eugenia and Rangel Blanco, Lidia and Del Angel
   Cortes, Lilian},
Title = {The interdisciplinarity of communication in the face of the challenges
   of climate change},
