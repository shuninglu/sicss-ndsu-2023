@inproceedings{ WOS:000228805400013,
Author = {Rochelle-Newall, E and Klein, RJT and Nicholls, RJ and Barrett, K and
   Behrendt, H and Bresser, THM and Cieslak, A and de Bruin, EFLM and
   Edwards, T and Herman, PMJ and Laane, RPWM and Ledoux, L and Lindeboom,
   H and Lise, W and Moncheva, S and Moschella, PS and Stive, MJF and
   Vermaat, JE},
Editor = {Vermaat, JE and Bouwer, L and Turner, K and Salomons, W},
Title = {Group report: Global change and the European coast - climate change and
   economic development},
Booktitle = {MANAGING EUROPEAN COASTS: PAST, PRESENT AND FUTURE},
Series = {Environmental Science},
Year = {2005},
Pages = {239-254},
Note = {ELOISE Workshop on Managing European Coasts, Egmond aan Zee,
   NETHERLANDS, JUN 02-05, 2003},
Organization = {ELOISE},
Abstract = {The effects of climate change on economic development in the coastal
   zone cannot be ignored in future coastal zone management plans. This
   chapter reports the outcome of the group discussion centred round the
   three questions posed by Nicholls and Klein on how the coastal zone can
   be effectively managed in the future. The first question asked how we
   can marry together the human and natural values of a system in the
   upcoming decades. The results of the discussion highlighted the utility
   of using scenarios to obtain good management plans that take into
   account the three provisions of human safety, economic develepment and
   ecological integrity while still achieving a situation of sustainable
   development. The second question addressed the tools required to achieve
   these management goals and discusses the role of public participation
   and media communication. The third question asked what proactive
   strategies can be used to effectivoly manage the coastal zone in the
   21(st) Century and an example of the Humber estuary management plan is
