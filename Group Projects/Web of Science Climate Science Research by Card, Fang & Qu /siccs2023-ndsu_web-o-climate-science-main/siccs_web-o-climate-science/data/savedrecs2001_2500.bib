@inproceedings{ WOS:000279102700020,
Author = {Zaalberg, Ruud and Midden, Cees},
Editor = {Ploug, T and Hasle, P and OinasKukkonen, H},
Title = {Enhancing Human Responses to Climate Change Risks through Simulated
   Flooding Experiences},
Booktitle = {PERSUASIVE TECHNOLOGY, PROCEEDINGS},
Series = {Lecture Notes in Computer Science},
Year = {2010},
Volume = {6137},
Pages = {205-210},
Note = {5th International Conference on Persuasive Technology, Copenhagen,
   DENMARK, JUN 07-10, 2010},
Organization = {Royal Sch Lib \& Informat Sci; Aalborg Univ; Univ Oulu},
Abstract = {Delta areas are threatened by global climate change. The general aims of
   our research were (1) to increase our understanding of climate and flood
   risk perceptions and the factors that influence these judgments, and (2)
   to seek for interventions that can contribute to a realistic assessment
   by laypersons of long-term flooding risks. We argue that awareness of
   one's own vulnerability to future flooding and insights into the
   effectiveness of coping strategies is driven by direct flooding
   experiences. In the current research multimodal sensory stimulation by
   means of interactive 3D technology is used to simulate direct flooding
   experiences at the experiential or sensory level, thereby going beyond
   traditional persuasion attempts using fear-evoking images. Our results
   suggest that future communication efforts should not only use these new
   technologies to transfer knowledge about effective coping strategies and
   flooding risks, but should especially be directed towards residents
   living in flood prone areas, but who lack direct flooding experiences as
   their guiding principle.},
