@incollection{ WOS:000266959700004,
Author = {Ward, Robert E. T.},
Editor = {Liverman, DGE and Pereira, CPG and Marker, B},
Title = {Good and bad practice in the communication of uncertainties associated
   with the relationship between climate change and weather-related natural
   disasters},
Booktitle = {COMMUNICATING ENVIRONMENTAL GEOSCIENCE},
Series = {Geological Society Special Publication},
Year = {2008},
Volume = {305},
Pages = {19-37},
Abstract = {There is evidence that climate change is affecting the frequency,
   intensity and geographical distribution of weather-related natural
   disasters. However, the exact details of this relationship are subject
   to varying degrees of uncertainty. Scientists, policy-makers and others
   have recognized how this connection could influence public support for
   decisions and policies around climate change, such as measures to reduce
   greenhouse gas emissions. This has led to a number of cases in which
   stakeholders in the public policy debate about climate change have
   overstated or understated uncertainties, relative to the scientific
   assessments of the Intergovernmental Panel on Climate Change. Although
   such distortions in the expression of uncertainties are motivated by a
   desire to justify particular policy positions, they could instead
   undermine the credibility of, and trust in, information sources and
   diminish public support.},
