@article{ WOS:000074538700013,
Author = {Sutherst, RW},
Title = {Implications of global change and climate variability for vectorborne
   diseases: generic approaches to impact assessments},
Journal = {INTERNATIONAL JOURNAL FOR PARASITOLOGY},
