@article{ WOS:000989942300003,
Author = {Nicoletta, Gerardo Costabile and Carpentier, Nico},
Title = {Shades of technocratic solutionism: A discursive-material political
   ecology approach to the analysis of the Swedish TV series Hallbart
   naringsliv ('Sustainable business')},
Journal = {EMPEDOCLES-EUROPEAN JOURNAL FOR THE PHILOSOPHY OF COMMUNICATION},
Year = {2022},
Volume = {13},
Number = {2},
Pages = {117-134},
Abstract = {This article analyses the Swedish TV series Hallbart naringsliv (HN) to
   study hegemonic discursive formations over the meaning of the climate
   crisis. Combining new materialist approaches in discourse studies with a
   political ecology understanding of the socio-ecological entanglement, we
   propose the concept of technocratic solutionism to understand how the
   neo-liberal green economy secures instrumentalist discourses on nature
   in the Swedish context. The discourse-theoretical analysis of nine HN
   episodes identifies four nodal points which articulate the technocratic
   solutionist discourse: capital's leading role, Nordic exceptionalism,
   substitutionalism and long-termism. We argue that the climate crisis can
   be understood as a materiality that dislocates capitalist assemblages,
   which then, in response, deploy a techno-solutionism discourse to
   protect the core principles of economic growth and profitability while
   marginalizing potential radical alternatives.},
