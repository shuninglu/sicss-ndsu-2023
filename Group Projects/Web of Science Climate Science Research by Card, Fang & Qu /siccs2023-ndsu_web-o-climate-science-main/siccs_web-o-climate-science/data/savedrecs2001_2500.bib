@incollection{ WOS:000424510800006,
Author = {Grittmann, Elke},
Editor = {Schneider, B and Nocke, T},
Title = {Between Risk, Beauty and the Sublime: The Visualization of Climate
   Change in Media Coverage during COP 15 in Copenhagen 2009},
Booktitle = {IMAGE POLITICS OF CLIMATE CHANGE: VISUALIZATIONS, IMAGINATIONS,
   DOCUMENTATIONS},
Series = {Image-Series},
Year = {2014},
Volume = {55},
Pages = {127-151},
Abstract = {Climate change is one of the top issues in media coverage. Based on
   approaches of iconography, aesthetics and visual framing, this paper
   examines how climate change is framed visually and how media discourse
   on climate change is shaped by powerful visual iconography. Media are
   highly event-driven; thus, the visual coverage on climate change and the
   negotiation during and around the United Nations Climate Change
   Conference in Copenhagen in 2009 (COP 15) offers a good case study how
   the topic is communicated visually. This article focuses on visual
   coverage in German print media, It will be shown that the German case is
   of international interest, Given that media affect public perception of
   climate change issues, the role of visuals in this process will be
   explored. By referring to Entman's framing approach the analysis
   examines which pictures become part of the argumentation in media
   discourse on climate change. The results of the analysis indicate that
   the visuals support the urgency of action claimed by the media. They
   transform the topic into a visual argumentation that focuses on the
   problem, its causes and possible solutions. However, this article argues
   that visuals are not only a matter of knowledge. By adopting the
   aesthetics of beauty and the sublime, the visualization also tries to
   affect the audience emotionally and ethically.},
