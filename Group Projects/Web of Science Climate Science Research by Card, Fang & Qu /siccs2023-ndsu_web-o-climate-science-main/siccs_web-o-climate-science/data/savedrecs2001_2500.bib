@article{ WOS:000312545700011,
Author = {Rohr, Jason R. and Johnson, Philip and Hickey, Christopher W. and Helm,
   Roger C. and Fritz, Alyce and Brasfield, Sandra},
Title = {Implications of global climate change for natural resource damage
   assessment, restoration, and rehabilitation},
Journal = {ENVIRONMENTAL TOXICOLOGY AND CHEMISTRY},
Year = {2013},
