@article{ WOS:000503442800003,
Author = {Abu Qdais, Hani and Abdulla, Fayez and Kurbatova, Anna},
Title = {Wastewater reuse in Jordan and its potential as an adaptation measure to
   climate change},
Journal = {JOURNAL OF ENVIRONMENTAL ENGINEERING AND SCIENCE},
