@article{ WOS:000483202700002,
Author = {Jorgenson, Simon N. and Stephens, Jennie C. and White, Beth},
Title = {Environmental education in transition: A critical review of recent
   research on climate change and energy education},
Journal = {JOURNAL OF ENVIRONMENTAL EDUCATION},
Year = {2019},
Volume = {50},
Number = {3},
Pages = {160-171},
Abstract = {The transition from centralized energy systems based on fossil fuels to
   renewable-based systems is a macro-level societal shift necessitated by
   climate change. This review of recent environmental education (EE)
   research identifies gaps and opportunities for promoting environmental
   action in this new context. We found that environmental educators and
   researchers are currently focused on researching and promoting energy
   conservation behavior with an emphasis on children and youth. We also
   found an emerging research focus on energy transitions at the regional
   and national levels. We recommend that environmental educators and
   researchers adopt a vision and strategy for climate change and energy
   education that more explicitly addresses the role of collective action,
   multiactor networks, and sociotechnical innovation in shaping energy
   transition processes.},
