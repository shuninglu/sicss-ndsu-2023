@article{ WOS:000741682100007,
Author = {Ishtiaque, Asif and Estoque, Ronald C. and Eakin, Hallie and Parajuli,
   Jagadish and Rabby, Yasin Wahid},
Title = {IPCC's current conceptualization of `vulnerability' needs more
   clarification for climate change vulnerability assessments},
Journal = {JOURNAL OF ENVIRONMENTAL MANAGEMENT},
Year = {2022},
Volume = {303},
Month = {FEB 1},
Abstract = {In this Short Communication, we raise the concern that the existing
   conceptualization of `vulnerability', introduced in the IPCC Fifth
   Assessment Report (AR5), is not facilitative for standalone
   vulnerability assessments and that this conceptualization has not been
   well accepted by the vulnerability researchers. We identify three key
   reasons for low adoption of the AR5 conceptualization in climate change
   vulnerability assessments, and urge the IPCC Working Group II to clarify
   how the current conceptualization of `vulnerability' can facilitate
   standalone climate change vulnerability assessments. We propose treating
   `exposure' not only as a precondition for vulnerability but also as a
   secondary driver of vulnerability to capture the influence of
   differential exposure.},
