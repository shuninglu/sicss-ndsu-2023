@article{ WOS:000431796500004,
Author = {Crawford, Pat and Beyea, Wayne and Bode, Claire and Doll, Julie and
   Menon, Rohit},
Title = {Creating climate change adaptation plans for rural coastal communities
   using Deliberation with Analysis as public participation for social
   learning},
Journal = {TOWN PLANNING REVIEW},
