@article{ WOS:000799780700001,
Author = {Schaefer, Svenja and Greber, Hannah and Suelflow, Michael and Lecheler,
   Sophie},
Title = {A Matter of Perspective: An Experimental Study on Potentials of
   Constructive Journalism for Communicating a Crisis},
Journal = {JOURNALISM \& MASS COMMUNICATION QUARTERLY},
Year = {2022},
Month = {2022 MAY 16},
Abstract = {Restorative narratives describe a new form of journalism that attempts
   to overcome the detrimental effects of the more prevalent negative and
   destructive tone of news coverage. This study investigates the
   potentials and risks of restorative narratives in the coverage of crises
   with a 2 (restorative/negative) x 2 (COVID-19/climate crisis)
   experimental online study (n = 829) for emotional, cognitive,
   evaluative, and behavioral outcomes. For both crises, results
   demonstrate that restorative narratives evoked more positive emotional
   reactions to the news, were more likely to be endorsed, and improved
   quality ratings of the news article compared with negative narratives.
   We found no effects for elaboration and information-seeking.},
