@article{ WOS:000356814800028,
Author = {Hart, P. Sol and Nisbet, Erik C. and Myers, Teresa A.},
Title = {Public attention to science and political news and support for climate
   change mitigation},
Journal = {NATURE CLIMATE CHANGE},
