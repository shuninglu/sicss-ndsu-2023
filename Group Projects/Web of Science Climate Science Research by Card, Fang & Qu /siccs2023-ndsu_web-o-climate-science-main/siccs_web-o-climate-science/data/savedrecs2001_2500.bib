@inproceedings{ WOS:000278798400116,
Author = {Kim, Eunsook and Kim, Yong-woon and Kim, Hyoung Jun and Jung, Hae Won},
Book-Group-Author = {IEEE},
Title = {Standardization Activities on ICTs \& Climate Change},
Booktitle = {INTELEC 09 - 31ST INTERNATIONAL TELECOMMUNICATIONS ENERGY CONFERENCE},
Series = {International Telecommunications Energy Conference-INTELEC},
Year = {2009},
Pages = {623-628},
Note = {31st International Telecommunications Energy Conference (INTELEC 09),
   Incheon, SOUTH KOREA, OCT 18-22, 2009},
Abstract = {Information and Communication Technology (ICT) sector was observed to
   emit 2 to 2.5 \% of GHG among the total GHG emission. However, it is
   predicted that ICT sector will be one of the major GHG sources as it
   could be reach to 10 to 15 \% of the total emission by the increased use
   of ICTs on our life and many sectors. Major International standard
