@article{ WOS:000436806200077,
Author = {Zhao, Guangju and Mu, Xingmin and Jiao, Juying and Gao, Peng and Sun,
   Wenyi and Li, Erhui and Wei, Yanhong and Huang, Jiacong},
Title = {Assessing response of sediment load variation to climate change and
   human activities with six different approaches},
