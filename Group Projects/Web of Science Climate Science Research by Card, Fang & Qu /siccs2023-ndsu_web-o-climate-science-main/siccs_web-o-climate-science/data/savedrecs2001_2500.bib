@incollection{ WOS:000788001200019,
Author = {Skanavis, Constantina and Antonopoulos, Kyriakos and Plaka, Valentina
   and Pollaki, Stefania-Pagonitsa and Tsagaki-Rekleitou, Evangelia and
   Koresi, Georgia and Oursouzidou, Charikleia},
