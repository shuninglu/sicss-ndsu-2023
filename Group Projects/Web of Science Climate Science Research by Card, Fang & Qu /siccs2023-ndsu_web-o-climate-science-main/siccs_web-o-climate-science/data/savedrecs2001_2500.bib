Title = {Impact of Projected Emission and Climate Changes on Air Quality in the
   US: from National to State Level},
Booktitle = {14TH INTERNATIONAL CONFERENCE ON MOBILE SYSTEMS AND PERVASIVE COMPUTING
   (MOBISPC 2017) / 12TH INTERNATIONAL CONFERENCE ON FUTURE NETWORKS AND
   COMMUNICATIONS (FNC 2017) / AFFILIATED WORKSHOPS},
Series = {Procedia Computer Science},
Year = {2017},
