@article{ WOS:000908739300001,
Author = {Mullins-Jaime, Charmaine and Wachter, Jan K.},
Title = {Motivating Personal Climate Action through a Safety and Health Risk
   Management Framework},
Journal = {INTERNATIONAL JOURNAL OF ENVIRONMENTAL RESEARCH AND PUBLIC HEALTH},
