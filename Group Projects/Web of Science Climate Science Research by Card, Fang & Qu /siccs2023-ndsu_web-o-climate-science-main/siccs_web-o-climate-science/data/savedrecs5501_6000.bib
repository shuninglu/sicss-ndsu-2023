@article{ WOS:000488774200010,
Author = {Simonsen, Marius Folden and Baccolo, Giovanni and Blunier, Thomas and
   Borunda, Alejandra and Delmonte, Barbara and Frei, Robert and Goldstein,
   Steven and Grinsted, Aslak and Kjaer, Helle Astrid and Sowers, Todd and
   Svensson, Anders and Vinther, Bo and Vladimirova, Diana and Winckler,
   Gisela and Winstrup, Mai and Vallelonga, Paul},
Title = {East Greenland ice core dust record reveals timing of Greenland ice
   sheet advance and retreat},
Journal = {NATURE COMMUNICATIONS},
Year = {2019},
Volume = {10},
Month = {OCT 3},
Abstract = {Accurate estimates of the past extent of the Greenland ice sheet provide
   critical constraints for ice sheet models used to determine Greenland's
   response to climate forcing and contribution to global sea level. Here
   we use a continuous ice core dust record from the Renland ice cap on the
   east coast of Greenland to constrain the timing of changes to the ice
   sheet margin and relative sea level over the last glacial cycle. During
   the Holocene and the previous interglacial period (Eemian) the dust
   record was dominated by coarse particles consistent with rock samples
   from central East Greenland. From the coarse particle concentration
   record we infer the East Greenland ice sheet margin advanced from 113.4
   +/- 0.4 to 111.0 +/- 0.4 ka BP during the glacial onset and retreated
   from 12.1 +/- 0.1 to 9.0 +/- 0.1 ka BP during the last deglaciation.
   These findings constrain the possible response of the Greenland ice
   sheet to climate forcings.},
