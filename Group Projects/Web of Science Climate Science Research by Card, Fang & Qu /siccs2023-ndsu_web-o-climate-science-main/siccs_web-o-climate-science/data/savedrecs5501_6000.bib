@article{ WOS:000700368100086,
Author = {Ruban, Dmitry A. and Yashalova, Natalia N.},
Title = {`Greening' business ethics in gold mining: Evidence from the codes of
   conduct},
Journal = {RESOURCES POLICY},
Year = {2021},
Volume = {74},
Month = {DEC},
Abstract = {Corporate codes of conduct help to communicate various aspects of
   company policy to the staff, as well as to prescribe the desirable
   behavior. Gold-mining activity leaves significant environmental imprint,
   and, thus, pro-environmental business ethics is expected from the
   companies of this industry. Consideration of environment in the codes of
   conduct of 15 world-leading gold-mining companies is examined with two
   approaches, namely general assessment via scoring by three criteria and
   theme analysis. It is established that 93\% of the analyzed codes of
   conduct deal with environment, and 47\% of them demonstrate significant
   degree of `greening'. The codes reflect 30 environment-related topics
   that can be grouped into five principal themes, namely environment as
   higher priority, industrial ecologization, environmental management,
   environmental law and regulation, and climate change. The first four
   themes are found each in the majority of the codes, whereas attention to
   climate change seems to be insufficient. The geological heritage issues
   are missed, although they seem to be linked directly to the corporate
   behavior in the discussed industry. Despite the moderate-to-high degree
   of the business ethics `greening', many codes of conduct of the leading
   gold-mining companies need further improvement.},
