Title = {Development of climate data storage and processing model},
Booktitle = {INTERNATIONAL CONFERENCE AND EARLY CAREER SCIENTISTS SCHOOL ON
   ENVIRONMENTAL OBSERVATIONS, MODELLING AND INFORMATION SYSTEMS
   (ENVIROMIS-2016)},
