@article{ WOS:000211902900006,
Author = {Miller, Brant and Cox, Christopher and Hougham, Justin and Walden, Von
   and Eitel, Karla and Albano, Anthony},
Title = {Adventure learning as a curricular approach that transcends geographies
   and connects people to place},
