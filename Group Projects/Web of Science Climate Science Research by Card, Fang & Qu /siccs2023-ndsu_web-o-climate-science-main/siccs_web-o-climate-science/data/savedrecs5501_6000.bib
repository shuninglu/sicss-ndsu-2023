@article{ WOS:000328559200009,
Author = {Leonard, Michael and Westra, Seth and Phatak, Aloke and Lambert, Martin
   and van den Hurk, Bart and McInnes, Kathleen and Risbey, James and
   Schuster, Sandra and Jakob, Doerte and Stafford-Smith, Mark},
Title = {A compound event framework for understanding extreme impacts},
Journal = {WILEY INTERDISCIPLINARY REVIEWS-CLIMATE CHANGE},
Year = {2014},
Volume = {5},
Number = {1},
Pages = {113-128},
Month = {JAN},
Abstract = {Climate and weather variables such as rainfall, temperature, and
   pressure are indicators for hazards such as tropical cyclones, floods,
   and fires. The impact of these events can be due to a single variable
   being in an extreme state, but more often it is the result of a
   combination of variables not all of which are necessarily extreme. Here,
   the combination of variables or events that lead to an extreme impact is
   referred to as a compound event. Any given compound event will depend
   upon the nature and number of physical variables, the range of spatial
   and temporal scales, the strength of dependence between processes, and
   the perspective of the stakeholder who defines the impact. Modeling
   compound events is a large, complex, and interdisciplinary undertaking.
   To facilitate this task we propose the use of influence diagrams for
   defining, mapping, analyzing, modeling, and communicating the risk of
   the compound event. Ultimately, a greater appreciation of compound
   events will lead to further insight and a changed perspective on how
   impact risks are associated with climate-related hazards. (C) 2013 John
   Wiley \& Sons, Ltd.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
Type = {Review},
Language = {English},
