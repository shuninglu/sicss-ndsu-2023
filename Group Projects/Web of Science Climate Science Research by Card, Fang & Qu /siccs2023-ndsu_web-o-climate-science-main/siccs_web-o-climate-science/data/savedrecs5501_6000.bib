Author = {Ishizawa, Misa and Chan, Douglas and Worthy, Doug and Chan, Elton and
   Vogel, Felix and Maksyutov, Shamil},
Title = {Analysis of atmospheric CH4 in Canadian Arctic and estimation of the
   regional CH4 fluxes},
