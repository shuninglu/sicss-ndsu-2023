@article{ WOS:000409968200002,
Author = {McFadzean, Andrew James},
Title = {Organisational climate change is here: memory curators in the digital
   age},
Journal = {VINE JOURNAL OF INFORMATION AND KNOWLEDGE MANAGEMENT SYSTEMS},
