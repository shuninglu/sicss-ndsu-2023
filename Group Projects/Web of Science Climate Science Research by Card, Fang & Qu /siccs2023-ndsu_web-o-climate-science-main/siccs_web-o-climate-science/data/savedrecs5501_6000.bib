@article{ WOS:000211132600009,
Author = {Stebbing, M. and Carey, M. and Sinclair, M. and Sim, M.},
Title = {Understanding the vulnerability, resilience and adaptive capacity of
   households in rural Victorian towns in the context of long-term water
   insecurity},
