@article{ WOS:000548037600001,
Author = {Harcourt, Rachel and de Bruin, Wandi Bruine and Dessai, Suraje and
   Taylor, Andrea},
Title = {What Adaptation Stories are UK Newspapers Telling? A Narrative Analysis},
Journal = {ENVIRONMENTAL COMMUNICATION-A JOURNAL OF NATURE AND CULTURE},
Year = {2020},
Volume = {14},
Number = {8},
Pages = {1061-1078},
Month = {NOV 16},
Abstract = {Media coverage of climate change impacts and adaptation will likely
   influence how citizens think society can and should adapt. Here, we
   undertook the first analysis of UK newspaper adaptation coverage. We
   identified five prominent adaptation narratives: (1) the government
   should build more flood defences, (2) home owners should buy flood
   insurance, (3) individuals should become more informed, (4) the farming
   industry should innovate, (5) and the natural environment should fight
   for its survival. We find that only some of the more immediate climate
   change impacts likely to affect the UK are presented as necessitating a
   response. The government is considered primarily responsible while UK
   citizens are given few and narrow responsibilities. The range of
   adaptive actions under consideration is limited and unchallenging to the
   status quo. In summary, newspaper coverage presents a restricted view as
