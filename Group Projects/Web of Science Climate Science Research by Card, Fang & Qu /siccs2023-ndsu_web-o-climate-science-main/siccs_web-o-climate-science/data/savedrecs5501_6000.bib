Title = {Impact of Organizational Justice on Knowledge Employees' Creativity:
   Mediated by Organizational Commitment},
Booktitle = {PROCEEDINGS OF THE SIXTEENTH INTERNATIONAL CONFERENCE ON MANAGEMENT
   SCIENCE AND ENGINEERING MANAGEMENT, VOL 2},
