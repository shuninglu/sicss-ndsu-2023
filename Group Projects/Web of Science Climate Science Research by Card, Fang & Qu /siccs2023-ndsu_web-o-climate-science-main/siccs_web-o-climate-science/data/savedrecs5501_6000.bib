@article{ WOS:000287495000003,
Author = {Salvador, Michael and Norton, Todd},
Title = {The Flood Myth in the Age of Global Climate Change},
Journal = {ENVIRONMENTAL COMMUNICATION-A JOURNAL OF NATURE AND CULTURE},
Year = {2011},
Volume = {5},
Number = {1},
Pages = {45-61},
Abstract = {When released, The Day After Tomorrow was widely described by critics
   and the movie's creators as a pro-environmentalist film. This essay
   argues that The Day After Tomorrow articulates a variation of
   apocalyptic discourse identified as a flood myth. The authors conclude
   that this version of the flood myth largely undermines contemporary
   environmental discourse that attempts to generate public activism in
   addressing ecological problems, by replacing an emphasis on human
   efficacy with symbolic vindication and exchanging collective effort for
   individual survivalism. The film thus serves as a cautionary tale about
   the potential consequences of contemporary mythic discourse presented as
   supporting environmental activism.},
