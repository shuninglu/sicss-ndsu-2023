@article{ WOS:000695133200001,
Author = {Klocker, Natascha and Gillon, Charles and Gibbs, Leah and Atchison,
   Jennifer and Waitt, Gordon},
Title = {Hope and grief in the human geography classroom},
Journal = {JOURNAL OF GEOGRAPHY IN HIGHER EDUCATION},
Year = {2021},
Month = {2021 SEP 14},
Abstract = {Human geographers engage students in learning about a world
   characterized by environmental and social disarray. It follows that our
   students are exposed to deeply confronting topics: climate change,
   global inequality, food insecurity, and racism, to name a few. Prompted
   by scholarly debate on the effects of painful emotions elicited by
   public climate change communication, we asked human geography students
   at the University of Wollongong about their experiences of our teaching.
   Via focus group discussions, we invited them to consider which emotions
   ``belong{''} in our classrooms and how difficult emotions might be
   managed to minimize harm. The students explained that they expected to
   feel distress as part-and-parcel of their chosen area of study and found
   being confronted a productive experience. With regard to pedagogical
   practice, they considered the responsibility for managing difficult
   course-related emotions to be distributed, and identified a range of
   strategies to prevent painful emotions from corroding their wellbeing.
   Some of these strategies, particularly making room for difficult
   emotions - by acknowledging, holding onto, and working through them in
   our classrooms - will be challenging as more universities turn to online
   teaching.},
