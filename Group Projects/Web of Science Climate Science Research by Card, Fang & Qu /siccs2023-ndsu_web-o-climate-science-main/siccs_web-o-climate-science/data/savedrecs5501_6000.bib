@article{ WOS:000432607000006,
Author = {Fdez-Arroyabe, Pablo and Lecha Estela, Luis and Schimt, Falko},
Title = {Digital divide, biometeorological data infrastructures and human
   vulnerability definition},
Journal = {INTERNATIONAL JOURNAL OF BIOMETEOROLOGY},
Year = {2018},
Volume = {62},
Number = {5, SI},
