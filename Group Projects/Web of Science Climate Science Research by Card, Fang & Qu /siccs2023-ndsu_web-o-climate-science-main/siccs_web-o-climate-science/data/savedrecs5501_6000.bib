Author = {Pahwa, J. S. and Brewer, R. and Sutton, T. and Yesson, C. and Burgess,
   M. and Xu, X. and Jones, A. C. and White, R. J. and Gray, W. A. and
   Fiddian, N. J. and Bisby, F. A. and Culham, A. and Caithness, N. and
   Scoble, M. and Williams, P. and Bhagwat, S.},
Editor = {Turner, SJ and Lee, BS and Cai, W},
Title = {Biodiversity World: A problem-solving environment for analysing
   biodiversity patterns},
Booktitle = {SIXTH IEEE INTERNATIONAL SYMPOSIUM ON CLUSTER COMPUTING AND THE GRID:
   SPANNING THE WORLD AND BEYOND},
Year = {2006},
