@article{ WOS:000960899600001,
Author = {Nishio, Akihiro and Shibuya, Fumiko and de los Reyes, Calvin S. and
   Estrada, Crystal Amiel M. and Gregorio Jr, Ernesto R. R. and Sari, Dian
   Puspita and Warnaini, Cut and Kadriyan, Hamsu and Cruz, Maria Sandra M.
