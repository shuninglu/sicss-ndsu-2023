@article{ WOS:000422650500003,
Author = {Hare, Vincent J. and Loftus, Emma and Jeffrey, Amy and Ramsey,
   Christopher Bronk},
Title = {Atmospheric CO2 effect on stable carbon isotope composition of
   terrestrial fossil archives},
