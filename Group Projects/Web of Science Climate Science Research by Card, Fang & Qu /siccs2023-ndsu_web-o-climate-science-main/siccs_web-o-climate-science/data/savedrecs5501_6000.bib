@article{ WOS:000380951200003,
Author = {Maljean-Dubois, Sandrine},
Title = {The Paris Agreement: A New Step in the Gradual Evolution of Differential
   Treatment in the Climate Regime?},
Journal = {REVIEW OF EUROPEAN COMPARATIVE \& INTERNATIONAL ENVIRONMENTAL LAW},
