@article{ WOS:000361164100022,
Author = {Dong, Xinling and Wang, Tao and Wang, Hongda},
Title = {APPLICATION OF DATA ENVELOPMENT ANALYSIS TO THE ENVIRONMENTAL
   PERFORMANCE EVALUATION AND THE EMPIRICAL DISCUSSION IN HI-TECH INDUSTRY},
