@article{ WOS:000419738000012,
Author = {Mainsah, Henry and Morrison, Andrew and Edeholt, Hakan},
Title = {Building networks for knowledge exchange, and design strategies for
   climate futures.},
Journal = {DESIGN JOURNAL},
Year = {2017},
Volume = {20},
Number = {1},
