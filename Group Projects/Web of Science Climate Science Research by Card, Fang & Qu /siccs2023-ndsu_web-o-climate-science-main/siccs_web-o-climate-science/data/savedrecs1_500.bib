@article{ WOS:000259854100013,
Author = {Kelly, Tim and Adolph, Martin},
Title = {ITU-T initiatives on climate change},
Journal = {IEEE COMMUNICATIONS MAGAZINE},
Year = {2008},
Volume = {46},
Number = {10},
Pages = {108-114},
Month = {OCT},
Abstract = {It is estimated that ICTs contribute around 2 percent of global
   greenhouse gas emissions. These percentages are likely to grow as ICTs
   become more widely available. At the same time, ICTs can be a major
   linchpin in efforts to combat climate change and serve as a potent
   cross-cutting tool to limit and ultimately reduce GHG emissions across
   economic and social sectors. This article looks at the potential role
   ICTs play at different stages, from contributing to global warming to
   monitoring it, to developing long-term solutions to mitigate its
   effects, both directly in the ICT sector and in other sectors such as
   energy, transport, buildings, and finally to helping adapt to its
   effects. The article addresses the efforts made by the ITU and its
   membership to develop new climate-friendly technologies.},
