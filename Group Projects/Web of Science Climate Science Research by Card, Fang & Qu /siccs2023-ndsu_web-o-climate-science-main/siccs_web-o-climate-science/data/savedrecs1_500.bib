@article{ WOS:000414226500006,
Author = {Wozniak, Antal and Wessler, Hartmut and Lueck, Julia},
Title = {Who Prevails in the Visual Framing Contest about the United Nations
   Climate Change Conferences?},
Journal = {JOURNALISM STUDIES},
Year = {2017},
