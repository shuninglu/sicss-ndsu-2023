@article{ WOS:000574233000005,
Author = {Das, Abhinaba},
Title = {Combatting Climate Change Denial},
Journal = {RESONANCE-JOURNAL OF SCIENCE EDUCATION},
Year = {2020},
Volume = {25},
Number = {7},
Pages = {933-945},
Month = {JUL},
Abstract = {Despite the scientific consensus on the issue of climate change, we are
   yet to marshal a social consensus. Sorting this apparent dissonance is a
   challenge. The topic of climate change has become entrapped in so-called
   economic and socio-political wars. Scientific miscommunication has
   thwarted efforts to effectively engage with the public on this issue. If
   we can actively seek out the reasons that motivate denial, and try to
   understand the underlying structure that engenders misinformation, then
   perhaps we can find ways to address them. There is an acute urgency to
   better strategize climate change communication. The scientific community
   should recognize the need for new pathways of effective communication.
   In summary, we need to adapt. The faster we affect a change, the better
   chances we have at avoiding a catastrophe. Time is of the essence!},
