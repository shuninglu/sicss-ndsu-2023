@article{ WOS:000270095100007,
Author = {Gavin, Neil T.},
Title = {Addressing climate change: a media perspective},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2009},
Volume = {18},
Number = {5},
Pages = {765-780},
Abstract = {Analysing media coverage can shed light on the politics of climate
   policy. Conventional notions of how the public sphere represents (or
   fails to represent) significant social and political issues to citizens
   are examined. The prominence of global warming stories in the British
   press is a principal focus. The contours of climate change discourse and
   the coverage of emissions trading are also touched upon, alongside the
   political implications that follow. It is then argued that the coverage
   contributes to a public sphere that is truly international in character
   but only in a modest way, making political intervention that much more
   difficult. Climate change policy must take this international public
   sphere into account if we are to understand how the mitigation of global
   warming is to be approached and encouraged ( and with what limitations).},
