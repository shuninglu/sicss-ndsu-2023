@article{ WOS:000209887000002,
Author = {Kennel, Charles F.},
Title = {``Hiatus{''} in Global Warming: Paradox and Complexity in Climate
   Science},
Journal = {PROCEEDINGS OF THE AMERICAN PHILOSOPHICAL SOCIETY},
Year = {2015},
Volume = {159},
