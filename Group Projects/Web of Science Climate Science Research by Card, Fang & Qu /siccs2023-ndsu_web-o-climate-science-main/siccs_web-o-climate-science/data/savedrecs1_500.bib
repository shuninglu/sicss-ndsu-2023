@article{ WOS:000843135900001,
Author = {Reichel, Christian and Plueschke-Altof, Bianka and Plaan, Joonas},
Title = {Speaking of a `climate crisis': Fridays for Future's attempts to reframe
   climate change},
Journal = {INNOVATION-THE EUROPEAN JOURNAL OF SOCIAL SCIENCE RESEARCH},
Year = {2022},
Volume = {35},
Number = {3, SI},
Pages = {370-388},
Month = {JUL 3},
Abstract = {The paper explores how the Fridays for Future movement (FfF) attempts to
   reframe climate change as a climate crisis. Adding to the research on
   the role of climate change communication for inducing behavioural
   change, we analyse the way in which this reframing is conducted and
   locally negotiated by FfF groups in Germany and Estonia. The paper thus
   focuses on the question of how FfF's global blueprint for a climate
   strike, which builds on the notion of crisis, is transferred into
   different societal contexts. Aided by multisite ethnographies,
   similarities and differences in reframing and protest strategies and the
   negotiation of the local reception of this message are elaborated by
   referencing the example of FfF groups in Berlin and Frankfurt (Germany),
   as well as Tallinn, Tartu and Parnu (Estonia).},
