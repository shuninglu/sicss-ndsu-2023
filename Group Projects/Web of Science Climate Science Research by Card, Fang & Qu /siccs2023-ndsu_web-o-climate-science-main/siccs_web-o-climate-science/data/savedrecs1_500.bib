@article{ WOS:000345830500007,
Author = {Schuldt, Jonathon P. and Roh, Sungjong},
Title = {Media Frames and Cognitive Accessibility: What Do ``Global Warming{''}
   and ``Climate Change{''} Evoke in Partisan Minds?},
Journal = {ENVIRONMENTAL COMMUNICATION-A JOURNAL OF NATURE AND CULTURE},
Year = {2014},
Volume = {8},
Number = {4},
Pages = {529-548},
Month = {OCT 2},
Abstract = {Decades of research demonstrate that how the public thinks about a given
   issue is affected by how it is framed by the media. Typically, studies
   of framing vary how an issue is portrayed (often, by altering the text
   of written communication) and compare subsequent beliefs, attitudes, or
   preferences-taking a framing effect as evidence that a media frame (or
   frame in communication) instantiated a particular audience frame (or
   frame in thought). Less work, however, has attempted to measure frames
   in thought directly, which may illuminate cognitive mechanisms that
   underlie framing effects. In this vein, we describe a Web experiment (n
   = 400) in which US political partisans reported the extent to which a
   ``global warming{''} or ``climate change{''} frame brought to mind
   various climate-related concepts. Although the media frequently employ
   them interchangeably, these frames evoked distinct patterns of cognitive
   accessibility across partisans: Whereas conservatives associated
   heat-related impacts (rising temperatures, melting ice) more strongly
   with ``global warming{''} than with ``climate change,{''} liberals
   associated these impacts equally with both phrases. Discussion focuses
   on implications for media framing of climate issues and framing theory
   more broadly.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
