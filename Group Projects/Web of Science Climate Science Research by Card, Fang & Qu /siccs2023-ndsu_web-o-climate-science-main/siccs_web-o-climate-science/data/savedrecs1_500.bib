@article{ WOS:000747986800006,
Author = {Gole, Iulian and Popescu, Maria Loredana and Diaconu, Amelia and Gombos,
   Svetlana Platagea},
Title = {Similitudes and differences between global warming and COVID-19 crises -
   Lessons to be learned},
Journal = {PROCEEDINGS OF THE INTERNATIONAL CONFERENCE ON BUSINESS EXCELLENCE},
Year = {2021},
Volume = {15},
Number = {1},
Pages = {434-443},
