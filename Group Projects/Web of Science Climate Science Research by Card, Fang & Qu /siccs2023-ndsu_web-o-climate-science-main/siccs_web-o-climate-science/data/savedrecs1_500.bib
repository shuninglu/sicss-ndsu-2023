@article{ WOS:000219209700001,
Author = {Stoutenborough, James W. and Fette, Robert Nicholas and Vedlitz, Arnold
   and Goldsmith, Carol L.},
Title = {Understanding the Communication of Climate Change Risk: Climate
   Scientists' Perspectives of Media Sources and Policy Makers},
Journal = {RISK HAZARDS \& CRISIS IN PUBLIC POLICY},
Year = {2014},
Volume = {5},
