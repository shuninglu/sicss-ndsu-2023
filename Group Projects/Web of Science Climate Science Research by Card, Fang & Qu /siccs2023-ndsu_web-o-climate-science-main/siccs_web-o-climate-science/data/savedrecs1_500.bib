@inproceedings{ WOS:000503762800029,
Author = {Moletsane, Ramadile},
Editor = {Silhavy, R},
Title = {Awareness of Information and Communication Technology Induced Climate
   Change and the Developing Countries},
Booktitle = {ARTIFICIAL INTELLIGENCE METHODS IN INTELLIGENT ALGORITHMS},
