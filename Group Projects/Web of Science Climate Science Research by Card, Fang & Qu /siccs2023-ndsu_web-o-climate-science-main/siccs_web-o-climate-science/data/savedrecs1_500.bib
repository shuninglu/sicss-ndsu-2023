@article{ WOS:000455197200001,
Author = {Kause, Astrid and Townsend, Tarlise and Gaissmaier, Wolfgang},
Title = {Framing Climate Uncertainty: Frame Choices Reveal and Influence Climate
   Change Beliefs},
