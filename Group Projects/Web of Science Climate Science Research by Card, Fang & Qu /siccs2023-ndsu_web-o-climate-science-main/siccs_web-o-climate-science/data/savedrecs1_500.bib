@inproceedings{ WOS:000174317100003,
Author = {Hart, MB and Hart, AB},
Editor = {Williams, CL},
Title = {Global climate change: A geological perspective},
Booktitle = {GEOSCIENCE IN SOUTH-WEST ENGLAND, VOL 10, PT 1 2000},
