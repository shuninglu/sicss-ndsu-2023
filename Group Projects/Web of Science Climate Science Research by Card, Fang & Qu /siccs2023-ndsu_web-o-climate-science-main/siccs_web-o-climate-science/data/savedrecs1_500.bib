@article{ WOS:000250508100008,
Author = {Hertzler, Greg},
Title = {Adapting to climate change and managing climate risks by using real
   options},
Journal = {AUSTRALIAN JOURNAL OF AGRICULTURAL RESEARCH},
Year = {2007},
