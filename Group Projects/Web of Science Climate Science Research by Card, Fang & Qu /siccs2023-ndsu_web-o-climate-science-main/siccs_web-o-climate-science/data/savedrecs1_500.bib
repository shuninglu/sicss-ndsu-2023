@article{ WOS:000389613600005,
Author = {Roper, Juliet and Ganesh, Shiv and Zorn, Theodore E.},
Title = {Doubt, Delay, and Discourse: Skeptics' Strategies to Politicize Climate
   Change},
Journal = {SCIENCE COMMUNICATION},
Year = {2016},
Volume = {38},
Number = {6},
Pages = {776-799},
Month = {DEC},
Abstract = {This article proposes a theoretical framework to explain climate
   skeptics' persuasive discursive strategies against anthropogenic global
   warming. By combining Bourdieu's notions of political and social fields
   with discursive articulation the framework explains skeptics' strategies
   to politicise climate change, generate debate, and delay political
   action. To illustrate the framework, we analyze publications of the New
   Zealand Climate Science Coalition, an internationally influential
   organization with strong links to U.S. conservative think tanks.
   Findings demonstrate strategies for polarization along lines of Left
   versus Right ideologies by linking climate change issues to prominent
   social discourses, thus garnering public support and delaying policy
   action.},
