@article{ WOS:000688467400007,
Author = {Maehle, Natalia and Otte, Pia Piroschka and Huijben, Boukje and de
   Vries, Jorick},
Title = {Crowdfunding for climate change: Exploring the use of climate frames by
   environmental entrepreneurs},
Journal = {JOURNAL OF CLEANER PRODUCTION},
Year = {2021},
Volume = {314},
Month = {SEP 10},
Abstract = {This study investigates which climate change frames environmental
   entrepreneurs can employ in their project descriptions while seeking
   crowdfunding on online platforms. An explorative analysis of 58 climate
   change mitigation projects was conducted in four countries with
   different degrees of maturity of crowdfunding market. The following
   climate change frames prevail, and appear particularly in the
   descriptions of the projects with successful campaigns: promotion goal
   frame, humans-related impact frame, positive valence frame, and near
   future and now time frame. Many projects with successful crowdfunding
   campaigns also mention their location. This study contributes to the
   sustainable crowdfunding literature by addressing an underexplored topic
   of framing and following a qualitative in-depth approach. Moreover, it
   can help environmental entrepreneurs understand the landscape of framing
   opportunities and therefore make a more informed choice of what kind of
   frames to employ in their project descriptions.},
