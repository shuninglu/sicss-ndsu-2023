@inproceedings{ WOS:000481988400025,
Author = {Rosana Romero, Olga and Olivar, Gerard and Bianchi, Carmine},
Editor = {FigueroaGarcia, JC and LopezSantana, ER and RodriguezMolano, JI},
Title = {Dynamic Performance of the Agricultural Sector Under Conditions of
   Climate Change and Armed Post-conflict},
Booktitle = {APPLIED COMPUTER SCIENCES IN ENGINEERING, WEA 2018, PT I},
