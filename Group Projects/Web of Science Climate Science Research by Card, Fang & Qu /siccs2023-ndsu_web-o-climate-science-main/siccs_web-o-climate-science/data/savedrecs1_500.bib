@article{ WOS:000675454800010,
Author = {Magioglou, Thalia and Coen, Sharon},
Title = {The Construction of a Hegemonic Social Representation Climate Crisis and
   the Role of COVID-19 in Defining Survival},
Journal = {EUROPEAN PSYCHOLOGIST},
