@article{ WOS:000570626500001,
Author = {Arikan, Gizem and Gunay, Defne},
Title = {Public attitudes towards climate change: A cross-country analysis},
Journal = {BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS},
Year = {2021},
Volume = {23},
Number = {1},
Pages = {158-174},
Month = {FEB},
Abstract = {Addressing climate change requires international effort from both
   governments and the public. Climate change concern is a crucial variable
   influencing public support for measures to address climate change.
   Combining country-level data with data from the Pew Research Center
   Spring 2015 Global Attitudes Survey, we test whether perceived threats
   from climate change influence climate change concern. We distinguish
   between personal threat and planetary threat and we find that both
   threats have substantive effects on climate change concern, with
   personal threat exerting a greater influence on climate change concern
   than planetary threat. The effects of both types of threats are also
   moderated by Gross Domestic Product per capita, such that threats have
   stronger effects on climate change concern in high-income countries than
   in low-income countries. Our findings contribute to the existing
   literature and open up new debates concerning the role of threats in
   climate change concern and have implications for climate change
   communication.},
