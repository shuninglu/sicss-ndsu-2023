@article{ WOS:000810945500006,
Author = {Endreson, Thorunn Gullaksen},
Title = {Climate Change and the Carnivalesque in Erlend O. Nodtvedt's Vestlandet},
Journal = {NORDEUROPA FORUM-ZEITSCHRIFT FUR KULTURSTUDIEN},
Year = {2022},
Number = {SI},
Pages = {109-128},
Abstract = {Norwegian contemporary climate fiction often portrays humans as in
   denial of climate change) In Erlend Nodtvedes transgressive novel
   Vestlandet (2017), an alternative story is presented. In contrast to
   conventional climate change denial, the two protagonists turn the
   situation upside down and literally celebrate death and climate change
   and the exceptional Western Norwegian ``sublime{''} landscape. Drawing
   on Mikhail Bakhtin's concept of laughter and the carnivalesque, I will
   investigate Vestlandet as a response to a world in climate crisis, and
   thereby transcending the ecocritical reluctance to engage with laughter.
   Environmental humor offers an alternative vision, a possibility to see
   ourselves from another perspective. Vestlandetcontributes to the
   creation of new climate change narratives and communicates potential for
   change, but there is no guarantee that laughter will lead to increased
   awareness and action.},
