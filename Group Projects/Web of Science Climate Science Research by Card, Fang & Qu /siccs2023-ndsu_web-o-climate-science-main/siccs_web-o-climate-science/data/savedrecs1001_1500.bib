@article{ WOS:000453622500003,
Author = {Chan, Eugene Y.},
Title = {Climate change is the world's greatest threat - In Celsius or
   Fahrenheit?},
Journal = {JOURNAL OF ENVIRONMENTAL PSYCHOLOGY},
Year = {2018},
Volume = {60},
Pages = {21-26},
Month = {DEC},
Abstract = {In two experiments, participants who were told that the Earth's average
   temperature was -24 degrees C thought that it was more important to
   limit climate change than those who were told that it was-16 degrees C.
   However, participants who were told that the average temperature was -11
   degrees F thought it was less important to reduce the carbon footprint
   than those who were told that it was 3 degrees F. The findings
   contradict each other since -24 degrees C is the same as -11 degrees F,
   and -16 degrees C is the same as 3 degrees F. We draw on research on
   numerosity and goal-pursuit from behavioral psychology to explain the
   intriguingly-opposite findings. We measure both the perceived influence
   of and actual behavior to help fight climate change. Thus, we offer the
   novel hypothesis that presenting climate change figures in Celsius or
   Fahrenheit two primary units to communicate temperature-can influence
   people's belief in or concern regarding climate change.},
