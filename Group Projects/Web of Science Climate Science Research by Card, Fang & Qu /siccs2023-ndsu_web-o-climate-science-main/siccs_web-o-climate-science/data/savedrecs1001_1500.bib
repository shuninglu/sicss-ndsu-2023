@article{ WOS:000697575000001,
Author = {Foong, Lai Heng and Huntley, Rebecca},
Title = {Communicating about climate change - Who is listening, who isn't and
   why: Implications for medical professionals},
Journal = {JOURNAL OF PAEDIATRICS AND CHILD HEALTH},
Year = {2021},
Volume = {57},
Number = {11},
Pages = {1826-1829},
Month = {NOV},
Abstract = {Community concern about climate change in the community in general is
   strong and getting stronger but greater action on climate change is
   contingent upon whether the public sees strong links between climate
   effects and personal and community wellbeing in the present as well as
   the future. Medical and health-care professions can be effective
   messengers about the impacts of climate given their trusted status and
   ability to draw connections between climate change and physical and
   mental health.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
