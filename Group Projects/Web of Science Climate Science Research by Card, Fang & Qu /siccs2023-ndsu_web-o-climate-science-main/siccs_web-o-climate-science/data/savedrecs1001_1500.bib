@article{ WOS:000386420900015,
Author = {Boullis, Antoine and Detrain, Claire and Francis, Frederic and
   Verheggen, Francois J.},
Title = {Will climate change affect insect pheromonal communication?},
Journal = {CURRENT OPINION IN INSECT SCIENCE},
Year = {2016},
Volume = {17},
Pages = {87-91},
Month = {OCT},
Abstract = {Understanding how climate change will affect species interactions is a
   challenge for all branches of ecology. We have only limited
   understanding of how increasing temperature and atmospheric CO2 and O-3
   levels will affect pheromone mediated communication among insects. Based
   on the existing literature, we suggest that the entire process of
   pheromonal communication, from production to behavioural response, is
   likely to be impacted by increases in temperature and modifications to
   atmospheric CO2 and O-3 levels. We argue that insect species relying on
   long-range chemical signals will be most impacted, because these signals
   will likely suffer from longer exposure to oxidative gases during
   dispersal. We provide future directions for research programmes
   investigating the consequences of climate change on insect pheromonal
   communication.},
