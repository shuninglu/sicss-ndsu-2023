@article{ WOS:000492386000006,
Author = {Labosier, Christopher F. and Fay, Isabel},
Title = {Integrating Science and Rhetoric on Climate Change in the Classroom},
Journal = {BULLETIN OF THE AMERICAN METEOROLOGICAL SOCIETY},
