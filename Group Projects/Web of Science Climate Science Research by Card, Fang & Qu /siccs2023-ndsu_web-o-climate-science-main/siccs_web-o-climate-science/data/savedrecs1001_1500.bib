Title = {A Statistical Bias Correction Tool for Generating Climate Change
   Scenarios in Indonesia based on CMIP5 Datasets},
Booktitle = {3RD INTERNATIONAL SEMINAR ON SCIENCES SCIENCES ON PRECISION AND
   SUSTAINABLE AGRICULTURE (ISS-2016)},
Series = {IOP Conference Series-Earth and Environmental Science},
