@article{ WOS:000623976700001,
Author = {Herold, N. and Downes, S. M. and Gross, M. H. and Ji, F. and Nishant, N.
   and Macadam, I and Ridder, N. N. and Beyer, K.},
Title = {Projected changes in the frequency of climate extremes over southeast
   Australia},
