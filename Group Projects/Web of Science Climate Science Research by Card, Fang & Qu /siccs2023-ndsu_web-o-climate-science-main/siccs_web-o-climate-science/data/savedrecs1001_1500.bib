Author = {Barker, T},
Title = {Representing global climate change, adaptation and mitigation},
Journal = {GLOBAL ENVIRONMENTAL CHANGE-HUMAN AND POLICY DIMENSIONS},
Year = {2003},
Volume = {13},
Number = {1},
Pages = {1-6},
Month = {APR},
Abstract = {The diagrammatic representation of climate change, adaptation and
   mitigation is important in conceptualizing the problem, identifying
   important feedbacks, and communicating between disciplines. The
