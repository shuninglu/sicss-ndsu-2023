@article{ WOS:000692118700005,
Author = {Henry, Catherine and Carter, Kate},
Title = {Communicating Climate Change Content in Small and Mid-Sized Museums:
   Challenges and Opportunities},
Journal = {JOURNAL OF MUSEUM EDUCATION},
Year = {2021},
Volume = {46},
Number = {3},
Pages = {321-333},
Month = {JUL 3},
Abstract = {Informal education and outreach can be highly effective at shifting
   public opinion and action on climate change, particularly in rural areas
   where climate change may be avoided in regular school curricula. Small
   and mid-sized museums in more rural areas can serve as important sources
   of climate change content, even when they are not primarily science
   museums. However, there are perceived and actual barriers that museums
   must overcome in order to incorporate this content. In this study, we
   interviewed staff members of small to mid-sized museums in rural
   Michigan to learn more about their attitudes and experiences with
   climate change. We present a summary of common challenges to
   incorporating climate change in rural museums, along with an overview of
   how partnerships can overcome these barriers using theory-based science
   communication approaches.},
