@article{ WOS:000445138000003,
Author = {Sarkar, Uttam Kumar and Roy, Koushik and Karnatak, Gunjan and Nandy,
   Saurav Kumar},
Title = {Adaptive climate change resilient indigenous fisheries strategies in the
   floodplain wetlands of West Bengal, India},
Journal = {JOURNAL OF WATER AND CLIMATE CHANGE},
