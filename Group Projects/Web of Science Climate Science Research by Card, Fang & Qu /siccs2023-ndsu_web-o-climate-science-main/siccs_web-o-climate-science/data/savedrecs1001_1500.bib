@article{ WOS:000484932500001,
Author = {You, Myoungsoon and Ju, Youngkee},
Title = {Comparing Outrage Effect on the Risk Perception of Climate Change versus
   Fine Dust},
Journal = {HEALTH COMMUNICATION},
Year = {2020},
