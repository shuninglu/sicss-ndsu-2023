@article{ WOS:000406556100005,
Author = {Dixon, Graham and Hmielowski, Jay and Ma, Yanni},
Title = {Improving Climate Change Acceptance Among US Conservatives Through
   Value-Based Message Targeting},
Journal = {SCIENCE COMMUNICATION},
