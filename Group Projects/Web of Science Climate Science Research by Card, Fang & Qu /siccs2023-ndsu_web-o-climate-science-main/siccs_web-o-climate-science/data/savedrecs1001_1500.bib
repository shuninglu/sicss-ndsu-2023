@article{ WOS:000497937400001,
Author = {Hopke, Jill E.},
Title = {Connecting Extreme Heat Events to Climate Change: Media Coverage of Heat
   Waves and Wildfires},
Journal = {ENVIRONMENTAL COMMUNICATION-A JOURNAL OF NATURE AND CULTURE},
Year = {2020},
Volume = {14},
Number = {4},
Pages = {492-508},
Month = {MAY 18},
Abstract = {This research tests the popular press assumption that media are not
   discussing climate change in coverage of heat waves and wildfires.
   Change over time in English-language media coverage is assessed, from
   2013 to 2018, including 37 news outlets from five countries, with the
   addition of Agence France-Presse and Climatewire. Mentions of climate
   issues increased for both types of extreme weather. Climatewire provided
   the highest degree of climate change issue attention. Elite,
   climate-specialist news outlets gave more attention to climate issues in
   both heat wave and wildfire reporting across the years studied. The
   2013-2018 time period saw an uptick in media coverage of heat waves,
   particularly in Chinese English-language media.},
