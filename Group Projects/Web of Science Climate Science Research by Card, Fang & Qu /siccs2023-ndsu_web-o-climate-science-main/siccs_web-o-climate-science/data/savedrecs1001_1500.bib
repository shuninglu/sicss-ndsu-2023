@article{ WOS:000211228800004,
Author = {Fleischer, Amy},
Title = {From Theory to Practice: How Mass Audubon Is Incorporating Strategic
   Framing about Climate Change},
Journal = {JOURNAL OF MUSEUM EDUCATION},
Year = {2013},
Volume = {38},
Number = {3},
Pages = {273-278},
Abstract = {Mass Audubon recognized that climate change was significantly impacting
   bird species distribution and seasonality. Unsure of how and when to
   engage visitors to their network of wildlife sanctuaries on the topic of
   climate change, its educators turned to the National Network of Ocean
   and Climate Change Interpreters' Study Circle (NNOCCI). Through
   participation in NNOCCI, with its focus on strategic framing, staff
   increased their confidence and abilities to communicate with their
   visitors about climate change and its impact on the varied habitats and
   wildlife in Mass Audubon's care. Educators from across the state
   customized their climate change programming for each unique site,
   including the creation of new program models that empowered visitors to
   collaborate with site staff to take individual and community-based
   action related to climate change adaptation.},
