@article{ WOS:000550636300001,
Author = {Bender, Adrian M. and Lease, Richard O. and Corbett, Lee B. and Bierman,
   Paul R. and Caffee, Marc W. and Rittenour, Tammy M.},
Title = {Late Cenozoic climate change paces landscape adjustments to Yukon River
   capture},
