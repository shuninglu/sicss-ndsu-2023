@article{ WOS:000647381200001,
Author = {Poberezhskaya, Marianna and Danilova, Nataliya},
Title = {Reconciling climate change leadership with resource nationalism and
   regional vulnerabilities: a case-study of Kazakhstan},
Journal = {ENVIRONMENTAL POLITICS},
Year = {2022},
Volume = {31},
Number = {3},
Pages = {429-452},
Month = {APR 16},
Abstract = {This contribution explores how climate change is presented in one of the
   most established Kazakhstani newspapers, Kazakhstanskaya Pravda. Using
   quantitative and qualitative frame analyses, we explore patterns of
   climate change communication through the focus on framing of climate
   science, consequences, and responsibilities. We argue that resource
   nationalism shapes climate discussion in Kazakhstan with climate change
   emerging as a state-controlled `resource'. We also identify that the
   coverage of national and international responsibilities is reflective of
   Kazakhstan's geopolitical vulnerabilities arising from its ambiguous
   relationship with Russia as well as attempts at presenting itself as
   being at the front of global sustainable development.},
