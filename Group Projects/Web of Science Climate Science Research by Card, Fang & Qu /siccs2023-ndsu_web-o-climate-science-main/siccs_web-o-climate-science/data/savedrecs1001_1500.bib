@article{ WOS:000616466700002,
Author = {Oyebola, Oyediran O. and Efitre, Jackson and Musinguzi, Laban and
   Falaye, Augustine E.},
Title = {Potential adaptation strategies for climate change impact among
   flood-prone fish farmers in climate hotspot Uganda},
