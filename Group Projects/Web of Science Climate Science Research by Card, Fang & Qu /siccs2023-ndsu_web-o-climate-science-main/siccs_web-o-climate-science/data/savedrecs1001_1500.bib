@article{ WOS:000375706900007,
Author = {Hyland, John J. and Jones, Davey L. and Parkhill, Karen A. and Barnes,
   Andrew P. and Williams, A. Prysor},
Title = {Farmers' perceptions of climate change: identifying types},
Journal = {AGRICULTURE AND HUMAN VALUES},
