@article{ WOS:000367122700012,
Author = {Kumar, K. Ravi and Nain, M. S. and Singh, Rashmi and Chahal, V. P. and
   Bana, R. S.},
Title = {Analysis of farmers' communication network and factors of knowledge
   regarding agro meteorological parameters},
