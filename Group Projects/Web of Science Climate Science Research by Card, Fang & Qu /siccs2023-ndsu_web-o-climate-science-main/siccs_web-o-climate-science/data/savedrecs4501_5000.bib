@article{ WOS:000365094700020,
Author = {Bagheri, S. F. and Alizadeh, M. and Nadarajah, S.},
Title = {Efficient Estimation of the PDF and the CDF of the Exponentiated Gumbel
   Distribution},
Journal = {COMMUNICATIONS IN STATISTICS-SIMULATION AND COMPUTATION},
Year = {2016},
Volume = {45},
Number = {1},
Pages = {339-361},
Abstract = {The exponentiated Gumbel model has been shown to be useful in climate
   modeling including global warming problem, flood frequency analysis,
   offshore modeling, rainfall modeling, and wind speed modeling. Here, we
   consider estimation of the probability density function (PDF) and the
   cumulative distribution function (CDF) of the exponentiated Gumbel
   distribution. The following estimators are considered: uniformly minimum
   variance unbiased (UMVU) estimator, maximum likelihood (ML) estimator,
   percentile (PC) estimator, least-square (LS) estimator, and weighted
   least-square (WLS) estimator. Analytical expressions are derived for the
   bias and the mean squared error. Simulation studies and real data
   applications show that the ML estimator performs better than others.},
