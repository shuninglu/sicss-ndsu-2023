@article{ WOS:000315842100004,
Author = {Prinn, Ronald G.},
Title = {Development and application of earth system models},
Journal = {PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA},
Year = {2013},
Volume = {110},
