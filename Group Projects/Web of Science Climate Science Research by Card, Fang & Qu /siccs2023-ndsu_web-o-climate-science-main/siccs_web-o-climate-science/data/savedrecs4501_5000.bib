@article{ WOS:000607147300001,
Author = {Holsman, K. K. and Haynie, A. C. and Hollowed, A. B. and Reum, J. C. P.
   and Aydin, K. and Hermann, A. J. and Cheng, W. and Faig, A. and Ianelli,
   J. N. and Kearney, K. A. and Punt, A. E.},
Title = {Ecosystem-based fisheries management forestalls climate-driven collapse},
Journal = {NATURE COMMUNICATIONS},
Year = {2020},
Volume = {11},
Number = {1},
Month = {SEP 11},
Abstract = {Climate change is impacting fisheries worldwide with uncertain outcomes
   for food and nutritional security. Using management strategy evaluations
   for key US fisheries in the eastern Bering Sea we find that Ecosystem
   Based Fisheries Management (EBFM) measures forestall future declines
   under climate change over non-EBFM approaches. Yet, benefits are
   species-specific and decrease markedly after 2050. Under high-baseline
   carbon emission scenarios (RCP 8.5), end-of-century (2075-2100) pollock
   and Pacific cod fisheries collapse in >70\% and >35\% of all
   simulations, respectively. Our analysis suggests that 2.1-2.3 degrees C
