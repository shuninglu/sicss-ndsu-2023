@article{ WOS:000260963800093,
Author = {Hausner, Csaba and Mate, Ferenc and Mako, Andras and Sisak, Istvan},
Title = {CLIMATIC DETERMINATION OF LONG-TERM YIELD VARIABILITY OF MAJOR CROPS IN
   HUNGARY},
