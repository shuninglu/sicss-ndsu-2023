@article{ WOS:000596625800005,
Author = {Creutzig, Felix and Kapmeier, Florian},
Title = {Engage, don't preach: Active learning triggers climate action},
Journal = {ENERGY RESEARCH \& SOCIAL SCIENCE},
Year = {2020},
Volume = {70},
Month = {DEC},
Abstract = {Traditional communication of research on climate change fails to
   encourage individual, corporate, and political leaders to take
   appropriate action. We argue that this problem is based on an overly
   simplistic unidirectional model of science communication. Conversely,
   theory shows that active learning processes are better suited to
   initiate and mobilize engagement among all stakeholders. Here, we
   integrate theoretical insights on active learning with empirical
   evidence from serious gaming: communication should be understood as an
   integral design feature that relates active learning on climate change
   to tangible action.},
