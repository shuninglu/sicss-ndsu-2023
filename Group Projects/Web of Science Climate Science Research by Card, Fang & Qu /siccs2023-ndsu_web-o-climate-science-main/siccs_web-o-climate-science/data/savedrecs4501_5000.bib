@incollection{ WOS:000427255400012,
Author = {Bachelet, Dominique and CBI Data Basin Team},
Editor = {Bachelet, D and Turner, D},
Title = {Bringing MC1 Model Results to Data Basin to Facilitate Access,
   Distribution, and Interpretation},
Booktitle = {GLOBAL VEGETATION DYNAMICS: CONCEPTS AND APPLICATIONS IN THE MC1 MODEL},
Series = {Geophysical Monograph Book Series},
Year = {2015},
Volume = {213},
Pages = {171-176},
Abstract = {Managing ecosystems for resilience to climate change requires
   collaboration, innovation and communication. The abundance of data and
   documents describing the uncertainty around both climate change
   projections and impacts has become challenging to managers who have
   little funding and limited time to digest and incorporate these
   materials into planning and implementation documents. The Conservation
   Biology Institute (CBI) is working with resource agencies from federal,
   state, and local government to provide usable information for a variety
   of vulnerability assessments. CBI created Data Basin (databasin.org) to
   centralize valuable spatial data, including state-of-the-art climate and
   vegetation response projections, and to provide the necessary system
   functionality to help users work with the data and each other to make
   more informed decisions. MC1 is prominently featured in Data Basin and
   many MC1 results are now available through the site. CBI also developed
   a decision support framework, called the environmental evaluation
   modeling system (EEMS), that uses climate change information such as MC1
   and other simulation results together with observations and
   socioeconomic constraints to help develop more effective adaptation
   strategies.},
