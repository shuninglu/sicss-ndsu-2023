@article{ WOS:000754037600036,
Author = {Lu, Yangsiyu and Cohen, Francois and Smith, Stephen M. and Pfeiffer,
   Alexander},
Title = {Plant conversions and abatement technologies cannot prevent stranding of
   power plant assets in 2 degrees C scenarios},
Journal = {NATURE COMMUNICATIONS},
Year = {2022},
Volume = {13},
Number = {1},
Month = {FEB 10},
Abstract = {Many existing and in-the-pipeline fossil-fuel power plants will have to
   be decommissioned or underused to avoid climate change beyond 2 degrees
   C, even under optimistic technology assumptions and after accounting for
   emission-reducing conversions.
   Continued fossil fuel development puts existing assets at risk of
   exceeding the capacity compatible with limiting global warming below 2
   degrees C. However, it has been argued that plant conversions and new
   abatement technologies may allow for a smoother transition. We quantify
   the impact of future technology availability on the need for fossil fuel
   power plants to be stranded, i.e. decommissioned or underused. Even with
   carbon capture and storage (CCS) and bioenergy widely deployed in the
   future, a total of 267 PWh electricity generation (ten times global
   electricity production in 2018) may still be stranded. Coal-to-gas
   conversions could prevent 10-30 PWh of stranded generation. CCS
   retrofits, combined with biomass co-firing, could prevent 33-68 PWh. In
   contrast, lack of deployment of CCS or bioenergy could increase
   stranding by 69 or 45 percent respectively. Stranding risks remain under
   optimistic technology assumptions and even more so if CCS and bioenergy
   are not deployed at scale.},
