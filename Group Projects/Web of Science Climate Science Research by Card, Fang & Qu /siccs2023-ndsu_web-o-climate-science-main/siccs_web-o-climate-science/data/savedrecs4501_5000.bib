@article{ WOS:000702204100001,
Author = {Wadud, Mushfique},
Title = {Precariously Employed Climate Journalists the Challenges of Freelance
   Climate Journalists in South Asia},
Journal = {JOURNALISM PRACTICE},
Year = {2022},
Volume = {16},
Number = {2-3, SI},
Pages = {262-280},
Month = {MAR 16},
Abstract = {Declines in the number of foreign correspondents and bureaus have caused
   media to rely on freelance journalists, particularly on coverage of
   complex and topical issues such as climate change. This study examines
   the challenges freelance climate journalists in South Asia face and how
   they negotiate pay, benefits, and safety. Drawing from in-depth
   interviews of 15 journalists based in three South Asian countries-India,
   Pakistan and Bangladesh the study finds that low wages, inconsistent
   work and perceived risk/anxiety over security - are the key challenges.
   The study argues that lack of financial and organizational support from
   the news media organizations impacts climate change coverage from a
   vulnerable region like South Asia. Freelance journalists often do not
   report on issues that can put them at physical and financial risks.
   Findings show that COVID-19 further impacts their financial security as
   many media outlets cut their budgets.},
