@article{ WOS:000343646900004,
Author = {Rietig, Katharina},
Title = {Reinforcement of multilevel governance dynamics: creating momentum for
   increasing ambitions in international climate negotiations},
Journal = {INTERNATIONAL ENVIRONMENTAL AGREEMENTS-POLITICS LAW AND ECONOMICS},
