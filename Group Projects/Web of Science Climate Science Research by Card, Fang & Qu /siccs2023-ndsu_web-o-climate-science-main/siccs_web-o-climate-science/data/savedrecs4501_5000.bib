@article{ WOS:000664052300001,
Author = {Lebel, Louis and Jutagate, Tuantong and Nguyen Thanh Phuong and Akester,
   Michael Joseph and Rangsiwiwat, Amornrat and Lebel, Phimphakan and
   Phousavanh, Phouvin and Navy, Hap and Soe, Khin Maung and Lebel, Boripat},
Title = {Climate risk management practices of fish and shrimp farmers in the
   Mekong Region},
