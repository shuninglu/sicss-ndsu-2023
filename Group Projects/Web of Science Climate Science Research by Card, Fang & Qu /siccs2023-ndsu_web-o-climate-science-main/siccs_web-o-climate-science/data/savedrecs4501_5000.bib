@article{ WOS:000415031800002,
Author = {Bakaki, Zorzeta and Bernauer, Thomas},
Title = {Citizens show strong support for climate policy, but are they also
   willing to pay?},
Journal = {CLIMATIC CHANGE},
Year = {2017},
Volume = {145},
Number = {1-2},
Pages = {15-26},
Month = {NOV},
Abstract = {To what extent citizens are willing not only to support ambitious
   climate policy but also willing to pay for such policy remains subject
   to debate. Our analysis addresses three issues in this regard: whether,
   as is widely assumed but not empirically established, willingness to
   support (WTS) is higher than willingness to pay (WTP); whether the
   determinants of the two are similar; and what accounts for
   within-subject similarity between WTS and WTP. We address these issues
   based on data from an original nationally representative survey (N =
   2500) on forest conservation in Brazil, arguably the key climate policy
   issue in the country. The findings reveal that WTP is much lower than
   WTS. The determinants differ to some extent as well, regarding the
   effects of age, gender, and trust in government. The analysis also
   provides insights into factors influencing how much WTS and WTP line up
   within individuals, with respect to age, education, political ideology,
   salience of the deforestation issue, and trust in government. Our
   findings provide a more nuanced picture of how strong public support for
   climate change policy is and a starting point for more targeted climate
   policy communication.},
