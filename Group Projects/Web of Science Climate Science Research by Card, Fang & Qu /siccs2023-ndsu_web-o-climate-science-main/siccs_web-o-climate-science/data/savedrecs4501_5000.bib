Language = {English},
Affiliation = {Corry, O (Corresponding Author), Univ Copenhagen, Dept Polit Sci, Ostre Farimagsgade 5, DK-1353 Copenhagen, Denmark.
   Corry, Olaf, Univ Copenhagen, Dept Polit Sci, Int Relat, Copenhagen, Denmark.
   Reiner, David, Univ Cambridge, Technol Policy, Judge Business Sch, Cambridge, England.},
DOI = {10.1177/0038038520943107},
EarlyAccessDate = {SEP 2020},
Article-Number = {0038038520943107},
ISSN = {0038-0385},
EISSN = {1469-8684},
